{
  "presets": [
    "env",
    "stage-0",
    "stage-2"
  ],
  "plugins": [
    "istanbul",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-class-properties",
    [
      "transform-private",
      {
        "pattern": "^_"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-0"
      ],
      "plugins": [
        "transform-runtime",
        "transform-class-properties",
        "transform-decorators-legacy"
      ]
    }
  }
}

