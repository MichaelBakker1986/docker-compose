{
  "source": ".",
  "destination": "./docs",
  "package": "./package.json",
  "excludes": [
    "(node_modules|bower_components|tests|dist|docs|./package.json)"
  ],
  "includes": [
    "\\.(js)$"
  ],
  "plugins": [
    {
      "name": "esdoc-standard-plugin"
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "objectRestSpread": true,
        "doExpressions": true,
        "functionBind": true,
        "functionSent": true,
        "asyncGenerators": true,
        "decorators": true,
        "exportExtensions": true,
        "dynamicImport": true
      }
    }
  ]
}