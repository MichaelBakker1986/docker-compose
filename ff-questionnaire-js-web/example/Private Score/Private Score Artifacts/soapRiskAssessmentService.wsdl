      <xs:complexType name="InputType">
        <xs:annotation>
          <xs:documentation>A single input qualitative or collateral or guarantee, represented as a name-value pair.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="Value" nillable="true" type="tns:ValueType"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:NMTOKEN" use="required"/>
      </xs:complexType>
      <xs:complexType name="GuaranteeType">
        <xs:annotation>
          <xs:documentation>A guarantee that has been or must be allocated to some or all of the facilities.</xs:documentation>
          <xs:documentation>If the guarantee must still be allocated, the selection of facilities to which allocation must occur</xs:documentation>
          <xs:documentation>is made explicit by the presence of GuaranteeAllocation elements for this guarantee in those facilities.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="GuaranteeId" type="tns:NonBlankString">
            <xs:annotation>
              <xs:documentation>The global ID of the guarantee.</xs:documentation>
              <xs:documentation>This ID is generated by RAPID; there is no other reference system in which it is registered.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GuaranteeType" type="tns:NonBlankString">
            <xs:annotation>
              <xs:documentation>The type of the guarantee provided.</xs:documentation>
              <xs:documentation>The Guarantee Type Code is provided by the source system, which is then later resolved in FAIR.</xs:documentation>
              <xs:documentation>The Recovery Rate is retrieved from the Guarantee type for Guarantee Allocation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:choice>
            <xs:element minOccurs="0" name="GuaranteeValue" type="tns:AmountType">
              <xs:annotation>
                <xs:documentation>The amount of the guarantee, with currency and currency rate.</xs:documentation>
                <xs:documentation>This ID is generated by RAPID; there is no other reference system in which it is registered.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="GuaranteeFraction" type="tns:FractionType">
              <xs:annotation>
                <xs:documentation>The coverage of this guarantee as a fraction of the limit of each facility to which the guarantee is allocated.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:choice>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="CollateralType">
        <xs:annotation>
          <xs:documentation>A collateral that has been or must be allocated to some or all of the facilities.</xs:documentation>
          <xs:documentation>If the collateral must still be allocated, the selection of facilities to which allocation must occur</xs:documentation>
          <xs:documentation>is made explicit by the presence of CollateralAllocation elements for this collateral in those facilities.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="CollateralId" type="tns:NonBlankString">
            <xs:annotation>
              <xs:documentation>The global ID of the collateral.</xs:documentation>
              <xs:documentation>This ID is generated by RAPID; there is no other reference system in which it is registered.</xs:documentation>
              <xs:documentation>A reference system may become available later; there seem to be plans for it.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="CollateralType" type="tns:CollateralTypeType">
            <xs:annotation>
              <xs:documentation>The Basel II standardised collateral type.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="CollateralSubType" type="tns:NonBlankString">
            <xs:annotation>
              <xs:documentation>The Collateral sub type for a specific Basel II Collateral type.</xs:documentation>
