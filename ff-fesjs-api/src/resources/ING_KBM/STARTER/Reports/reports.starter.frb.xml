<?xml version="1.0" encoding="UTF-8"?>

<!--LET OP ONDERSTAAND VERSIENUMMER OPHOGEN-->

<document version="1.2" date="03-08-2015" author="Topicus Finan" title="ING-ScoreCard" xsi:noNamespaceSchemaLocation="../FRBXMLSchema.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<properties>
	<ReportName><data src="fesModel"><variables var="Root" function="getTitle"/></data></ReportName>
	<RelatieNummer><data src="fesModel"><columns number="1" /><variables var="FPS_VAR_KVKnr" /><presentation showSingleValueVariablesOnce="false" /></data></RelatieNummer>
	<RelatieNummerTitel>KvK nummer</RelatieNummerTitel>
	<MedewerkerVKO><data src="document" var="./document/properties/currentUser_Fullname/text()"/></MedewerkerVKO>
	<MedewerkerVKOTitel>Medewerker</MedewerkerVKOTitel>
	<CreationDate><data src="functions" name="getDate"><param key="format" value="d MMMMM yyyy"/></data></CreationDate>
	<CreationDateTitel>Datum</CreationDateTitel>
	<CompanyName><data src="fesModel"><columns number="1" /><variables depth="1" var="FPS_VAR_Naam"/></data></CompanyName>
	<ReportScaleDefault>1</ReportScaleDefault>
</properties>

<!-- =========data============================================================= -->
<newpage name="normal">
<p   id="watermark"  visible="./document/properties/isFinalReport/text()=0"/>
</newpage>

<!--Statement information-->
	<section class="section"><data src="fesModel"><variables var="Q_RESULT" function="getTitle"/></data></section>
	<p><data src="fesModel"><columns number="1" /><variables var="Q_RESULT"  /></data>.</p>

	<section class="section"><data src="fesModel"><variables var="Q_STATUS" function="getTitle"/></data></section>
	<table class="datatable">
		<data src="fes">
			<columns number="1" order="asc" />
			<!--<columns number="3" order="asc"/>-->
			<variables depth="0" var="Q_STATUS"/>
			<variables depth="0" var="Q_STATUS_FINAL_ON"/>
			<variables depth="0" var="Q_STATUS_FINAL_BY"/>
			<variables depth="0" var="Q_STATUS_FINAL_BY_NAME"/>
			<variables depth="0" var="Q_STATUS_STARTED_ON"/>
			<variables depth="0" var="ModelVersion"/>
			<variables depth="0" var="MatrixVersion"/>
		</data>
	</table>

<clearpage/>

	<section class="section"><data src="fesModel"><variables var="Root" function="getTitle"/></data></section>
	<table class="datatable">
	 	<data src="fes">
  			<columns number="1" order="asc" />
			<variables depth="4" var="Q_ROOT" recursiveSkip="false" />
			<presentation order="reverse" showSingleValueVariablesOnce="false" disableMinusCorrection="true" filterEmptyRows="false" showHeaderRow="false" scale="./document/properties/ReportScaleDefault/text()" prefill="false" appendEmptyColumns="0" prefillSingleValueVariable="false" >
				<!--<filterEmptyRowsForClass>header</filterEmptyRowsForClass>-->
				<overrideStyle old="level0empty">remove</overrideStyle>
				<overrideStyle old="level1">remove</overrideStyle>
				<overrideStyle old="level1empty">title</overrideStyle>
      	<overrideStyle old="level2empty">header</overrideStyle>
				<overrideStyle old="level2">remove</overrideStyle>
				<overrideStyle old="empty">remove</overrideStyle>
				<overrideStyle old="level0">remove</overrideStyle>
				<overrideStyle old="level3">level2</overrideStyle>
			</presentation>
		</data>
	</table>

<!-- =========End============================================================= -->
</document>
