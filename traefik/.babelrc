{
  "presets": [
    "env",
    "stage-0",
    "stage-2",
    "stage-3"
  ],
  "plugins": [
    "istanbul",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-class-properties",
    [
      "transform-private",
      {
        "pattern": "^_"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-0",
        "stage-2",
        "stage-3"
      ],
      "plugins": [
        [
          "transform-runtime",
          {
            "polyfill": false,
            "regenerator": true
          }
        ],
        "transform-class-properties",
        "transform-decorators-legacy"
      ]
    }
  }
}

