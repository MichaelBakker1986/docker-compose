import FinanMath.*;
import BaseModel.*;

model TESTMODEL uses BaseModel
{
  version: "1.14";
  root
  {
    variable TotalConfusedMore
    {
        title: "TotalConfusedMore";
        formula: 101;
        datatype: number;
        frequency: document;
        inputRequired: 1;
        variable TotalConfusedMoreChild
        {
            title: "TotalConfusedMoreChild";
            formula: 123;
            datatype: number;
            frequency: document;
            inputRequired: 1;
        }
    }
	variable VariableOne
	{
	    title: "VariableOne";
		datatype: number;
		formula: 101;
        frequency: document;
        inputRequired: 1;
	}
    variable Q_ROOT
    {
        title: "Q_ROOT";
        datatype: number;
        formula: 156;
        frequency: document;
        inputRequired: 1;
        variable Q_MAP01
        {
            title: "Q_MAP01";
            datatype: number;
            formula: 1321;
            frequency: document;
            inputRequired: 1;
        }
    }
	variable VariableTwo
	{
	    title: "VariableTwo";
	    formula: 102;
		datatype: number;
		inputRequired: Total > 1000;
		visible: VariableOne <> 100;
		frequency: document;
	}
	variable Total
	{
		title: "Total";
		formula: VariableOne + VariableTwo;
		datatype: number;
		frequency: document;
		locked: 1;
		inputRequired: 1;
	}
	variable OneFixedDecimal
	{
    	title: "OneFixedDecimal";
    	formula: 100.801 + 100.9999999;
    	datatype: number;
    	frequency: document;
	   	fixed_decimals: 1;
	}
	variable ZeroFixedDecimal
	{
    	title: "OneFixedDecimal";
    	formula: 100.801 + 100.9999999;
    	datatype: number;
    	frequency: document;
	   	fixed_decimals: 0;
	}
    variable EvaluateStringTitle
    {
        title: EvaluateAsString(GetTitle(Total));
        formula: EvaluateAsString(GetTitle(Total));
        datatype: string;
        frequency: document;
        locked: 1;
        inputRequired: 1;
    }
    variable DataEntered
    {
        title: "DataEntered";
        formula: DataEntered(Total);
        datatype: number;
        frequency: document;
    }
  }
}