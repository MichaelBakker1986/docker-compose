.Formulas NoTrend, Trend

CurrentRatio                         = OnER(AssetsCurrentTotal/LiabilitiesCurrentTotal,NA)
QuickRatio                           = OnER((AssetsCurrentTotal-Inventories)/LiabilitiesCurrentTotal,NA)
NetSalesWorkingCapital               = OnER(GetPoint(Revenues,1)/WorkingCapital,NA)
TangibleNetWorth                     = OnER(EquityAndReserves-IntangibleAssetsNet,NA)
EffTangNetWorthActual                = OnER(TangibleNetWorth+LongTermDebtSubordinated+CurrentPortionOfLongTermDebtSubordinated+HybridsTreatedAsDebt,NA)
DebtNetWorth                         = OnER((TotalNetDebt)/EquityAndReserves,NA)
DebtTangNetWorth                     = OnER((TotalNetDebt)/TangibleNetWorth,NA)
DbtLessSubEffTNW                     = OnER(((TotalNetDebt-HybridsTreatedAsDebt)-(LongTermDebtSubordinated+CurrentPortionOfLongTermDebtSubordinated))/(EffTangNetWorthActual),NA)
;BorrowedFunds                        = OnER(InterestBearingBorrowingsNonCurrent+FinanceLeasesNonCurrent+InterestBearingBorrowingsCurrent+FinanceLeasesCurrent,NA)
BorrowedFundsTotalLiabilities        = OnER((BorrowedFunds)/(LiabilitiesNonCurrentTotal+LiabilitiesCurrentTotal),NA)
BorrowedFundsEffTNW                  = OnER((BorrowedFunds)/EffTangNetWorthActual,NA)
;BorrowedFundsEBITDA                  = OnER((BorrowedFunds)/(EarningsBeforeInterestTaxAndDepreciation*TsY),NA)
BorrowedFundsEBITDA                  = OnER((BorrowedFunds)/GetPoint(EarningsBeforeInterestTaxAndDepreciation,1),NA)
;BorrowedFundsEBITDA                  = OnER(GetFrac(EarningsBeforeInterestTaxAndDepreciation,(InterestBearingBorrowingsNonCurrent+FinanceLeasesNonCurrent+InterestBearingBorrowingsCurrent+FinanceLeasesCurrent)),NA)
;BorrowedFundsEBITDA                  = OnER((InterestBearingBorrowingsNonCurrent+FinanceLeasesNonCurrent+InterestBearingBorrowingsCurrent+FinanceLeasesCurrent)/EarningsBeforeInterestTaxAndDepreciation,NA)
;SeniorNetDebtEBITDA                  = OnER(SeniorNetDebt/EarningsBeforeInterestTaxAndDepreciation*TsY,NA)
SeniorNetDebtEBITDA                  = OnER(SeniorNetDebt/GetPoint(EarningsBeforeInterestTaxAndDepreciation,1),NA)
TotalNetDebtEBITDA                   = OnER(TotalNetDebt/GetPoint(EarningsBeforeInterestTaxAndDepreciation,1),NA)
TNDtoTNDNetWorth                     = OnER(TotalNetDebt/(TotalNetDebt+EquityAndReserves),NA)
TotalLiabilitiesTotalAssets          = OnER((LiabilitiesNonCurrentTotal+LiabilitiesCurrentTotal)/AssetsTotal,NA)
FinancialLeverage                    = OnER(AssetsTotal/EquityAndReserves,NA)
OffBalanceSheetLeverage              = OnER((((LiabilitiesNonCurrentTotal+LiabilitiesCurrentTotal)+(OperatingLeaseCommitments*0.79)+(OffBalanceSheetLiabilities))/(TangibleNetWorth+OffBalanceSheetAssets)),NA)
Interestcoverage                     = OnER(EarningsBeforeInterestAndTax/InterestExpense,NA)
EBITDAtoNetInterest                  = If((NetInterestIncomeExpense<=0) And (EarningsBeforeInterestTaxAndDepreciation>0),OnER(EarningsBeforeInterestTaxAndDepreciation/-NetInterestIncomeExpense,NA),NA)
EBITtoNetInterest                    = If((NetInterestIncomeExpense<=0) And (EarningsBeforeInterestAndTax>0)            ,OnER(EarningsBeforeInterestAndTax/-NetInterestIncomeExpense,NA),NA)
EarningsCoverage                     = OnER(GetPoint(EarningsBeforeInterestTaxAndDepreciation,1)/(GetPoint(InterestExpense,1)+CurrentPortionOfLongTermDebtBank+CurrentPortionOfFinancialLeaseLiabilities+CurrentPortionOfLongTermDebtOther+CurrentPortionOfLongTermDebtConvertible+CurrentPortionOfLongTermDebtSubordinated),NA)                                       ;JB 2013-01-24 Niet opgenomen in formule: DueToRelatedPartiesNonCurrent, DueToShareholderNonCurrent
CashFlowCoverageCFi                  = OnER(GetPoint(CFi_CashFlowsFromOperatingActivities,1)/(GetPoint(CFi_InterestPaidOper,1)*-1+GetPoint(CFi_InterestPaidFin,1)*-1+CurrentPortionOfLongTermDebtBank+CurrentPortionOfFinancialLeaseLiabilities+DueToShareholderCurrent+CurrentPortionOfLongTermDebtOther+DueToRelatedPartiesCurrent+CurrentPortionOfLongTermDebtConvertible+CurrentPortionOfLongTermDebtSubordinated),NA)           ;RvA:29-07-2010 Projections added   ;;; CurrentPortionOfBT_LongTermPayablesShareholdersParticipants+CurrentPortionOfBT_LongTermPayablesParticipatingInterestsAffiliatedCompanies uit de formule gehaald
CashFlowCoverageCFd                  = OnER(GetPoint(CFd_CashFlowFromOperatingActivities,1)/(GetPoint(InterestExpense,1)-GetPoint(DeferredInterestExpense,1)+InterestPayable+CurrentPortionOfLongTermDebtBank+CurrentPortionOfFinancialLeaseLiabilities+CurrentPortionOfLongTermDebtOther+DueToRelatedPartiesCurrent+DueToShareholderCurrent+CurrentPortionOfLongTermDebtConvertible+CurrentPortionOfLongTermDebtSubordinated),NA)  ;RvA:29-07-2010 Projections added   ;;; CurrentPortionOfBT_LongTermPayablesShareholdersParticipants+CurrentPortionOfBT_LongTermPayablesParticipatingInterestsAffiliatedCompanies uit de formule gehaald
CFOtoTotalNetDebt                    = OnER(GetPoint(CFd_CashFlowFromOperatingActivities,1)/TotalNetDebt,NA)
FCFtoTotalNetDebt                    = OnER(GetPoint(FreeCashFlowRatios,1)/TotalNetDebt,NA)
FCFitoTotalNetDebt                   = OnER(GetPoint(FreeCashFlowRatiosInput,1)/TotalNetDebt,NA)
CashGrossMargin                      = OnER(((Revenues-CostOfSales)/Revenues),NA)
EBITDAMargin                         = OnER(EarningsBeforeInterestTaxAndDepreciation/Revenues,NA)
NetOperatingEBITMargin               = OnER((EarningsBeforeInterestAndTax/Revenues),NA)
ProfitBeforeTaxMargin                = OnER(ProfitLossBeforeTax/Revenues,NA)
NetProfitMargin                      = OnER(NetProfit/Revenues,NA)
EffectiveTaxRate                     = OnER((IncomeTaxExpense+CurrentTaxPayables+DeferredTaxLiabilities)/(ProfitLossBeforeTax),NA)
ProfitBeforeTaxesTotalAssets         = OnER(GetPoint(ProfitLossBeforeTax,1)/AssetsTotal,NA)
ProfitBeforeTaxesTangibleNetWorth    = OnER(GetPoint(ProfitLossBeforeTax,1)/TangibleNetWorth,NA)
ProfitBeforeTaxesTotalEquityReserves = OnER(GetPoint(ProfitLossBeforeTax,1)/EquityAndReserves,NA)
ReturnOnAssets                       = OnER(GetPoint(NetProfit,1)/((AssetsTotal>0)*(HSum(AssetsTotal,GetT(T,-1),GetT(T,0))/HValues(AssetsTotal,GetT(T,-1),GetT(T,0)))),NA)
ReturnOnTangibleNetWorth             = OnER(GetPoint(NetProfit,1)/((TangibleNetWorth>0)*(HSum(TangibleNetWorth,GetT(T,-1),GetT(T,0))/HValues(TangibleNetWorth,GetT(T,-1),GetT(T,0)))),NA)
ReturnOnTotalEquityReserve           = OnER(GetPoint(NetProfit,1)/((EquityAndReserves>0)*(HSum(EquityAndReserves,GetT(T,-1),GetT(T,0))/HValues(EquityAndReserves,GetT(T,-1),GetT(T,0)))),NA)

CoSExclDeprImpairmCoS                = CostOfSales-DepreciationAndImpairmentCostOfSale ;JB 13-01-14 verborgen var
;InventoryDaysExclCOSDeprec           = OnER(Inventories/((CoSExclDeprImpairmCoS)*TsY)*365,NA)
;TradePayableDaysExclCOSDeprec        = OnER(TradeAndOtherPayablesCurrent/((CoSExclDeprImpairmCoS)*TsY)*365,NA)
InventoryDaysExclCOSDeprec           = OnZero((Inventories>0)*GetFrac(CoSExclDeprImpairmCoS,(HSum(Inventories,GetT(T,-1),GetT(T,0))/HValues(Inventories,GetT(T,-1),GetT(T,0))))*365,NA)    ;JB 13-01-14  aangepast
TradePayableDaysExclCOSDeprec        = OnZero((TradeAndOtherPayablesCurrent>0)*GetFrac(CoSExclDeprImpairmCoS,(HSum(TradeAndOtherPayablesCurrent,GetT(T,-1),GetT(T,0))/HValues(TradeAndOtherPayablesCurrent,GetT(T,-1),GetT(T,0))))*365,NA)     ;JB 13-01-14  aangepast

SalesTotalAssets                     = OnER(GetPoint(Revenues,1)/AssetsTotal,NA)
SalesRevenuesToTangibleNetWorth      = OnER(GetPoint(Revenues,1)/TangibleNetWorth,NA)
SalesRevenuesToNetFixedAssets        = OnER(GetPoint(Revenues,1)/FixedAssetsNet,NA)
TotalAssetsGrowth                    = OnER(((AssetsTotal-AssetsTotal[T-1])/Abs(AssetsTotal[T-1])),NA)

EBITDAGrowth                         = OnER(((EarningsBeforeInterestTaxAndDepreciation-EarningsBeforeInterestTaxAndDepreciation[T-1])/Abs(EarningsBeforeInterestTaxAndDepreciation[T-1])),NA)
NetOperatingProfitEBITGrowth         = OnER(((EarningsBeforeInterestAndTax-EarningsBeforeInterestAndTax[T-1])/Abs(EarningsBeforeInterestAndTax[T-1])),NA)
NetProfitGrowth                      = OnER(((NetProfit-NetProfit[T-1])/Abs(NetProfit[T-1])),NA)
SustainableGrowth                    = OnER(((NetProfitMargin*(1-DividendPayoutRate)*(1+DebtTangNetWorth))/((1/SalesTotalAssets)-(NetProfitMargin*(1-DividendPayoutRate)*(1+DebtTangNetWorth))))*100,NA)
GrossProfitMargin                    = OnER(GrossProfit/Revenues, NA)
SalesGrowth                          = OnEr((Revenues-GetValue(Revenues,T-1,TsY))/Abs(GetValue(Revenues,T-1,TsY)),NA)

