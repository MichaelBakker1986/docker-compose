PP      1A     3               "$>Validatie stap 18<$"                                                                 \Q_STEP18_VALIDATION
PP      1N %   3               "$>Voortgang van stap 18<$"                                                             \Q_STEP18_PROGRESS
PI      1N C   3               "$>Akkoord<$"                                                                           \Q_STEP18_ACCEPTED
PP      BN     3               "$>Aantal verplichte velden<$"                                                          \Q_STEP18_REQUIREDVARS
PP      BN     3               "$>Aantal ingevulde verplichte velden<$"                                                \Q_STEP18_ENTEREDREQUIREDVARS
PI      1N C   3               "$>Openstaande validatiemeldingen<$"                                                    \Q_STEP18_OPENVALIDATION
PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP18_VALIDATIONMESSAGE

.Choices
Q_STEP18_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP18_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP18_ACCEPTED=             ((Q_STEP18_OPENVALIDATION=1) Or (Q_STEP18_PROGRESS<100%))

.Formulas NoTrend
Q_STEP18_WARNING=              &If((Q_STEP18_PROGRESS[1]<1),&"Nog niet alle verplichte gegevens zijn ingevoerd.|","")&If((Q_STEP18_OPENVALIDATION[1]=0) And (Q_STEP18_PROGRESS[1]>99%) And (Q_STEP18_ACCEPTED[1]=0),&"|Deze stap moet nog worden geaccordeerd","")
Q_STEP18_VALIDATION=           &If(Q_STEP18_OPENVALIDATION[1]=1,&"|"&Q_STEP18_VALIDATIONMESSAGE[1],"")
Q_STEP18_INFO=                 &If(Q_STEP18_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
Q_STEP18_PROGRESS=             If(HSum(Q_STEP18_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP18_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP18_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP18_ACCEPTED=             0
Q_STEP18_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP18,Q_STEP18_WARNING),InputRequired(X))
Q_STEP18_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP18,Q_STEP18_WARNING),InputRequired(X) And DataAvailable(X))

