; Systeem variabelen bij stap 1
;

PP      1A   1 3               "$>Waarschuwing bij stap 1<$"                                                           \Q_STEP01_WARNING
PP      1A     3               "$>Info bij stap 1<$"                                                                   \Q_STEP01_INFO
PP      1A     3               "$>Validatie stap 1<$"                                                                  \Q_STEP01_VALIDATION
PP      1N %   3               "$>Voortgang van stap 1<$"                                                              \Q_STEP01_PROGRESS
PI      1N C   3               "$>Akkoord<$"                                                                           \Q_STEP01_ACCEPTED
PP      BN     3               "$>Aantal verplichte velden<$"                                                          \Q_STEP01_REQUIREDVARS
PP      BN     3               "$>Aantal ingevulde verplichte velden<$"                                                \Q_STEP01_ENTEREDREQUIREDVARS
PI      1N C   3               "$>Openstaande validatiemeldingen<$"                                                    \Q_STEP01_OPENVALIDATION
PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP01_VALIDATIONMESSAGE

.Choices
Q_STEP01_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP01_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP01_ACCEPTED=             ((Q_STEP01_OPENVALIDATION=1) Or (Q_STEP01_PROGRESS<100%))

.Formulas NoTrend
Q_STEP01_WARNING=              &If((Q_STEP01_PROGRESS[1]<1),&"Nog niet alle verplichte gegevens zijn ingevoerd.|","")&If((Q_STEP01_OPENVALIDATION[1]=0) And (Q_STEP01_PROGRESS[1]>99%) And (Q_STEP01_ACCEPTED[1]=0),&"|Deze stap moet nog worden geaccordeerd","")
Q_STEP01_VALIDATION=           &If(Q_STEP01_OPENVALIDATION[1]=1,&"|"&Q_STEP01_VALIDATIONMESSAGE[1],"")
Q_STEP01_INFO=                 &If(Q_STEP01_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
Q_STEP01_PROGRESS=             If(HSum(Q_STEP01_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP01_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP01_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP01_ACCEPTED=             0
Q_STEP01_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP01,Q_STEP01_WARNING),InputRequired(X))
Q_STEP01_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP01,Q_STEP01_WARNING),InputRequired(X) And DataAvailable(X))

