;--------------------------------------------------------------------------------------------------------------------------------------------
; Systeem variabelen bij stap 12
;

PP      1A   1 3               "$>Waarschuwing bij stap 12<$"                                                          \Q_STEP12_WARNING
PP      1A     3               "$>Info bij stap 12<$"                                                                  \Q_STEP12_INFO
PP      1A     3               "$>Validatie stap 12<$"                                                                 \Q_STEP12_VALIDATION
PP      1N %   3               "$>Voortgang van stap 12<$"                                                             \Q_STEP12_PROGRESS
PI      1N C   3               "$>Akkoord<$"                                                                           \Q_STEP12_ACCEPTED
PP      BN     3               "$>Aantal verplichte velden<$"                                                          \Q_STEP12_REQUIREDVARS
PP      BN     3               "$>Aantal ingevulde verplichte velden<$"                                                \Q_STEP12_ENTEREDREQUIREDVARS
PI      1N C   3               "$>Openstaande validatiemeldingen<$"                                                    \Q_STEP12_OPENVALIDATION
PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP12_VALIDATIONMESSAGE

.Choices
Q_STEP12_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP12_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP12_ACCEPTED=             ((Q_STEP12_OPENVALIDATION=1) Or (Q_STEP12_PROGRESS<100%))

.Formulas NoTrend
Q_STEP12_WARNING=              &If((Q_STEP12_PROGRESS[1]<1),&"Nog niet alle verplichte gegevens zijn ingevoerd.|","")&If((Q_STEP12_OPENVALIDATION[1]=0) And (Q_STEP12_PROGRESS[1]>99%) And (Q_STEP12_ACCEPTED[1]=0),&"|Deze stap moet nog worden geaccordeerd","")
Q_STEP12_VALIDATION=           &If(Q_STEP12_OPENVALIDATION[1]=1,&"|"&Q_STEP12_VALIDATIONMESSAGE[1],"")
Q_STEP12_INFO=                 &If(Q_STEP12_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
Q_STEP12_PROGRESS=             If(HSum(Q_STEP12_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP12_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP12_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP12_ACCEPTED=             0
Q_STEP12_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP12,Q_STEP12_WARNING),InputRequired(X))
Q_STEP12_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP12,Q_STEP12_WARNING),InputRequired(X) And DataAvailable(X))

