Q_STEP05_PROGRESS=             If(HSum(Q_STEP05_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP05_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP05_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP05_ACCEPTED=             0
Q_STEP05_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP05,Q_STEP05_WARNING),InputRequired(X))
Q_STEP05_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP05,Q_STEP05_WARNING),InputRequired(X) And DataAvailable(X))

