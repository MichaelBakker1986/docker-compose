.Formulas InputRequired
h884_bdiExchangeRateFinancialYearFunctionalCurrencySchemeAverage=1
h884_bdiFunctionalCurrencySchemeCurrencyFactor=1
h884_bdiTaxableAmountBaseForTaxCalculation=1

.Variables
;--------------------------------------------------------------------------------------------------------------------------------------------
; Systeem variabelen bij stap 2
;

PP      1A   1 3               "$>Waarschuwing bij stap 2<$"                                                           \Q_STEP02_WARNING
PP      1A     3               "$>Info bij stap 2<$"                                                                   \Q_STEP02_INFO
PP      1A     3               "$>Validatie stap 2<$"                                                                  \Q_STEP02_VALIDATION
PP      1N %   3               "$>Voortgang van stap 2<$"                                                              \Q_STEP02_PROGRESS
PI      1N C   3               "$>Akkoord<$"                                                                           \Q_STEP02_ACCEPTED
PP      BN     3               "$>Aantal verplichte velden<$"                                                          \Q_STEP02_REQUIREDVARS
PP      BN     3               "$>Aantal ingevulde verplichte velden<$"                                                \Q_STEP02_ENTEREDREQUIREDVARS
PI      1N C   3               "$>Openstaande validatiemeldingen<$"                                                    \Q_STEP02_OPENVALIDATION
PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP02_VALIDATIONMESSAGE

.Choices
Q_STEP02_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP02_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP02_ACCEPTED=             ((Q_STEP02_OPENVALIDATION=1) Or (Q_STEP02_PROGRESS<100%))

.Formulas NoTrend
Q_STEP02_WARNING=              &If((Q_STEP02_PROGRESS[1]<1),&"Nog niet alle verplichte gegevens zijn ingevoerd.|","")&If((Q_STEP02_OPENVALIDATION[1]=0) And (Q_STEP02_PROGRESS[1]>99%) And (Q_STEP02_ACCEPTED[1]=0),&"|Deze stap moet nog worden geaccordeerd","")
Q_STEP02_VALIDATION=           &If(Q_STEP02_OPENVALIDATION[1]=1,&"|"&Q_STEP02_VALIDATIONMESSAGE[1],"")
Q_STEP02_INFO=                 &If(Q_STEP02_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
