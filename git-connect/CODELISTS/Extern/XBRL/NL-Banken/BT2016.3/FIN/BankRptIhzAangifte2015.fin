PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP03_VALIDATIONMESSAGE

.Choices
Q_STEP03_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP03_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP03_ACCEPTED=             ((Q_STEP03_OPENVALIDATION=1) Or (Q_STEP03_PROGRESS<100%))

.Formulas NoTrend
