Q_STEP03_INFO=                 &If(Q_STEP03_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
Q_STEP03_PROGRESS=             If(HSum(Q_STEP03_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP03_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP03_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP03_ACCEPTED=             0
Q_STEP03_OPENVALIDATION=       0
Q_STEP03_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP03,Q_STEP03_WARNING),InputRequired(X))
Q_STEP03_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP03,Q_STEP03_WARNING),InputRequired(X) And DataAvailable(X))

