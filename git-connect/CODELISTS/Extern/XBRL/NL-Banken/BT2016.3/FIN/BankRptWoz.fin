Q_STEP02_ACCEPTED=             0
Q_STEP02_OPENVALIDATION=       0
Q_STEP02_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP02,Q_STEP02_WARNING),InputRequired(X))
Q_STEP02_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP02,Q_STEP02_WARNING),InputRequired(X) And DataAvailable(X))

