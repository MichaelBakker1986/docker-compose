PI      1N C   3               "$>Openstaande validatiemeldingen<$"                                                    \Q_STEP08_OPENVALIDATION
PI      1M C   3               "$>Validatiemeldingen<$"                                                                \Q_STEP08_VALIDATIONMESSAGE

.Choices
Q_STEP08_ACCEPTED=             "0:Nee|1:Ja"
Q_STEP08_OPENVALIDATION=       "0:Nee|1:Ja"

.Formulas Locked
Q_STEP08_ACCEPTED=             ((Q_STEP08_OPENVALIDATION=1) Or (Q_STEP08_PROGRESS<100%))

.Formulas NoTrend
Q_STEP08_WARNING=              &If((Q_STEP08_PROGRESS[1]<1),&"Nog niet alle verplichte gegevens zijn ingevoerd.|","")&If((Q_STEP08_OPENVALIDATION[1]=0) And (Q_STEP08_PROGRESS[1]>99%) And (Q_STEP08_ACCEPTED[1]=0),&"|Deze stap moet nog worden geaccordeerd","")
Q_STEP08_VALIDATION=           &If(Q_STEP08_OPENVALIDATION[1]=1,&"|"&Q_STEP08_VALIDATIONMESSAGE[1],"")
Q_STEP08_INFO=                 &If(Q_STEP08_ACCEPTED[1]=1,&"|Deze stap is afgerond","")
Q_STEP08_PROGRESS=             If(HSum(Q_STEP08_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))=0,1,HSum(Q_STEP08_ENTEREDREQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend))/HSum(Q_STEP08_REQUIREDVARS,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend)))
Q_STEP08_ACCEPTED=             0
Q_STEP08_OPENVALIDATION=       0
Q_STEP08_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP08,Q_STEP08_WARNING),InputRequired(X))
Q_STEP08_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP08,Q_STEP08_WARNING),InputRequired(X) And DataAvailable(X))

