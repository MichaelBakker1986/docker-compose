Q_STEP11_ACCEPTED=             0
Q_STEP11_OPENVALIDATION=       0
Q_STEP11_REQUIREDVARS=         Count(X,SelectDescendants(Q_STEP11,Q_STEP11_WARNING),InputRequired(X))
Q_STEP11_ENTEREDREQUIREDVARS=  Count(X,SelectDescendants(Q_STEP11,Q_STEP11_WARNING),InputRequired(X) And DataAvailable(X))

