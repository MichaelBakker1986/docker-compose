; BalanceSheet
;

.Formulas NoTrend,Trend
FixedAssets                    = IntangibleFixedAssets+MaterialFixedAssets+FinancialFixedAssets
CurrentAssets                  = Inventories+Receivables+CashAndEquivalents+ProjectsInProgress


.Formulas Trend
TreasuryShares                 = Self[T-1]
CorrectionWorkInProgress       = Self[T-1]
MaterialsInventory             = If(CostOfMaterials=NA,MaterialsInventory[T-1],If(MaterialsInventoryTerm=NA,ExpandFraction(MaterialsInventory,CostOfMaterials),GetPoint(CostOfMaterials,MaterialsInventoryTerm/52)))        <-> MaterialsInventoryTerm
WorkInProgress                 = If(WorkInProgressTerm=NA,ExpandFraction(WorkInProgress,NetSales),GetPoint(NetSales,WorkInProgressTerm/52)) <-> WorkInProgressTerm
ProjectsInProgress             = Self[T-1]
InventoryFinishedProducts      = If(DataEntered(ChangeInFinishedProduct),InventoryFinishedProducts[T-1]+ChangeInFinishedProduct,Max(Min(InventoryFinishedProducts[T-1]+ProductionValue[T-1]-NetSales-ChangeInWorkInProgress,MaximumInventoryFinishedProducts),MinimumInventoryFinishedProducts)) <-> ChangeInFinishedProduct ;EJS: r41: NetSales verandert in CatKosten

ReceivablesGroupCompanies      = If(T=FirstTinFormulaset(Trend),Max(ReceivablesGroupCompanies[T-1]-CurrentAccountGroupCompanies[T-1],NA),Self[T-1])
ReceivablesAffiliatedCompanies = If(T=FirstTinFormulaset(Trend),Max(ReceivablesAffiliatedCompanies[T-1]-CurrentAccountAffiliatedCompanies[T-1],NA),Self[T-1])
OtherReceivables               = ExpandFraction(OtherReceivables,NetSales)
;TaxOnProfitsReceivable         = OnZero(OnEr(MAX(TaxOnProfits[GetT(T,-TsY,0,TsY)]/TsY-TaxOnProfits+If(T=ValueOfT(FirstTinYear),NA,TaxOnProfitsReceivable[T-1]-TaxOnProfitsPayable[T-1]),NA),NA),NA)
VATReceivable                  = Max(If(VATFrequency=0,NA,If(TsY<VATFrequency,-TsY/VATFrequency*VATBalance,If(((T-FirstTinFormulaSet) mod Round(TsY/VATFrequency))=0,-VATBalance,VATReceivable[T-1]-VATPayable[T-1]-VATBalance))),NA)

MarketableSecurities           = MarketableSecurities[T-1]
OperationalCash                = If(OperationalCashTotalPercentage=NA,ExpandLevel(OperationalCash),GetPoint(SalesIncludingVat,1)*OperationalCashTotalPercentage)

.Formulas NoTrend,Trend,User
RoundingDifference             = OnZero(If(Abs(TotalEquityAndLiabilities-VSUM(IntangibleFixedAssets,Receivables)-MarketableSecurities-ExcessCash-OperationalCash)>=Max(0.0001*TotalEquityAndLiabilities,(3*ViewScaleFactor*10^(-Decimals))),NA,TotalEquityAndLiabilities-VSUM(IntangibleFixedAssets,Receivables)-MarketableSecurities-ExcessCash-OperationalCash),NA)


.Formulas NoTrend, Trend
IssueanceSharedCapitalOrdinaryShares = If(LimitedLiabilityCompany,SharedCapitalOrdinaryShares-SharedCapitalOrdinaryShares[T-1],NA)
IssueanceSharedCapitalPreferenceShares = If(LimitedLiabilityCompany,SharedCapitalPreferenceShares-SharedCapitalPreferenceShares[T-1],NA)
ContributionOfCapitalToSharePremium = If(LimitedLiabilityCompany,SharePremium-SharePremium[T-1],NA)
ChangeInTreasuryShares = If(LimitedLiabilityCompany,TreasuryShares-TreasuryShares[T-1],NA)
AccountsPayableExpensesExclVAT = OnER(AccountsPayableExpenses/((OtherOperatingExpensesIncludingVAT)/TotalOtherOperatingExpenses),NA)
OperatingExpensesExclPersonnel = OnZero(OperatingExpenses-PersonnelExpenses-ContractedOut,NA)

.Formulas Trend
SharedCapitalOrdinaryShares    = If(LimitedLiabilityCompany, If(DataEntered(IssueanceSharedCapitalOrdinaryShares), SharedCapitalOrdinaryShares[T-1]+IssueanceSharedCapitalOrdinaryShares,SharedCapitalOrdinaryShares[T-1]),NA) <-> IssueanceSharedCapitalOrdinaryShares
SharedCapitalPreferenceShares  = If(LimitedLiabilityCompany, If(DataEntered(IssueanceSharedCapitalPreferenceShares), SharedCapitalPreferenceShares[T-1]+IssueanceSharedCapitalPreferenceShares,SharedCapitalPreferenceShares[T-1]),NA) <-> IssueanceSharedCapitalPreferenceShares
SharePremium                   = If(LimitedLiabilityCompany, If(DataEntered(ContributionOfCapitalToSharePremium), SharePremium[T-1]+ContributionOfCapitalToSharePremium,SharePremium[T-1]),NA) <-> ContributionOfCapitalToSharePremium

ReservedProfit                 = If(LimitedLiabilityCompany,ReservedProfit[T-1]+RetainedProfit-(OtherReserve-OtherReserve[T-1])+ContributionOfCapitalPartners,ReservedProfit[T-1] )
RevaluationReserve             = RevaluationReserve[T-1]+MutationRevaluationReserve
StatutoryReserve               = If(LimitedLiabilityCompany,StatutoryReserve[T-1],NA)
OtherReserve                   = If(LimitedLiabilityCompany,OtherReserve[T-1],NA)
MinorityInterests              = If(LimitedLiabilityCompany,MinorityInterests[T-1],NA)
PensionProvisions              = Self[T-1]
StandingRight                  = StandingRight[T-1]-StandingRightPayable
DeferredTaxes                  = DeferredTaxes[T-1]
ProvisionForReorganisation     = ProvisionForReorganisation[T-1]
OtherProvisions                = OtherProvisions[T-1]
ProvisionDoubtfulAccountsReceivable = ProvisionDoubtfulAccountsReceivable[T-1]
ProvisionObsoleteInventories   = ProvisionObsoleteInventories[T-1]

OtherPayables                  = OnZero(If(DataEntered(OtherPayablesTerm)   ,Max(0,GetPoint(OtherOperatingExpensesIncludingVAT,OtherPayablesTerm/(52+1/7)))   -AccountsPayableExpensesSubtotal,If(AccountsPayableExpensesSubtotal=NA,Max(0,GetPoint(OtherOperatingExpensesIncludingVAT,OtherPayablesTerm[T-1]/(52+1/7)))   -AccountsPayableExpensesSubtotal,OtherPayables[T-1]   )),NA) <-> OtherPayablesTerm
TradePayables                  = OnZero(If(DataEntered(TradePayablesTerm)   ,Max(0,GetPoint(TotalPurchasesIncVAT              ,TradePayablesTerm/(52+1/7)))   -CostOfSalesPayable             ,If(CostOfSalesPayable             =NA,Max(0,GetPoint(TotalPurchasesIncVAT              ,TradePayablesTerm[T-1]/(52+1/7)))   -CostOfSalesPayable             ,TradePayables[T-1]   )),NA) <-> TradePayablesTerm
TradeReceivables               = OnZero(If(DataEntered(TradeReceivablesTerm),Max(0,GetPoint(ProductSalesInvoicedNet           ,TradeReceivablesTerm/(52+1/7)))-TradeReceivablesSalesGroups    ,If(TradeReceivablesSalesGroups    =NA,Max(0,GetPoint(ProductSalesInvoicedNet           ,TradeReceivablesTerm[T-1]/(52+1/7)))-TradeReceivablesSalesGroups    ,TradeReceivables[T-1])),NA) <-> TradeReceivablesTerm

OtherPayablesTerm              = GetFrac(OperatingExpensesExclPersonnel    ,AccountsPayableExpensesExclVAT)*(52+1/7) <-> OtherPayables
TradePayablesTerm              = GetFrac(TotalPurchasesIncVAT              ,TradePayablesTotal)*(52+1/7)      <-> TradePayables
TradeReceivablesTerm           = GetFrac(ProductSalesInvoicedNet           ,TradeReceivablesTotal)*(52+1/7)   <-> TradeReceivables

OtherAccountsReceivable        = If(OtherAccountsReceivableTerm=NA,ExpandLevel(OtherAccountsReceivable),GetPoint(SalesIncludingVat,OtherAccountsReceivableTerm/52)) <-> OtherAccountsReceivableTerm
