.Formulas NoTrend
SubordinatedDebt_tpDataExists                     = If(Abs(HSum(SubordinatedDebt_tpBookValue,FirstTinFormulaSet(Notrend,MainPeriod),LastTinFormulaSet(Trend,MainPeriod)))>=1,0,1)
SubordinatedDebt_tpNewLoan                       = MutCalc*OnZero(SubordinatedDebt_tpPrincipal-SubordinatedDebt_tpPrincipal[T-1]+SubordinatedDebt_tpRepayment,NA)
SubordinatedDebt_tpRepayment                     = Max(SubordinatedDebt_tpPrincipal[T-1]-SubordinatedDebt_tpPrincipal,NA)

.Formulas NoTrend,Trend
SubordinatedDebt_tpPaymentFrequency              = DefaultDebtPaymentFrequency

.Formulas NoTrend
SubordinatedDebt_tpInterestPayment               = SubordinatedDebt_tpInterestExpenses
SubordinatedDebt_tpInterestRate                  = OnNeg(OnER(SubordinatedDebt_tpInterestExpenses/If((TsY/SubordinatedDebt_tpPaymentFrequency[1])>=1,SubordinatedDebt_tpPrincipal[T-1],SubordinatedDebt_tpPrincipal[T-1]-(1-(TsY/SubordinatedDebt_tpPaymentFrequency))/2*SubordinatedDebt_tpRepayment)*TsY,NA),NA)

.Formulas Trend
;SubordinatedDebt_tpRepayment                     = OnEr(Min(SubordinatedDebt_tpPrincipal[T-1],If((ValueOfT(GetT(T,-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1)))>= FirstTinFormulaSet) or (SubordinatedDebt_tpPaymentFrequency>=TsY),SubordinatedDebt_tpRepayment[(GetT(T,-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1)))],If(ValueOfT(T)-FirstTinFormulaSet+1=TsY/SubordinatedDebt_tpPaymentFrequency,GetValue(SubordinatedDebt_tpRepayment,GetT(T,-TsY,0,TsY),Min(SubordinatedDebt_tpPaymentFrequency,TsY)),NA)))+OnER(SubordinatedDebt_tpNewLoan[(GetT(T,-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1)))]/SubordinatedDebt_tpTerm[(GetT(T,-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1)))]/Min(SubordinatedDebt_tpPaymentFrequency,TsY),NA),NA)
SubordinatedDebt_tpShortTermPart                 = OnZero(Min(OnER(SubordinatedDebt_tpNewLoan/SubordinatedDebt_tpTerm,NA)+SubordinatedDebt_tpShortTermPart[T-1],SubordinatedDebt_tpPrincipal)*DefaultShortTermPartOfLongTermDebts,NA)

SubordinatedDebt_tpPrincipal                     = OnZero(If(DataEntered(SubordinatedDebt_tpBookValue),SubordinatedDebt_tpBookValue+SubordinatedDebt_tpShortTermPart,SubordinatedDebt_tpPrincipal[T-1]-SubordinatedDebt_tpRepayment+SubordinatedDebt_tpNewLoan),NA)
SubordinatedDebt_tpTerm                          = If(SubordinatedDebt_tpNewLoan>0,0,NA)
SubordinatedDebt_tpBookValue                     = SubordinatedDebt_tpPrincipal-SubordinatedDebt_tpShortTermPart <-> SubordinatedDebt_tpNewLoan
SubordinatedDebt_tpInterestExpenses              = OnZero(SubordinatedDebt_tpInterestRate/TsY*(SubordinatedDebt_tpPrincipal[T-1]+SubordinatedDebt_tpPrincipal)/2,NA)
;SubordinatedDebt_tpInterestRate                  = OnER(If(DataEntered(SubordinatedDebt_tpInterestExpenses),SubordinatedDebt_tpInterestExpenses/If((TsY/SubordinatedDebt_tpPaymentFrequency)>=1,SubordinatedDebt_tpPrincipal[T-1]+SubordinatedDebt_tpNewLoan/2,SubordinatedDebt_tpPrincipal[T-1]-(1-(TsY/SubordinatedDebt_tpPaymentFrequency))/2*SubordinatedDebt_tpShortTermPart)*TsY,If(SubordinatedDebt_tpInterestRate[T-1]<>NA,SubordinatedDebt_tpInterestRate[T-1],If(SubordinatedDebt_tpInterestExpenses[T-1]=NA, DefaultInterestRateSubordinatedDebt[1] ,GetValue(SubordinatedDebt_tpInterestExpenses,GetT(T,-1),1)/(SubordinatedDebt_tpPrincipal[T-1]+GetValue(SubordinatedDebt_tpShortTermPart,GetT(T,-1),1)/2)))),NA) <-> SubordinatedDebt_tpInterestExpenses
;SubordinatedDebt_tpInterestPayment               = If((T-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1))<ValueOfT(FirstValueT(SubordinatedDebt_tpInterestExpenses,FirstTinPeriod,T)),NA,If(HSum(SubordinatedDebt_tpInterestPayment,(T-Max(TsY/SubordinatedDebt_tpPaymentFrequency,1)+1),T-1)>0,NA,SubordinatedDebt_tpDeferredInterest[T-1]+SubordinatedDebt_tpInterestRate/TsY*(SubordinatedDebt_tpPrincipal[T-1]*0.5)))
;Betaling rente                                       indien de eerste rente/aflossings-termijn nog niet is verstreken                                                    ,NA, Indien er een rentebetaling is geweest in de afgelopen periode ter grootte van de rentebetalingstermijn, NA, anders is er kennelijk een termijn verstreken en betaal je de overlopende rente + rente over de helft van de huiidige kolomperiode.
SubordinatedDebt_tpDeferredInterest              = OnZero(SubordinatedDebt_tpDeferredInterest[T-1]+SubordinatedDebt_tpInterestExpenses-SubordinatedDebt_tpInterestPayment,NA)

.Choices
SubordinatedDebt_tpPaymentFrequency              = "|$>year<$|$>half year<$||$>quarter<$||||||||$>month<$"

