SafetyMargin                   = OnER((1-BreakEvenSales/NetSales),NA)
BreakEvenSales                 = OnER((GrossProfit-OrdinaryProfitBeforeTaxes)/GrossProfitMargin,NA)
;EBITDA                         = OnER(OperatingIncome+DepreciationOfAssets,NA)
;EBITDA                         = OnER(EBIT+DepreciationOfAssets,NA)
DebtToEBITDA                   = OnEr(CorrectedTotalDebt/(EBITDA*TsY),NA)
SalesToEBITDA                  = OnEr(EBITDA/NetSales,NA)
DebtToEBITA                    = OnEr(CorrectedTotalDebt/(EBITA*TsY),NA)
SalesToEBITA                   = OnEr(EBITA/NetSales,NA)
OperationalCashflowReturn      = OnER((NetOperatingProfit+DepreciationOfAssets)/(NetSales+OtherOperatingIncome),NA)
Leverage                       = If(NetWorth<=0,NA,OnER((ReturnOnAssets-(FinancialExpenses-TransferOfPaymentExpenses)*TsY/(TotalEquityAndLiabilities-NetWorth))*(TotalEquityAndLiabilities-NetWorth)/NetWorth,NA))
ReturnOnEquity                 = OnER((OrdinaryProfitBeforeTaxes+ShareResultFromSubsidiaries+ExtraordinaryIncomeAndExpenses)*TsY/Max(NetWorth,NA),NA)
ReturnOnAssets                 = OnER((OrdinaryProfitBeforeTaxes+ShareResultFromSubsidiaries+ExtraordinaryIncomeAndExpenses+FinancialExpenses-TransferOfPaymentExpenses)*TsY/TotalEquityAndLiabilities,NA)

; Productiviteitsratio's

.Formulas NoTrend,Trend
TotalWagesToGrossProfit = OnER(TotalWagesDepartment/GrossProfit,NA)
GrossProfitToTotalWages = OnER(GrossProfit/TotalWagesDepartment,NA)
SalesToTotalWages       = OnER(NetSales/TotalWagesDepartment,NA)

.Formulas NoTrend,Trend
SalesToNumberOfEmployees       = OnER(NetSales*ScaleFactor/TotalNumberOfEmployees,NA)
GrossProfitToNumberOfEmployees = OnER(GrossProfit*ScaleFactor/TotalNumberOfEmployees,NA)
PersonnelCostToNumberOfEmployees = OnER(PersonnelExpenses*ScaleFactor/TotalNumberOfEmployees,NA)

