        <MedewerkerVKOTitel>User</MedewerkerVKOTitel>
        <CreationDate><data src="functions" name="getDate"><param key="format" value="dd-MM-yyyy"/></data></CreationDate>
        <CreationDateTitle>Generated</CreationDateTitle>
        <!--  <CompanyName><data src="fesModel_TDSCRATING" referenceMode="warn"><columns number="1" /><variables depth="1" var="FPS_VAR_Naam"/></data></CompanyName> -->

        <Agreement><data src="totaalbericht_current" method="singleValue" select="(:DocumentGenerationType/:ProcessReferenceNumber/text())[1]"/></Agreement>
        <MainBorrower><data src="totaalbericht_current" method="singleValue" select="(:DocumentGenerationType/:Borrower[./:SelectedCounterpartyRoles/:DefinitionCode/text()='Hoofdaanvrager' or ./:SelectedCounterpartyRoles/:DefinitionCode/text()='Main applicant' or ./:SelectedCounterpartyRoles/:DefinitionCode/text()='Main borrower']/:OrganisationName/text())[1]"/></MainBorrower>

        <ReportScale>1</ReportScale>

        <visible_Declaration><data src="fesModel_TDSCRATING_current" referenceMode="warn"><columns number="1" /><variables depth="1" var="Declaration"/></data></visible_Declaration>
        <visible_IndependentOfScorecardType>false</visible_IndependentOfScorecardType>
        <!-- 1:Generic|2:Project|3:Cultural and Social|4:Renewable Energy| -->
        <visible_ScorecardType><data src="fesModel_TDSCRATING_current" referenceMode="warn"><columns number="1" /><variables depth="1" var="ScorecardType"/></data></visible_ScorecardType>
        <visible_ScorecardType_previous><data src="fesModel_TDSCRATING_previous" referenceMode="warn"><columns number="1" /><variables depth="1" var="ScorecardType"/></data></visible_ScorecardType_previous>

        <StartYear><data src="fesModel_TDSCRATING_current" referenceMode="warn"><columns number="1" order="desc"/><variables function="getEndDate" /><presentation format="yyyy"/></data></StartYear>
        <EndYear><data src="fesModel_TDSCRATING_current" referenceMode="warn"><columns number="1" order="asc"/><variables function="getEndDate" /><presentation format="yyyy"/></data></EndYear>


        <numJuridischePartij><data src="totaalbericht_current" method="singleValue" select="count(:DocumentGenerationType/:LegalStructure/:LegalParty)"/></numJuridischePartij>

        <AvailableModels>
          <TDSCAPPROVAL><data src="fesModel_TDSCAPPROVAL_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCAPPROVAL>
          <TDSCAPPROVALCAHQ><data src="fesModel_TDSCAPPROVALCAHQ_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCAPPROVALCAHQ>
          <TDSCAPPROVALHQ><data src="fesModel_TDSCAPPROVALHQ_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCAPPROVALHQ>
          <!--       <TDSCASSESSMENT><data src="fesModel_TDSCASSESSMENT" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCASSESSMENT> -->
          <!--       <TDSCASSESSMENT2><data src="fesModel_TDSCASSESSMENT2" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCASSESSMENT2> -->
          <!--       <TDSCCREDITLETTER><data src="fesModel_TDSCCREDITLETTER" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCCREDITLETTER> -->
          <TDSCCREDITPROPOSAL><data src="fesModel_TDSCCREDITPROPOSAL_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCCREDITPROPOSAL>
          <TDSCCREDITPROPOSALCA><data src="fesModel_TDSCCREDITPROPOSALCA_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCCREDITPROPOSALCA>
          <TDSCCREDITPROPOSALCM><data src="fesModel_TDSCCREDITPROPOSALCM_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCCREDITPROPOSALCM>
          <TDSCCREDITPROPOSALLE><data src="fesModel_TDSCCREDITPROPOSALLE_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCCREDITPROPOSALLE>
          <!--       <TDSCPRICING><data src="fesModel_TDSCPRICING" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCPRICING> -->
          <!--       <TDSCRATING><data src="fesModel_TDSCRATING" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></TDSCRATING> -->
          <fes_secondary_entities_TDSCRATING><data src="fes_secondary_entities_TDSCRATING_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="Q_ModelVersion" /><presentation scale="1"/></data></fes_secondary_entities_TDSCRATING>
        </AvailableModels>

        <TDSCAPPROVAL_ProposalApproved><data src="fesModel_TDSCAPPROVAL_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="ProposalApproved" /><presentation scale="1"/></data></TDSCAPPROVAL_ProposalApproved>
        <TDSCAPPROVALHQ_ProposalApproved><data src="fesModel_TDSCAPPROVALHQ_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="ProposalApproved" /><presentation scale="1"/></data></TDSCAPPROVALHQ_ProposalApproved>

        <TDSCAPPROVAL_ProposalApproved_previous><data src="fesModel_TDSCAPPROVAL_previous" referenceMode="warn"><columns number="1"/><variables depth="0" var="ProposalApproved" /><presentation scale="1"/></data></TDSCAPPROVAL_ProposalApproved_previous>
        <TDSCAPPROVALHQ_ProposalApproved_previous><data src="fesModel_TDSCAPPROVALHQ_previous" referenceMode="warn"><columns number="1"/><variables depth="0" var="ProposalApproved" /><presentation scale="1"/></data></TDSCAPPROVALHQ_ProposalApproved_previous>

        <triodosEntity_whitelabel><data src="totaalbericht_current" method="singleValue" select="count(/:DocumentGenerationType/:BusinessAgreement//:ProductLineDefinitionIdentification/:DefinitionDescription[not(contains(./text(), 'groenfonds') or contains(./text(), 'Groenfonds') or contains(./text(), 'cultuurfonds') or contains(./text(), 'Cultuurfonds'))])"/></triodosEntity_whitelabel>
        <triodosEntity_TGF><data src="totaalbericht_current" method="singleValue" select="count(/:DocumentGenerationType/:BusinessAgreement//:ProductLineDefinitionIdentification/:DefinitionDescription[contains(./text(), 'groenfonds') or contains(./text(), 'Groenfonds')])"/></triodosEntity_TGF>
        <triodosEntity_TCF><data src="totaalbericht_current" method="singleValue" select="count(/:DocumentGenerationType/:BusinessAgreement//:ProductLineDefinitionIdentification/:DefinitionDescription[contains(./text(), 'cultuurfonds') or contains(./text(), 'Cultuurfonds')])"/></triodosEntity_TCF>

        <proposalType><data src="fesModel_TDSCCREDITPROPOSAL_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="scorecardType" /><presentation scale="1"/></data></proposalType>
      </properties>

      <xsl:variable name="scorecardTypeCurrent">
        <data src="fesModel_TDSCRATING_current"><columns number="1"/><variables depth="0" var="ScorecardType" /><presentation scale="1"/></data>
      </xsl:variable>

      <xsl:variable name="scorecardTypePrevious">
        <xsl:value-of select="''"/>
        <data src="fesModel_TDSCRATING_previous"><columns number="1"/><variables depth="0" var="ScorecardType" /><presentation scale="1"/></data>
      </xsl:variable>

      <xsl:variable name="viewCurrency"><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables var="Root" function="getViewCurrency"/></data></xsl:variable>

      <xsl:variable name="pricingDocuments" select="fes:getDocumentIndicesByModelName('TDSCPRICING')" as="xs:integer *"/>
      <xsl:variable name="currentPricingDocument" as="xs:integer">
        <xsl:choose>
          <xsl:when test="count($pricingDocuments) >= 1">
            <xsl:value-of select="$pricingDocuments[1]"/>
          </xsl:when>
          <xsl:otherwise>
            <xs:integer>-1</xs:integer>
          </xsl:otherwise>
        </xsl:choose>
      </xsl:variable>
      <xsl:variable name="previousPricingDocument" as="xs:integer">
        <xsl:choose>
          <xsl:when test="count($pricingDocuments) >= 2">
            <xsl:value-of select="$pricingDocuments[2]"/>
          </xsl:when>
          <xsl:otherwise>
            <xs:integer>-1</xs:integer>
          </xsl:otherwise>
        </xsl:choose>
      </xsl:variable>

	  <xsl:variable name="complyBoolean"><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables depth="0" var="ComplyFlcA"></variables></data></xsl:variable>
	  
      <!-- =========data============================================================= -->

      <newpage name="normal">
        <p   id="watermark"  visible="./document/properties/isFinalReport/text()=0"/>
      </newpage>

      <xsl:variable name="facilities">
        <facilities>
          <xsl:for-each select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities']">
            <identifierABC><xsl:attribute name="id"><xsl:value-of select="fbl:FacilityReferenceNumber/text()" /></xsl:attribute><xsl:value-of select="codepoints-to-string(position()+64)" /></identifierABC>
          </xsl:for-each>
        </facilities>
      </xsl:variable>
      <!--
<p>TDSCAPPROVAL: <data src="document" type="calc" var="string(count(./*/*/document[1]/properties/AvailableModels/TDSCAPPROVAL/text()))" /></p>
<p>TDSCCREDITPROPOSAL: <data src="document" type="calc" var="string(count(./*/*/document[1]/properties/AvailableModels/TDSCCREDITPROPOSAL/text()))" /></p>
<p>fes_secondary_entities_TDSCRATING: <data src="document" var="./*/*/document[1]/properties/AvailableModels/fes_secondary_entities_TDSCRATING/text()" /></p>
-->

      <!-- oude stijl: -->
      <xsl:variable name="viewCurrency"><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables var="Root" function="getViewCurrency"/></data></xsl:variable>
      <xsl:variable name="tdscpricing">
        <table class="invisible">
          <tr>
            <td>fes_TDSCPRICING</td>
          </tr>
          <data src="fes_TDSCPRICING_current">
            <columns number="1" order="asc" />
            <variables depth="6" var="Facility" recursiveSkip="false"/>
            <presentation titleFunction="showVarName" disableMinusCorrection="true" titleSuffix="" showSingleValueVariablesOnce="true" showHeaderRow="false" scale="./*/*/document[1]/properties/ReportScale/text()">
              <overrideStyle old="level-1">header</overrideStyle>
              <overrideStyle old="level0empty">header</overrideStyle>
            </presentation>
          </data>
        </table>
      </xsl:variable>

      <xsl:variable name="tdscpricing_previous">
        <table class="invisible_previous">
          <tr>
            <td>fes_TDSCPRICING</td>
          </tr>
          <data src="fes_TDSCPRICING_previous">
            <columns number="1" order="asc" />
            <variables depth="6" var="Facility" recursiveSkip="false"/>
            <presentation titleFunction="showVarName" titleSuffix="" showSingleValueVariablesOnce="true" showHeaderRow="false" scale="./*/*/document[1]/properties/ReportScale/text()">
              <overrideStyle old="level-1">header</overrideStyle>
              <overrideStyle old="level0empty">header</overrideStyle>
            </presentation>
          </data>
        </table>
      </xsl:variable>

      <!-- <p>fes_TDSCPRICING1: <xsl:value-of select="($tdscpricing/table/tr[td/text()='Facility_tpID']/td[2]/text())[1]"></xsl:value-of></p> -->

      <!-- oude stijl: -->
      <xsl:variable name="tdscrating">
        <table class="invisible">
          <tr>
            <td>fes_TDSCRATING</td>
          </tr>
          <!-- MAIN BORROWER -->
          <data src="fes_TDSCRATING_current">
            <columns number="1" order="asc" />
            <variables depth="2" var="ReportVariables" recursiveSkip="false" />
            <presentation titleFunction="showVarName" titleSuffix="" showInvisibleVariables="true" showSingleValueVariablesOnce="true" showHeaderRow="false" scale="./*/*/document[1]/properties/ReportScale/text()">
              <overrideStyle old="level-1">header</overrideStyle>
              <overrideStyle old="level0empty">header</overrideStyle>
            </presentation>
          </data>
          <!-- SECONDARY BORROWERS -->
          <data src="fes_secondary_entities_TDSCRATING_current">
            <columns number="1" order="asc" />
            <variables depth="2" var="ReportVariables" recursiveSkip="false" loopDocuments="true" />
            <presentation titleFunction="showVarName" titleSuffix="" showInvisibleVariables="true" showSingleValueVariablesOnce="true" showHeaderRow="false" scale="./*/*/document[1]/properties/ReportScale/text()">
              <overrideStyle old="level-1">header</overrideStyle>
              <overrideStyle old="level0empty">header</overrideStyle>
            </presentation>
          </data>
        </table>
      </xsl:variable>

      <xsl:variable name="TotalAmountActiveProvisionsCurrent" select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//fbl:TotalAmountActiveProvisions/fbl:Amount" />
      <xsl:variable name="TotalAmountActiveProvisionsPrevious" select="$totaalbericht_xml_previous/fbl:DocumentGenerationType/fbl:BusinessAgreement//fbl:TotalAmountActiveProvisions/fbl:Amount" />

      <xsl:variable name="TotalAmountActiveProvisionsCurrentFormatted">
        <xsl:choose>
          <xsl:when test="$TotalAmountActiveProvisionsCurrent != ''">
            <xsl:value-of select="$TotalAmountActiveProvisionsCurrent" />
          </xsl:when>
          <xsl:otherwise>
            <xsl:value-of select="0" />
          </xsl:otherwise>
        </xsl:choose>
      </xsl:variable>

      <xsl:variable name="TotalAmountActiveProvisionsPreviousFormatted">
        <xsl:choose>
          <xsl:when test="$TotalAmountActiveProvisionsPrevious != ''">
            <xsl:value-of select="$TotalAmountActiveProvisionsPrevious" />
          </xsl:when>
          <xsl:otherwise>
            <xsl:value-of select="0" />
          </xsl:otherwise>
        </xsl:choose>
      </xsl:variable>


      <xsl:variable name="AgreementData" >

        <Current>
          <xsl:for-each select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement/fbl:Facilities">
            <Facility>
              <FaciltyReferenceNumber>
                <xsl:value-of select="fbl:FacilityReferenceNumber" />
              </FaciltyReferenceNumber>
					 <xsl:if test="fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text() !=''">
						<Limit>
						   <xsl:value-of select="fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount"/>
						</Limit>
					 </xsl:if>
              <UnauthorisedOverdraft>
                <xsl:choose>
                  <xsl:when test="fbl:UnauthorisedOverdraft/fbl:Amount" >
                    <xsl:value-of select="fbl:UnauthorisedOverdraft/fbl:Amount" />
                  </xsl:when>
                  <xsl:otherwise>0</xsl:otherwise>
                </xsl:choose>
              </UnauthorisedOverdraft>
            </Facility>
          </xsl:for-each>

          <xsl:for-each select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:PledgingCover">
            <Collateral>
              <xsl:choose>
                <xsl:when test="fbl:CustomEconomicCapitalRecoveryValue/fbl:Amount != ''" >
                  <EC><xsl:value-of select="fbl:CustomEconomicCapitalRecoveryValue/fbl:Amount" /></EC>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:if test="fbl:EconomicCapitalRecoveryValue/fbl:Amount != ''">
                    <EC><xsl:value-of select="fbl:EconomicCapitalRecoveryValue/fbl:Amount" /></EC>
                  </xsl:if>
                </xsl:otherwise>
              </xsl:choose>
            </Collateral>
          </xsl:for-each>

        </Current>

        <Previous>
          <xsl:for-each select="$totaalbericht_xml_previous/fbl:DocumentGenerationType/fbl:BusinessAgreement/fbl:Facilities">
            <Facility>
					 <xsl:if test="fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text() !=''">
						<Limit>
						   <xsl:value-of select="fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount"/>
						</Limit>
					 </xsl:if>
			  <UnauthorisedOverdraft>
                <xsl:choose>
                  <xsl:when test="fbl:UnauthorisedOverdraft/fbl:Amount" >
                    <xsl:value-of select="fbl:UnauthorisedOverdraft/fbl:Amount" />
                  </xsl:when>
                  <xsl:otherwise>0</xsl:otherwise>
                </xsl:choose>
              </UnauthorisedOverdraft>
            </Facility>
          </xsl:for-each>

          <xsl:for-each select="$totaalbericht_xml_previous/fbl:DocumentGenerationType/fbl:PledgingCover">
            <Collateral>
              <xsl:choose>
                <xsl:when test="fbl:CustomEconomicCapitalRecoveryValue/fbl:Amount != ''" >
                  <EC><xsl:value-of select="fbl:CustomEconomicCapitalRecoveryValue/fbl:Amount" /></EC>
                </xsl:when>
                <xsl:otherwise>
                  <xsl:if test="fbl:EconomicCapitalRecoveryValue/fbl:Amount != ''">
                    <EC><xsl:value-of select="fbl:EconomicCapitalRecoveryValue/fbl:Amount" /></EC>
                  </xsl:if>
                </xsl:otherwise>
              </xsl:choose>
            </Collateral>
          </xsl:for-each>
        </Previous>

      </xsl:variable>


      <p class="section"><data src="fesModel_TDSCCREDITPROPOSAL_current" referenceMode="warn"><columns number="1"/><variables depth="0" var="ScorecardType" /><presentation scale="1"/></data></p>

      <p class="subsection">SUMMARY</p>

