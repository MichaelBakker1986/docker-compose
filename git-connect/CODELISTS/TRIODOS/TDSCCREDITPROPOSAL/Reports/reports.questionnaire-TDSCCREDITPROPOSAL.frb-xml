        </tr>
        <tr class="summary3cols">
          <td></td>
          <td>New</td>
          <td>Old</td>
        </tr>
        <xsl:for-each select="$allFacilities">
          <xsl:variable name="id" select="."/>
          <xsl:variable name="identifierABC" select="codepoints-to-string(position()+64)" />
          <xsl:variable name="currentFacility" select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities'][fbl:FacilityReferenceNumber/text() = $id]" />
          <xsl:variable name="previousFacility" select="$totaalbericht_xml_previous/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities'][fbl:FacilityReferenceNumber/text() = $id]" />
          <xsl:variable name="currentTupleIndices" select="fes:getTupleIndices($currentPricingDocument, 'Facility_tpID', $id)" as="xs:integer *" />
          <xsl:variable name="previousTupleIndices" select="fes:getTupleIndices($previousPricingDocument, 'Facility_tpID', $id)" as="xs:integer *" />

          <tr class="summary3cols">
            <td>Facility <xsl:value-of select="$identifierABC"/>:</td>
            <td><xsl:value-of select="$currentFacility/fbl:FacilityReferenceNumber/text()"/></td>
            <td><xsl:value-of select="$previousFacility/fbl:FacilityReferenceNumber/text()"/></td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Type:</td>
            <td><xsl:value-of select="$currentFacility/fbl:ProductLineDefinitionIdentification/fbl:DefinitionDescription/text()"/></td>
            <td><xsl:value-of select="$previousFacility/fbl:ProductLineDefinitionIdentification/fbl:DefinitionDescription/text()"/></td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Contractual Limit:</td>
            <td>
			 <xsl:if test="count($currentFacility/fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text())&gt;0">
				<xsl:value-of select="concat($viewCurrency, ' ')"/> <xsl:value-of select="format-number(number($currentFacility/fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text()), '0')"/>
			 </xsl:if> 
            </td>
			<!--To do: Use view currency vanuit limit in plaats van viewCurrency zoals hierboven gedefinieerd-->
            <td>
			 <xsl:if test="count($previousFacility/fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text())&gt;0">
				<xsl:value-of select="concat($viewCurrency, ' ')"/> <xsl:value-of select="format-number(number($previousFacility/fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount/text()), '0')"/>
			 </xsl:if> 
            </td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Agreed Term:</td>
            <td><xsl:value-of select="concat($currentFacility/fbl:Product[1]/fbl:Duration/text(), substring(' months', 1 div number(boolean(count($currentFacility/fbl:Product[1]/fbl:Duration/text())>0))))"/></td>
            <td><xsl:value-of select="concat($previousFacility/fbl:Product[1]/fbl:Duration/text(), substring(' months', 1 div number(boolean(count($previousFacility/fbl:Product[1]/fbl:Duration/text())>0))))"/></td>
          </tr>
          <xsl:if test="not($currentFacility/fbl:ProductLineDefinitionIdentification/text()='Guarantee account') and not($currentFacility/fbl:ProductLineDefinitionIdentification/text()='Current account') and not($previousFacility/fbl:ProductLineDefinitionIdentification/text()='Guarantee account') and not($previousFacility/fbl:ProductLineDefinitionIdentification/text()='Current account')">
            <tr class="summary3cols1indent">
              <td>Amortization:</td>
              <td>
                <xsl:if test="not($currentFacility/fbl:ProductLineDefinitionIdentification/text()='Guarantee account') and not($currentFacility/fbl:ProductLineDefinitionIdentification/text()='Current account')">
                  <xsl:value-of select="$currentFacility/fbl:Product[1]/fbl:Repayment/fbl:RepaymentMethodDefinitionIdentification/fbl:DefinitionDescription/text()"/>
                </xsl:if>
              </td>
              <td>
                <xsl:if test="not($previousFacility/fbl:ProductLineDefinitionIdentification/text()='Guarantee account') and not($previousFacility/fbl:ProductLineDefinitionIdentification/text()='Current account')">
                  <xsl:value-of select="$previousFacility/fbl:Product[1]/fbl:Repayment/fbl:RepaymentMethodDefinitionIdentification/fbl:DefinitionDescription/text()"/>
                </xsl:if>
              </td>
            </tr>
          </xsl:if>
          <tr class="summary3cols1indent">
            <td>Frequency:</td>
            <td><xsl:value-of select="$currentFacility/fbl:Product[1]/fbl:Repayment/fbl:RepaymentPeriodicityDefinitionIdentification/fbl:DefinitionDescription/text()"/></td>
            <td><xsl:value-of select="$previousFacility/fbl:Product[1]/fbl:Repayment/fbl:RepaymentPeriodicityDefinitionIdentification/fbl:DefinitionDescription/text()"/></td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Interest Rate Type:</td>
            <td><xsl:value-of select="$currentFacility/fbl:Product[1]/fbl:InterestType/text()"/></td>
            <td><xsl:value-of select="$previousFacility/fbl:Product[1]/fbl:InterestType/text()"/></td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Interest Percentage:</td>
            <td><xsl:if test="count($currentFacility) > 0 and count($currentTupleIndices) > 0"><xsl:value-of select="fes:getValue($currentPricingDocument, 'Facility_tpPrintedRate', $currentTupleIndices)"/></xsl:if></td>
            <td><xsl:if test="count($previousFacility) > 0 and count($previousTupleIndices) > 0"><xsl:value-of select="fes:getValue($previousPricingDocument, 'Facility_tpPrintedRate', $previousTupleIndices)"/></xsl:if></td>
          </tr>
          <tr class="summary3cols1indent">
            <td>Interest Only Period:</td>
            <td><xsl:value-of select="concat($currentFacility/fbl:Product[1]/fbl:Repayment/fbl:FirstRepaymentAfterXMonths/text(), substring(' months', 1 div number(boolean(count($currentFacility/fbl:Product[1]/fbl:Repayment/fbl:FirstRepaymentAfterXMonths/text())>0))))"/></td>
            <td><xsl:value-of select="concat($previousFacility/fbl:Product[1]/fbl:Repayment/fbl:FirstRepaymentAfterXMonths/text(), substring(' months', 1 div number(boolean(count($previousFacility/fbl:Product[1]/fbl:Repayment/fbl:FirstRepaymentAfterXMonths/text())>0))))"/></td>
          </tr>
          <xsl:if test="count($currentFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text())>0 or count($previousFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text())>0">
            <tr class="summary3cols1indent">
              <td>Floor Interest Rate:</td>
              <td><xsl:if test="count($currentFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text())>0"><xsl:value-of select="$currentFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text()"/>%</xsl:if></td>
              <td><xsl:if test="count($previousFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text())>0"><xsl:value-of select="$previousFacility/fbl:Product[1]/fbl:NominalFloorInterestRate/text()"/>%</xsl:if></td>
            </tr>
          </xsl:if>
          <xsl:if test="count($currentFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text())>0 or count($previousFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text())>0">
            <tr class="summary3cols1indent">
              <td>Cap Interest Rate:</td>
              <td><xsl:if test="count($currentFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text())>0"><xsl:value-of select="$currentFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text()"/>%</xsl:if></td>
              <td><xsl:if test="count($previousFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text())>0"><xsl:value-of select="$previousFacility/fbl:Product[1]/fbl:NominalCapInterestRate/text()"/>%</xsl:if></td>
            </tr>
          </xsl:if>
          <tr class="summary3cols1indent">
            <td></td>
            <td></td>
            <td></td>
            <td></td>
          </tr>
        </xsl:for-each>
      </table>

