        <xsl:for-each select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities']">
          <xsl:value-of select="fbl:FacilityReferenceNumber/text()"/>
        </xsl:for-each>
        <xsl:for-each select="$totaalbericht_xml_previous/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities']">
          <xsl:variable name="id" select="fbl:FacilityReferenceNumber/text()"/>
          <xsl:if test="count($totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities'][fbl:FacilityReferenceNumber/text() = $id]) = 0">
            <xsl:value-of select="fbl:FacilityReferenceNumber/text()"/>
          </xsl:if>
        </xsl:for-each>
      </xsl:variable>

      <table class="summarytable2">
        <tr class="header">
