        <xsl:for-each select="$totaalbericht_xml_current/fbl:DocumentGenerationType/fbl:BusinessAgreement//*[local-name()='Facility' or local-name()='Facilities']">
          <xsl:variable name="id" select="fbl:FacilityReferenceNumber/text()"/>
          <xsl:variable name="security" select="($tdscpricing/table/tr[./td/text()=$id][./td/text()='Facility_tpID']/following-sibling::tr[./td/text()='Facility_tpAllocatedColTotal']/td[2])[1]/text()" />
          <xsl:variable name="expectedLoss" select="($tdscpricing/table/tr[./td/text()=$id][./td/text()='Facility_tpID']/following-sibling::tr[./td/text()='Facility_tpEL']/td[2])[1]/text()" />
          <tr class="summary2cols">
            <td>Facility <xsl:value-of select="$facilities//identifierABC[@id=$id]/text()"/> :</td>
            <td><xsl:value-of select="$id" /></td>
          </tr>
          <tr class="summary2cols1indent">
            <td>Name:</td>
            <td><xsl:value-of select="./fbl:ProductLineDefinitionIdentification/fbl:DefinitionDescription/text()"/></td>
          </tr>
          <tr class="summary2cols1indent">
            <td>Exposure Triodos bank:</td>
            <td><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables var="Root" function="getViewCurrency"/></data><xsl:text> </xsl:text>
              <xsl:variable name="unauthOverdraft">
                <xsl:choose>
                  <xsl:when test="./fbl:UnauthorisedOverdraft/fbl:Amount/text() != ''"><xsl:value-of select="number(./fbl:UnauthorisedOverdraft/fbl:Amount)" /></xsl:when>
                  <xsl:otherwise>0</xsl:otherwise>
                </xsl:choose>
              </xsl:variable>
			  
              <xsl:value-of select="format-number(number(fbl:Limit[./fbl:LimitDefinitionIdentification/fbl:DefinitionCode/text() ='ContractueleLimiet']/fbl:Value/fbl:Amount) + $unauthOverdraft, '.###,##', 'euroFormat')" /></td>  <!-- moet zijn totaal aan faciliteiten + valuta -->
          </tr>
          <tr class="summary2cols1indent">
            <td>Collateral:</td>
            <td><xsl:if test="$security != ''"><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables var="Root" function="getViewCurrency"/></data><xsl:text> </xsl:text>
              <xsl:value-of select="$security"/></xsl:if></td>
          </tr>
          <tr class="summary2cols1indent">
            <td>Expected Loss:</td>
            <td><xsl:if test="$expectedLoss != ''"><data src="fesModel_TDSCCREDITPROPOSAL_current"><variables var="Root" function="getViewCurrency"/></data><xsl:text> </xsl:text>
              <xsl:value-of select="$expectedLoss"/></xsl:if></td>
          </tr>
          <tr class="summary2cols1indent"><td></td><td></td></tr>
        </xsl:for-each>
      </table>

