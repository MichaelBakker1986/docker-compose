CostsVolumeRatio=              "Calculation: (Cost of Sales + Operating Expenses + Depreciation of Assets + Financial Expenses) / Net Sales"
DebtCoverageRatio=             "Calculation: (Operatingincome + Depreciation of Assets) / Financial Expenses"
DebtRatio=                     "Calculation: (NonCurrent Liabilities + Current Liabilities - Cash and Equivalents - Other Securities) / Total  Assets"
EquityRatio=                   "Calculation: NetWorth / Total Assets"
CashRatio=                     "Calculation: Cash and Equivalents / Current Liabilities"
ReturnonAssets=                "Calculation: Profit after Tax / Total Assets"
TotalSales=                    "Is equal to Net Sales"

.Formulas NoTrend
CostsVolumeRatio=              OnER(((CostOfSales[LastTinFormulaSet(NoTrend,MainPeriod),1] + OperatingExpenses[LastTinFormulaSet(NoTrend,MainPeriod),1] + DepreciationOfAssets[LastTinFormulaSet(NoTrend,MainPeriod),1] + FinancialExpenses[LastTinFormulaSet(NoTrend,MainPeriod),1]) / NetSales[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
DebtCoverageRatio=             OnER(((OperatingIncome[LastTinFormulaSet(NoTrend,MainPeriod),1] + DepreciationOfAssets[LastTinFormulaSet(NoTrend,MainPeriod),1]) / FinancialExpenses[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
DebtRatio=                     OnER(((((NonCurrentLiabilities[LastTinFormulaSet(NoTrend,MainPeriod),1] + CurrentLiabilities[LastTinFormulaSet(NoTrend,MainPeriod),1]) - CashAndEquivalents[LastTinFormulaSet(NoTrend,MainPeriod),1]) - OtherSecurities[LastTinFormulaSet(NoTrend,MainPeriod),1]) / TotalAssets[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
EquityRatio=                   OnER((NetWorth[LastTinFormulaSet(NoTrend,MainPeriod),1] / TotalAssets[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
CashRatio=                     OnER((CashAndEquivalents[LastTinFormulaSet(NoTrend,MainPeriod),1] / CurrentLiabilities[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
ReturnonAssets=                OnER((ProfitAfterTax[LastTinFormulaSet(NoTrend,MainPeriod),1] / TotalAssets[LastTinFormulaSet(NoTrend,MainPeriod),1]),NA)
TotalSales=                    If( &SourceCurrency="EUR" ,NetSales[LastTinFormulaSet(NoTrend,MainPeriod),1],NA)

