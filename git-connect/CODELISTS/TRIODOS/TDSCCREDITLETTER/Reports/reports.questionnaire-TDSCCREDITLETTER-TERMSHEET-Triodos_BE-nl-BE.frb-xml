							<xsl:if test="$zekerheidsType_value != $fonds_bruxellois_garantie and $zekerheidsType_value != $debenture_subsidy_value and $zekerheidsType_value != $debentureReceivables_value and $zekerheidsType_value != $debentureInventory_value"><p>[color=#E36C0A][b]A collateral is missing, enter text manually[/b][/color]</p></xsl:if>
						</xsl:otherwise>
					</xsl:choose>
