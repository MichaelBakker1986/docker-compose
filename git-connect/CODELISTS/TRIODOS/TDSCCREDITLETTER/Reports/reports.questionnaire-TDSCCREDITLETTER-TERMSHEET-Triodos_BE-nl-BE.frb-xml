							<xsl:value-of select="$tupleIndex"/>
						</xs:sequence>
					</xsl:variable> 
					<!-- Loop rollen tuple -->
					<xsl:for-each select="fes:getTupleInstances($doc, $contractantRollen_variable, $tupleIndices)">
