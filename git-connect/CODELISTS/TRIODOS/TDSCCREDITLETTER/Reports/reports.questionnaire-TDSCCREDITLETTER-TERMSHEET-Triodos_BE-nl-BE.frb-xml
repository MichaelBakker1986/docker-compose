					<xsl:if test="$zekerheidsType_value = $waarborgbeheer_value">
						<waarborgbeheer>1</waarborgbeheer>
					</xsl:if>

					<xsl:if test="$zekerheidsType_value = $fonds_bruxellois_garantie">
