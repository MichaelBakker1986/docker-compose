							</xsl:for-each>
							<xs:sequence>
								<xsl:value-of select="$subTupleIndex"/>
							</xs:sequence>
						</xsl:variable>

						<xsl:variable name="rol_naam_value" select="fes:getFormattedValue($doc, $contractantRolcontractantNaam_variable, $subTupleIndices)" />
						<!-- Only for Main borrower -->
