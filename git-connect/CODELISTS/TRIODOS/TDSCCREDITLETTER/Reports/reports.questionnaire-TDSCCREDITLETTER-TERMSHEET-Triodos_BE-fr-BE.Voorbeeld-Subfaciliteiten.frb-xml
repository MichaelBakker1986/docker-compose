								<xsl:variable name="amountOfAllocatie" select="fes:getTupleInstanceCount($doc,$allocatie_variable,$tupleIndices)" />

								<!-- Collateral allocated to agreement -->
								<xsl:if test="$amountOfAllocatie = 0">
									<amount>1</amount>
								</xsl:if>

								<!-- Loop allocatie tuple -->
								<xsl:for-each select="fes:getTupleInstances($doc, $allocatie_variable,$tupleIndices)">
									<xsl:variable name="allocatieTupleIndex" select="." as="xs:integer"/>
