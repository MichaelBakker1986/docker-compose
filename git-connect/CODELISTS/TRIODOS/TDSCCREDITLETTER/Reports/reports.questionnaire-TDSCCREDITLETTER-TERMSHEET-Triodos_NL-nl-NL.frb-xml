			  <xsl:variable name="subTupleIndices" as="xs:integer *">
				  <xsl:for-each select="$tupleIndices">
					<xs:sequence><xsl:value-of select="."/></xs:sequence>
				  </xsl:for-each>
				  <xs:sequence><xsl:value-of select="$subTupleIndex"/></xs:sequence>
