					<xsl:variable name="subTupleIndex" select="." as="xs:integer"/>
					<xsl:variable name="subTupleIndices" as="xs:integer *">
						<xsl:for-each select="$tupleIndices">
							<xs:sequence>
								<xsl:value-of select="."/>
							</xs:sequence>
						</xsl:for-each>
