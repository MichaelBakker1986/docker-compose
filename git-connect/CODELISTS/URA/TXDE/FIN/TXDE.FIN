; v1.14.8 BK: Get Benchmark from Matrix
BenchmarkYearIndex    = If((YearInT>2011) and (YearInT<2020),YearInT-2011,NA)
branchIndexAlpha      = TableKeyLookup("sbiIndex",&FPS_VAR_BIK_CODE[MainPeriod])
branchIndexNumKey     = &If(IsValue(Val(&"0."&FPS_VAR_BIK_CODE[MainPeriod]&"1")),&"0."&FPS_VAR_BIK_CODE[MainPeriod]&"1",NA)
branchIndex           = OnNA(branchIndexAlpha,MatrixLookup("SRA_PDs.xls","SbiToIndex",&branchIndexNumKey,1))
branchBenchmarkIndex  = If(IsValue(BenchmarkYearIndex),MatrixLookup("SRA_PDs.xls","SbiMap",branchIndex,BenchmarkYearIndex),NA)
branchBenchmarkCode   = &MatrixLookup("SRA_PDs.xls","IndexToSbiAndName",branchBenchmarkIndex,1)
branchBenchmarkName   = &MatrixLookup("SRA_PDs.xls","IndexToSbiAndName",branchBenchmarkIndex,2)
krPDBenchmark         = If(IsValue(BenchmarkYearIndex),MatrixLookup("SRA_PDs.xls","BenchmarkPD",branchBenchmarkIndex,BenchmarkYearIndex),NA)
krPDBenchmarkKlasse   = &If(krPDBenchmark=NA," ",&TableLookup("krPDtoUraTitle",krPDBenchmark))

