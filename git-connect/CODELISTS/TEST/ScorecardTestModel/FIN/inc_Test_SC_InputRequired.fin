PII     1A     5               "Verplichte variabele dieper niveau"                                \tirSubLevelRequired

PII     1N C 1 4               "Verplicht veld originelen (hele var)"                              \tirOrgsInputRequired
PII     1N C   4               "Verplicht veld kopieën (hele var)"                                 \tirCopiesInputRequired
PII     1N C   4               "Verplicht veld originelen (last T in NoTrend)"                     \tirOrgsLastTInputRequired
PII     1N C   4               "Verplicht veld kopieën (last T in NoTrend)"                        \tirCopiesLastTInputRequired
PII     1N C   4               "Verplicht veld originelen (first T in NoTrend)"                    \tirOrgsFirstTInputRequired
PII     1N C   4               "Verplicht veld kopieën (first T in NoTrend)"                       \tirCopiesFirstTInputRequired
PII     1N C   4               "Zichtbaarheid tussenkopje dieper niveau"                           \tirVisibilitySubLevel

.Choices
tirOrgsInputRequired= "0:niet verplicht|1:wel verplicht"
tirCopiesInputRequired= "0:niet verplicht|1:wel verplicht"
tirOrgsLastTInputRequired= "0:niet verplicht|1:wel verplicht"
tirCopiesLastTInputRequired= "0:niet verplicht|1:wel verplicht"
tirOrgsFirstTInputRequired= "0:niet verplicht|1:wel verplicht"
tirCopiesFirstTInputRequired= "0:niet verplicht|1:wel verplicht"
tirVisibilitySubLevel= "0:hidden|1:visible"

.Formulas InputRequired
tirStrVarOrg= tirOrgsInputRequired or (tirOrgsLastTInputRequired and (T=LastTinFormulaSet(NoTrend))) or (tirOrgsFirstTInputRequired and (T=FirstTinFormulaSet(NoTrend)))
tirColVarOrg= tirOrgsInputRequired or (tirOrgsLastTInputRequired and (T=LastTinFormulaSet(NoTrend))) or (tirOrgsFirstTInputRequired and (T=FirstTinFormulaSet(NoTrend)))
tirStrVarCopy= tirCopiesInputRequired or (tirCopiesLastTInputRequired and (T=LastTinFormulaSet(NoTrend))) or (tirCopiesFirstTInputRequired and (T=FirstTinFormulaSet(NoTrend)))
tirColVarCopy= tirCopiesInputRequired or (tirCopiesLastTInputRequired and (T=LastTinFormulaSet(NoTrend))) or (tirCopiesFirstTInputRequired and (T=FirstTinFormulaSet(NoTrend)))
tirSubLevelRequired= 1

.Formulas Visible
tirSubLevelTop= tirVisibilitySubLevel

.Formulas NoTrend
tirSubLevelRequired= 1


