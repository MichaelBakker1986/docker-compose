{&&Language=English};Inc_InterestTest.fin

.Variables
PP    0 1N     1               "IRR / NPV Tests"              \IRRNPVTests

;-------------------------------------------------------------------------------------------------------------------------------------- A
; IRR
.Variables
PPP   0 1N     2               "IRR"                                      \IRRVar
PII   2 FN     3               "IRR CashFlow"                             \IRRCashFlow
PII   2 PN     3               "IRR Start Column"                         \IRRStartColumn
PII   2 PN     3-              "IRR End Column"                           \IRREndColumn
PPP   2 PN   1 3               "IRR Result"                               \IRRResult

.Formulas NoTrend
IRRStartColumn			= 1
IRREndColumn			= 12
IRRResult				= IRR(IRRCashFlow, IRRStartColumn, IRREndColumn)

;-------------------------------------------------------------------------------------------------------------------------------------- A
; NPV
.Variables
PPP   0 1N     2               "NPV"                                      \NPVVar
PII   2 PN %   3               "NPV Rate"                                 \NPVRate
PII   2 FN     3               "NPV CashFlow"                             \NPVCashFlow
PII   2 PN     3               "NPV Start Column"                         \NPVStartColumn
PII   2 PN     3               "NPV End Column"                           \NPVEndColumn
PII   2 PN D   3-              "NPV DateTime"                             \NPVDateTime
PPP   2 PN   1 3               "NPV Result"                               \NPVResult

.Formulas NoTrend
NPVRate					= 0.01
NPVStartColumn			= 1
NPVEndColumn			= 12
NPVDateTime				= 38353
NPVResult				= NPV(NPVRate, NPVCashFlow, NPVStartColumn, NPVEndColumn, NPVDateTime)

;-------------------------------------------------------------------------------------------------------------------------------------- A
; NPV2
.Variables
PPP   0 1N     2               "NPV2"                                      \NPV2Var
PII   2 BN %   3               "NPV2 Rate"                                 \NPV2Rate
PII   2 FN     3               "NPV2 CashFlow"                             \NPV2CashFlow
PII   2 PN     3               "NPV2 Start Column"                         \NPV2StartColumn
PII   2 PN     3               "NPV2 End Column"                           \NPV2EndColumn
PII   2 PN D   3-              "NPV2 DateTime"                             \NPV2DateTime
PPP   2 PN   1 3               "NPV2 Result"                               \NPV2Result

.Formulas NoTrend,Trend
NPV2Rate				= 0.0
NPV2StartColumn			= 1
NPV2EndColumn			= 12
NPV2DateTime			= 38353
NPV2Result				= NPV2(NPV2Rate, NPV2CashFlow, NPV2StartColumn, NPV2EndColumn, NPV2DateTime)
