PPP   0 1N     3               "(NA+NA)=NA"                              \LogicaTest020
PPP   0 1N     3               "(PM+NA)=PM"                              \LogicaTest021
PPP   0 1N     3               "(PM+PM)=PM"                              \LogicaTest022
;PPI   0 1N     3               "ER = ER"                                 \LogicaTest100
PPP   0=1N    -3               "PM/NA/Zero/ER"                           \=Logica000

.Formulas NoTrend
LogicaTest001    = Abs((0>PM)-1)
LogicaTest002    = Abs((0>NA)-1)
;LogicaTest003    = Abs((0>ER)-1)   Deze testen werken niet. 0>ER of 0<ER levert beide 0 op.
;LogicaTest004    = Abs((PM>NA)-1)  Deze testen werken niet.
;LogicaTest005    = Abs((PM>ER)-1)  Deze testen werken niet.
;LogicaTest006    = Abs((ER>NA)-1)  Deze testen werken niet.
LogicaTest007    = Abs((0<>PM)-1)
LogicaTest008    = Abs((0<>NA)-1)
LogicaTest009    = Abs((0<>ER)-1)
LogicaTest010    = Abs((PM<>NA)-1)
LogicaTest011    = Abs((PM<>ER)-1)
LogicaTest012    = Abs((ER<>NA)-1)
LogicaTest013    = Abs(((1/0)=ER)-1)
LogicaTest014    = Abs(((0+NA)=0)-1)
LogicaTest015    = Abs(((0+ER)=ER)-1)
LogicaTest016    = Abs(((0+PM)=0)-1)
LogicaTest017    = Abs(((ER+ER)=ER)-1)
LogicaTest018    = Abs(((ER+NA)=ER)-1)
LogicaTest019    = Abs(((ER+PM)=ER)-1)
LogicaTest020    = Abs(((NA+NA)=NA)-1)
LogicaTest021    = Abs(((PM+NA)=PM)-1)
LogicaTest022    = Abs(((PM+PM)=PM)-1)

;LogicaTest100    = Abs((ER=ER)-1)

.Variables
PPP   0 1N     2               "TRUE/FALSE/AND/OR/On/OFF"                \Logica100
PPP   0 1N     3               "TRUE=1"                                  \LogicaTest101
PPP   0 1N     3               "FALSE=0"                                 \LogicaTest102
PPP   0 1N     3               "FALSE<>TRUE"                             \LogicaTest103
