PPP   0 1N     3               "(0 AND 0) = 0"                           \LogicaTest106
PPP   0 1N     3               "(1 OR 0) = 1"                            \LogicaTest107
PPP   0 1N     3               "(1 OR 1) = 1"                            \LogicaTest108
PPP   0 1N     3               "(0 OR 0) = 0"                            \LogicaTest109
PPP   0 1N     3               "OFF = 0"                                 \LogicaTest110
PPP   0 1N     3               "ON = 1"                                  \LogicaTest111
PPP   0 1N     3               "NO = 0"                                  \LogicaTest112
PPP   0 1N     3               "YES = 1"                                 \LogicaTest113
PPP   0=1N    -3               "TRUE/FALSE/AND/OR"                       \=Logica100

.Formulas NoTrend
LogicaTest101    = Abs((TRUE=1)-1)
LogicaTest102    = Abs((FALSE=0)-1)
LogicaTest103    = Abs((FALSE<>TRUE)-1)
LogicaTest104    = Abs(((1 AND 1) = 1)-1)
LogicaTest105    = Abs(((1 AND 0) = 0)-1)
