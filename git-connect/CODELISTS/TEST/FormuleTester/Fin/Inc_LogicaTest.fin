LogicaTest107    = Abs(((1 OR 0) = 1)-1)
LogicaTest108    = Abs(((1 OR 1) = 1)-1)
LogicaTest109    = Abs(((0 OR 0) = 0)-1)
LogicaTest110    = Abs((Off = 0)-1)
LogicaTest111    = Abs((On = 1)-1)
LogicaTest112    = Abs((No = 0)-1)
LogicaTest113    = Abs((Yes = 1)-1)

;-------------------------------------------------------------------------------------------------------------------------------------- Z

.Variables
PPP   0=1N    -2               "Total logica tested failed"              \LogicaTestTotaal




