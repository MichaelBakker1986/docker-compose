.Hints
BNG_WSW_DSCRWegingsfactor                   = "$$CalcYearAs=MeanCalc"

.Choices
BNG_WSW_DSCRNoemerTest                      = "1:$>Noemer <= 0<$|0:$>Noemer <> 0<$"
BNG_WSW_DSCRtbvRisicoScoreNormGehaald       = "1:$>Voldoet aan WSW norm<$|0:$>Voldoet niet aan WSW norm<$"

.Formulas Visible
BNG_WSW_DSCRNoemerTest                      = (Self=1)
BNG_WSW_RenteuitgavenDSCRGewogenCumulatief  = 0
BNG_WSW_KasstroomVoorDSCRCumulatief         = 0
BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief = 0

.Formulas NoTrend, Trend
BNG_WSW_OnderhoudsuitgavenNormatief         = BNG_WSW_AantalVHEBeginPrognoseperiode * 700
BNG_WSW_KasstroomVoorDSCRHistorieGewogen    = OnZero(BNG_WSW_KasstroomVoorDSCRCumulatief[LastTinFormulaSet(NoTrend),1]/3,NA)
BNG_WSW_KasstroomVoorDSCRPrognoseGewogen    = OnZero(BNG_WSW_KasstroomVoorDSCRCumulatief[LastTinFormulaSet(Trend),1]/15,NA)
BNG_WSW_RenteuitgavenDSCRHistorieGewogen    = OnZero(BNG_WSW_RenteuitgavenDSCRGewogenCumulatief[LastTinFormulaSet(NoTrend),1]/3,NA)
BNG_WSW_RenteuitgavenDSCRPrognoseGewogen    = OnZero(BNG_WSW_RenteuitgavenDSCRGewogenCumulatief[LastTinFormulaSet(Trend),1]/15,NA)
BNG_WSW_VerkoopopbrDSCR5jrsCumulatiefHistorie = OnZero((BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief[LastTinFormulaSet(NoTrend)]/HSum(BNG_WSW_DSCRWegingsfactor,1,LastTinFormulaSet(NoTrend)))*5,NA)
BNG_WSW_VerkoopopbrDSCR5jrsCumulatiefPrognose = Min(OnZero(BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief[LastTinFormulaSet(Trend)]/HSum(BNG_WSW_DSCRWegingsfactor,FirstTinFormulaSet(Trend),LastTinFormulaSet(Trend))*5,NA),HSum(BNG_WSW_TotaalVerkoopontvangsten,FirstTinFormulaSet(Trend),LastTinFormulaSet(Trend)))

BNG_WSW_RestwaardePortefeuilleHistorie      = BNG_WSW_AantalVHEBeginPrognoseperiodeT*5000
BNG_WSW_RestwaardePortefeuillePrognose      = BNG_WSW_AantalVHEBeginPrognoseperiodeVT*5000
BNG_WSW_TheoretischAflossingsniveauHistorie = OnError(Max(0,(BNG_WSW_VreemdVermogenVastgoedInExploitatieT -BNG_WSW_VerkoopopbrDSCR5jrsCumulatiefHistorie-BNG_WSW_RestwaardePortefeuilleHistorie)/BNG_WSW_GemLevensduurBeginPrognoseperiodeT),0)
BNG_WSW_TheoretischAflossingsniveauPrognose = OnError(Max(0,(BNG_WSW_VreemdVermogenVastgoedInExploitatieVT-BNG_WSW_VerkoopopbrDSCR5jrsCumulatiefPrognose-BNG_WSW_RestwaardePortefeuillePrognose)/BNG_WSW_GemLevensduurBeginPrognoseperiodeVT),0)
BNG_WSW_GemiddeldGerealiseerdeDSCR          = OnError(BNG_WSW_KasstroomVoorDSCRHistorieGewogen/(BNG_WSW_RenteuitgavenDSCRHistorieGewogen+BNG_WSW_TheoretischAflossingsniveauHistorie),0)
BNG_WSW_GemiddeldGeprognosticeerdeDSCR      = OnError(BNG_WSW_KasstroomVoorDSCRPrognoseGewogen/(BNG_WSW_RenteuitgavenDSCRPrognoseGewogen+BNG_WSW_TheoretischAflossingsniveauPrognose),0)
BNG_WSW_DSCRNoemerTest                      = If(((BNG_WSW_RenteuitgavenDSCRHistorieGewogen+BNG_WSW_TheoretischAflossingsniveauHistorie)<=0) Or ((BNG_WSW_RenteuitgavenDSCRPrognoseGewogen+BNG_WSW_TheoretischAflossingsniveauPrognose)<=0),1,0)
BNG_WSW_DSCRtbvRisicoScore                  = Min(BNG_WSW_GemiddeldGerealiseerdeDSCR,BNG_WSW_GemiddeldGeprognosticeerdeDSCR)
BNG_WSW_DSCRtbvRisicoScoreNorm              = 1
BNG_WSW_DSCRtbvRisicoScoreNormGehaald       = (BNG_WSW_DSCRtbvRisicoScore>=BNG_WSW_DSCRtbvRisicoScoreNorm)

.Formulas NoTrend
BNG_WSW_DSCRWegingsfactor                   = If((LastTinFormulaSet(NoTrend)-T)<=(TsY(T)*3),1,NA)
BNG_WSW_KasstroomVoorDSCRCumulatief         = OnZero(BNG_WSW_KasstroomVoorDSCRCumulatief[T-1]+BNG_WSW_KasstroomVoorDSCR*BNG_WSW_DSCRWegingsfactor,NA)
BNG_WSW_RenteuitgavenDSCRGewogenCumulatief  = OnZero(BNG_WSW_RenteuitgavenDSCRGewogenCumulatief[T-1]+BNG_WSW_RenteuitgavenDSCR*BNG_WSW_DSCRWegingsfactor,NA)
BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief = OnZero(BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief[T-1]+BNG_WSW_TotaalVerkoopontvangsten*BNG_WSW_DSCRWegingsfactor,NA)

.Formulas Trend
BNG_WSW_DSCRWegingsfactor			              = Case(T-LastTinFormulaSet(NoTrend),[<=TsY(T):5|<=(TsY(T)*2):4|<=(TsY(T)*3):3|<=(TsY(T)*4):2|<=(TsY(T)*5):1|>(TsY(T)*5):NA])
BNG_WSW_KasstroomVoorDSCRCumulatief         = OnZero(BNG_WSW_KasstroomVoorDSCRCumulatief[T-1]*(T>FirstTinFormulaSet(Trend))+BNG_WSW_KasstroomVoorDSCR*BNG_WSW_DSCRWegingsfactor,NA)
BNG_WSW_RenteuitgavenDSCRGewogenCumulatief  = OnZero(BNG_WSW_RenteuitgavenDSCRGewogenCumulatief[T-1]*(T>FirstTinFormulaSet(Trend))+BNG_WSW_RenteuitgavenDSCR*BNG_WSW_DSCRWegingsfactor,NA)
BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief = OnZero(BNG_WSW_VerkoopopbrengstenDSCRGewogenCumulatief[T-1]*(T>FirstTinFormulaSet(Trend))+BNG_WSW_TotaalVerkoopontvangsten*BNG_WSW_DSCRWegingsfactor,NA)

