BNG_WSW_RenteuitgavenHistorieGewogen        = OnZero(BNG_WSW_RenteuitgavenGewogenCumulatief[LastTinFormulaSet(NoTrend),1]/3,NA)
BNG_WSW_RenteuitgavenPrognoseGewogen        = OnZero(BNG_WSW_RenteuitgavenGewogenCumulatief[LastTinFormulaSet(Trend),1]/15,NA)
BNG_WSW_GemiddeldeICRRealisatie             = OnER(If(BNG_WSW_RenteuitgavenHistorieGewogen=0,99999,BNG_WSW_KasstroomVoorRenteHistorieGewogen/BNG_WSW_RenteuitgavenHistorieGewogen),NA)
BNG_WSW_GemiddeldeICRPrognose               = OnER(If(BNG_WSW_RenteuitgavenPrognoseGewogen=0,99999,BNG_WSW_KasstroomVoorRentePrognoseGewogen/BNG_WSW_RenteuitgavenPrognoseGewogen),NA)
