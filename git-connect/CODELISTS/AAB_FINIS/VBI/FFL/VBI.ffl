				variable TaxCalculationBoxIII
				{
					datatype : number;
					frequency : document;
					title : "Tax calculation Box III";
					displaytype : currency;
					locked : On;
					visible : 1.0;
					options_title : locked;
					options_notrend : locked;
					options_trend : locked;
					variable TotalAssetsInclExemptions
					{
						datatype : number;
						frequency : document;
						formula_notrend : PT_Client_Box3BankAndSavings [ "Total" ] + PT_Client_Box3Securities [ "Total" ] + PT_Client_Box3OtherProperty [ "Total" ] +
						PT_Client_Box3CapitalInsurance [ "Total" ];
						title : "Total assets (incl. Exemptions)";
						displaytype : currency;
						top_blanklines : 1.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable TotalLiabilitiesTaxCalculationBoxIII
					{
						datatype : number;
						frequency : document;
						formula : PT_Client_Box3DebtToLtd [ "Total" ] + PT_Client_Box3CommercialRealEstateFinancing [ "Total" ] + PT_Client_Box3SecuritiesCredit [ "Total" ] +
						PT_Client_Box3OtherLiabilities [ "Total" ];
						title : "total liabilities";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable LessEfficiencyThreshold
					{
						datatype : number;
						frequency : document;
						formula_notrend : - 5800.0;
						title : "Less: efficiency threshold";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable YieldBasis
					{
						datatype : number;
						frequency : document;
						formula_notrend : TotalAssetsInclExemptions - TotalLiabilitiesTaxCalculationBoxIII + LessEfficiencyThreshold;
						title : "yield basis";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable LessTaxFreeAllowance
					{
						datatype : number;
						frequency : document;
						data_options : unscalable;
						title : "Less: tax-free allowance";
						displaytype : select;
						choices : "21139:21.139|42278:42.278";
						visible : 1.0;
						options_title : locked;
					}
					variable BenefitFromSavingsAndInvestments
					{
						datatype : number;
						frequency : document;
						formula_notrend : YieldBasis - LessTaxFreeAllowance;
						title : "Benefit from savings and investments";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable FixedReturn
					{
						datatype : number;
						frequency : document;
						formula_notrend : 0.04 * BenefitFromSavingsAndInvestments;
						title : "fixed return (4%)";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable TotalIncomeFromSavingsAndInvestmentsBoxIII
					{
						datatype : number;
						frequency : document;
						formula_notrend : OnNeg ((FixedReturn), (2.0 * FixedReturn));
						title : "Total income from savings and investments Box III";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable TaxBoxIIITaxCalculationBoxIII
					{
						datatype : number;
						frequency : document;
						formula_notrend : OnNeg ((0.3 * TotalIncomeFromSavingsAndInvestmentsBoxIII), NA);
						title : "Tax Box III";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
				}
				variable AdditionalTool442
				{
					datatype : none;
					frequency : document;
					data_options : unscalable;
					title : "Additional Tool 4.4.2";
					locked : On;
					visible : 1.0;
					options_title : locked;
					options_notrend : locked;
					options_trend : locked;
					variable InvestableAssets
					{
						datatype : number;
						frequency : document;
						formula_notrend : PT_Client_Box3BankAndSavings [ "Total" ] + PT_Client_Box3Securities [ "Total" ];
						title : "Investable assets";
						displaytype : currency;
						top_blanklines : 1.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable Correction
					{
						datatype : number;
						frequency : document;
						title : "Correction (-)";
						displaytype : currency;
						visible : 1.0;
						options_title : locked;
					}
					variable AdditionalToolTotal
					{
						datatype : number;
						frequency : document;
						formula_notrend : InvestableAssets - Correction;
						title : "TotalAT442";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable ActualReturnMax
					{
						datatype : number;
						frequency : document;
						formula_notrend : 0.03;
						data_options : unscalable;
						title : "Actual return (max. 3%)";
						displaytype : percentage;
						fixed_decimals : 1.0;
						visible : 1.0;
						options_title : locked;
					}
					variable AdditionalToolAmount
					{
						datatype : number;
						frequency : document;
						formula_notrend : AdditionalToolTotal * ActualReturnMax;
						title : "Amount";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable SecuritiesLendingAverageOverdraft
					{
						datatype : number;
						frequency : document;
						formula_notrend : (AmountSecuritiesLending * InterestSecuritiesLending) + RedemptionPerYearSecuritiesLending;
						title : "Securities lending (average overdraft)";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
						variable AmountSecuritiesLending
						{
							datatype : number;
							frequency : document;
							title : "Amount (Securities lending)";
							displaytype : currency;
							top_blanklines : 1.0;
							visible : 1.0;
							options_title : locked;
						}
						variable RedemptionPerYearSecuritiesLending
						{
							datatype : number;
							frequency : document;
							title : "Redemption per year (Securities lending)";
							displaytype : currency;
							visible : 1.0;
							options_title : locked;
						}
						variable InterestSecuritiesLending
						{
							datatype : number;
							frequency : document;
							formula_notrend : 0.04;
							data_options : unscalable;
							title : "Interest (Securities lending)";
							displaytype : percentage;
							fixed_decimals : 1.0;
							visible : 1.0;
							options_title : locked;
						}
					}
					variable ImportationVBIToolIncomeSourcesEquity
					{
						datatype : number;
						frequency : document;
						formula_notrend : AdditionalToolAmount - SecuritiesLendingAverageOverdraft;
						title : "Importation VBI tool Income Sources Equity";
						displaytype : currency;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
				}
			}
		}
