				variable ClientHidden
				{
					datatype : number;
					frequency : document;
					title : "Client Hidden";
					displaytype : currency;
					locked : On;
					visible : 0.0;
					options_title : locked, invisible;
					options_notrend : locked, invisible;
					options_trend : locked, invisible;
					variable Rol
					{
						datatype : number;
						frequency : document;
						formula_notrend : 0.0;
						data_options : unscalable;
						title : "Rol";
						displaytype : select;
						choices : "0:Geen IB ondernemer|1:IB ondernemer";
						top_blanklines : 1.0;
						visible : 1.0;
						options_title : locked;
					}
					variable SharedVal_WOZValueCopyForHidden
					{
						datatype : number;
						frequency : document;
						data_options : unscalable;
						title : "WOZ value";
						fixed_decimals : 0.0;
						formula : SharedVal_WOZValue [ "Shared" ];
						visible : 1.0;
						options_title : locked;
					}
					variable ExpatPrivileges
					{
						datatype : number;
						frequency : document;
						formula_notrend : 0.3;
						data_options : unscalable;
						title : "Expat privileges";
						displaytype : percentage;
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable FiscalSalaryfromSalariedLiq
					{
						datatype : number;
						frequency : document;
						formula : (VBICalc_Client_tpLaborAndBenefit [ "Client" ] + VBICalc_Client_tpCompanySalary [ "Client" ]);
						data_options : unscalable;
						title : "Fiscal Salary from salaried Liq";
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable FiscalSalaryfromSalariedFiscal
					{
						datatype : number;
						frequency : document;
						formula : If (((VBICalc_Client_tpPrivileges [ "Client" ]) = 1.0), ((FiscalSalaryfromSalariedLiq) * (1.0 - ExpatPrivileges)),
						(FiscalSalaryfromSalariedLiq));
						data_options : unscalable;
						title : "Fiscal Salary from salaried Fiscal";
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable TaxableIncomefromBusinessLiq
					{
						datatype : number;
						frequency : document;
						formula : Case (Rol, [
							1.0 : VBICalc_Client_tpCompanyProfitIB [ "Client" ] | 0.0 : VBICalc_Client_tpCompanyProfitIB [ "Client" ]
						]);
						data_options : unscalable;
						title : "Taxable income from business Liq";
						fixed_decimals : 0.0;
						top_blanklines : 1.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable TaxableIncomefromBusinessFiscal
					{
						datatype : number;
						frequency : document;
						formula_notrend : OnNeg ((TaxableIncomefromBusinessLiq), 0.0);
						data_options : unscalable;
						title : "Taxable income from business Fiscal";
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable ZelfstandigenAftrek
					{
						datatype : number;
						frequency : document;
						formula : - (If (((VBICalc_Client_tpCompanyProfitIB [ "Client" ]) < (MatrixLookUp ("VBI.xls", "Aftrek", 1.0, 2.0))), 0.0, (MatrixLookUp ("VBI.xls",
						"Aftrek", 1.0, 2.0))));
						data_options : unscalable;
						title : "Zelfstandigenaftrek";
						fixed_decimals : 0.0;
						top_blanklines : 1.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable StartersAftrek
					{
						datatype : number;
						frequency : document;
						data_options : unscalable;
						title : "Startersaftrek";
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable Ondernemersaftrek
					{
						datatype : number;
						frequency : document;
						formula : (FiscalSalaryfromSalariedFiscal + TaxableIncomefromBusinessLiq + ZelfstandigenAftrek);
						data_options : unscalable;
						title : "Ondernemersaftrek";
						fixed_decimals : 0.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable MKBPercentage
					{
						datatype : number;
						frequency : document;
						formula_notrend : MatrixLookUp ("VBI.xls", "Aftrek", 5.0, 2.0);
						data_options : unscalable;
						title : "MKB Percentage";
						displaytype : percentage;
						fixed_decimals : 0.0;
						top_blanklines : 1.0;
						locked : On;
						visible : 1.0;
						options_title : locked;
						options_notrend : locked;
						options_trend : locked;
					}
					variable MKBExemption
