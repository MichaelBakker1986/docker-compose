RCBlokkering1           = RCBlokkering1[T-1]
RCBlokkering2           = RCBlokkering2[T-1]
RekCour                 = RekCour[T-1]+MutRekCour
RCBank                  = Min(Max(-(RekCour-OvLiqMid),0),NettoLimietRC)
VermBehoefte            = Max(OnZero(-(RekCour-OvLiqMid)-NettoLimietRC,NA),NA)
VermOverschot           = Max(RekCour-OvLiqMid,NA)

