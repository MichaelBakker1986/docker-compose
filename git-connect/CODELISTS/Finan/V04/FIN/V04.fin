KostenOmzetCheck        = If( HYearOvr(BedrK,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))>0,0,1)
BalResRekCheck1         = OnER( GroeiNettoOmzet / ((BalAkt[T]/BalAkt[T-TsY])-1),0)
BalResRekCheck2         = If( (Abs(HAVG(GroeiNettoOmzet,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT)))>0.025) AND (BalResRekCheck1[MinValueT(BalResRekCheck1,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))])<0,1,0)
PrognInvCheck           = If( HYearOvr(ImmatAktInv,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))+HYearOvr(OnrGoedInv,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))+HYearOvr(BedrUitrInv,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))>0,0,1)
LiquidityCheckA         = If((LiquidityCheckA1[MaxValueT(LiquidityCheckA1,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))] < 1) and (LiquidityCheckA2[MaxValueT(LiquidityCheckA2,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))] < 1),0,1)
LiquidityCheckA1        = If( (CurrentRatio < 1.75) AND (CurrentRatio <> NA),1,0)
LiquidityCheckA2        = If( (QuickRatio < 0.9) AND (QuickRatio <> NA),1,0)
LiquidityCheckB         = If( HSum(VermBehoefte,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))>0,1,0)
SolvabilityCheck        = If( (SolvBal[MinValueT(SolvBal,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))]<Max(SolvBal[MinValueT(SolvBal,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(NoTrend,MainPeriod))],0.05)) AND (IntDekking[MinValueT(IntDekking,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT))]<2),1,0); controleren T enperiodes
;Als (solvabiliteit in bepaald jaar) < (Minimumsolvabiliteit historie of 5%) en (minimum-ICratio < 2) dan melding
ControlePrognose        = If(VSum(NettoOmzetCheck1,SolvabilityCheck)>0,1,0)

