{&&language=English}
; V04-RunMatrix
;

{&&if FEATURE="M"}

.SetButton Nr=1,Caption="M",Hint="FINAN Matrix"

.Script Name=EJSRunMatrix,Title="Quick start FINAN Matrix"
  .Event OnButtonClick 1
  .Application BeginSilent
  .Let FinanMatrixDone=0
  .Report Name=FinMatrix,Destination=Preview
  .Application EndSilent
  .If not SysVar("FinanMatrixDone")
    .Message "$>Unable to show FINAN Matrix!<$|$>Column(s) not valid?<$"
  .EndIf
.EndScript

{&&endif}
