<?xml version="1.0" encoding="UTF-8"?><gadget_screen_configuration xmlns="http://www.finan.nl/gadget_screen_configuration-1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.finan.nl/gadget_screen_configuration-1N:\Modelbouw\Gadget~1\XSD\GadgetScreenConfig.xsd">	<form>		<title>$&gt;Profitanalysis cockpit&lt;$</title>		<enableYearSelection>true</enableYearSelection>		<firstTFormula>FirstTinFormulaSet(Trend,PeriodInSheet,On)</firstTFormula>		<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>		<TsY>1</TsY>	</form>	<sliders>		<slider>			<title>$&gt;Growth in sales&lt;$</title>			<formatString>%s %%</formatString>			<min>-10%</min>			<max>20%</max>			<variableName>NettoOmzet</variableName>			<growthMode>true</growthMode>			<isPercentage>false</isPercentage>		</slider>		<slider>			<title></title>			<formatString>%s %%</formatString>			<min>0%</min>			<max>100%</max>			<variableName>BrutoMarge</variableName>			<growthMode>false</growthMode>			<isPercentage>true</isPercentage>		</slider>		<slider>			<title></title>			<formatString>%s personen</formatString>			<min>0</min>			<max>Max(TotKracht*2,(NettoOmzet/(100*ScaleFactor))*2)</max>			<variableName>TotKracht</variableName>			<growthMode>false</growthMode>			<isPercentage>false</isPercentage>		</slider>	</sliders>	<radiobuttons>		<radiobutton showValues="false" show3D="true">			<title>$&gt;Profitability&lt;$</title>			<axisTitle>$&gt;Amount&lt;$</axisTitle>			<chartType>area</chartType>			<lowLimit show="false" color="FF0000">0.9</lowLimit>			<highLimit show="false" color="FF0000">1.1</highLimit>			<variables>				<variable>					<transparency>50</transparency>					<color>0090FF</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>NettoOmzet</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>				<variable>					<transparency>70</transparency>					<color>003EBA</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>BrutoWinst</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>				<variable>					<transparency>50</transparency>					<color>00AF63</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>BedrRes</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>				<variable>					<transparency>50</transparency>					<color>890099</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>ResNaBel</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>			</variables>			<minY></minY>			<maxY></maxY>		</radiobutton>	</radiobuttons>	<help>		<title>$&gt;Profitanalysis cockpit&lt;$</title>		<helpText>$&gt;By moving the sliders, you can see the effects on the financial ratios.&lt;$|$&gt;At the right hand side you can select the ratio, which will be shown in the graph.&lt;$|$&gt;The button 'save and close' will save your changes.&lt;$||$&gt;Attention: the changes will be save in the current FINAN document.&lt;$|$&gt;If you don't want to save the changes, use the button 'cancel'.&lt;$</helpText>	</help></gadget_screen_configuration>