; ROIC tree
;
.Formulas NoTrend,Trend,User
NettoOmzetPNO           = 1
ProdVrdMutPNO           = OnER(ProdVrdMut/NettoOmzet,NA)
InkWPNO                 = OnER(InkW/NettoOmzet,NA)
BrutoWinstPNO           = OnER(BRUTOWINST/NettoOmzet,NA)
OvBedrOpbrPNO           = OnER(OvBedrOpbr/NettoOmzet,NA)
BedrKPNO                = OnER(BedrK/NettoOmzet,NA)
AktAfsPNO               = OnER(AktAfs/NettoOmzet,NA)
BedrResPNO              = OnER(BedrRes/NettoOmzet,NA)
CorrBedrResPNO          = OnER(CorrBedrRes/NettoOmzet,NA)
OpMargin                = OnER(EBIT/NettoOmzet,NA)
OpVastAktPNO            = MutCalc*OnER(OpVastAkt[T-1]/(TsY*NettoOmzet),NA)
OpWorkCapPNO            = MutCalc*OnER(OpWorkCap[T-1]/(TsY*NettoOmzet),NA)
CorrOpInvCapPNO         = MutCalc*OnER(CorrOpInvCap[T-1]/(TsY*NettoOmzet),NA)
OpInvCapPNO             = MutCalc*OnER(OpInvCap[T-1]/(TsY*NettoOmzet),NA)
CapTurnover             = MutCalc*OnER(TsY*NettoOmzet/OpInvCap[T-1],NA)
PretaxROIC              = OpMargin*CapTurnover
TaxRateOnEBIT           = OnER(TaxesOnEBIT/EBIT,NA)
ROIC                    = MutCalc*OnER(TsY*NOPLAT/OpInvCap[T-1],NA)
;ROIC                    = MutCalc*((1+OnER(NOPLAT/OpInvCap[T-1],NA))^TsY-1)
AvCapROIC               = MutCalc*OnER(TsY*NOPLAT/((OpInvCap[T-1]+OpInvCap)/2),NA)

;;@@
; WACC:
;
.Formulas NoTrend,Trend,User
RiskFreeRate            = OnNA(RiskFreeRate[T-1],6.5%)        ; invoer of 6,5%
RiskPremium             = OnNA(RiskPremium[T-1],5.5%)         ; invoer of 5,5%
; EV
Beta                    = If(DataEntered(PremiumEquity) or DataEntered(CostOfEq),PremiumEquity/RiskPremium,OnNA(Beta[T-1],1))
PremiumEquity           = If(DataEntered(CostOfEq),CostOfEq-RiskFreeRate,OnNA(PremiumEquity[T-1],RiskPremium*Beta)) <-> CostOfEq
CostOfEq                = RiskFreeRate+PremiumEquity
; VV
BetaDebt                = If(DataEntered(PremiumDebt) or DataEntered(CostOfDebt),PremiumDebt/RiskPremium,OnNA(BetaDebt[T-1],1.5/5.5))
PremiumDebt             = If(DataEntered(CostOfDebt)or(TakeInterestRatecenario),CostOfDebt-RiskFreeRate,OnNA(Premiumdebt[T-1],RiskPremium*BetaDebt)) <-> CostOfDebt
CostOfdebt              = If(TakeInterestRatecenario,APVCostOfDebt,RiskFreeRate+PremiumDebt)
;
NetCostOfDebt           = OnNA((1-MargWinstBelPerc)*CostOfDebt,CostOfDebt)

EquityFracChoice        = 0   ;EJS: v41  nooit automatisch i.v.m. rekeniteraties
TakeInterestRatecenario = 0
CalculatedWACC          = OnNA(EquityFrac*CostOfEq+(1-EquityFrac)*NetCostOfDebt,0.1)  ;deze uitbreiden met unlevered formule?
WACCcorrection          = If(DataEntered(WACC),WACC-CalculatedWACC,WACCcorrection[T-1])
WACC                    = CalculatedWACC+WACCcorrection <-> WACCcorrection
LastWACC                = WACC[LastTinPeriod(PeriodInT),1]
;RealEquityFrac          = OnER(DCFValue/(DCFPVTOT+NOTOpInvCap+ValueNoOpCashFlow),NA)
RealEquityFrac          = OnER((DCFPVTOT-DCFDebt)/DCFPVTOT,NA)

.Formulas Notrend,User,Trend
;EquityFrac        = Max(0,If(EquityFracchoice,OnNA(REalEquityFrac,OnNA(TargetEquityFrac[T-1],OnER(EigenVerm/BalPas,NA))) ,TargetEquityFrac))
EquityFrac        = If(EquityFracchoice,OnNA(REalEquityFrac,OnNA(TargetEquityFrac[T-1],OnER(EigenVerm/BalPas,NA))) ,TargetEquityFrac)
TargetEquityFrac  = If( HOvr(TargetEquityFrac,1,T),TargetEquityFrac[T-1],OnER(EigenVerm/BalPas,NA))

;;@@
; Economische winst
;
.Formulas NoTrend,Trend,User
Spread                  = ROIC-WACC
VorigOpInvCap           = OpInvCap[T-1]
EcProfit                = OnEr((ROIC-WACC)/TsY*VorigOpInvCap,NA)
;EcProfit                = OnEr((((1+ROIC)^(1/TsY)-1)-((1+WACC)^(1/TsY)-1))*VorigOpInvCap,NA)
ROICn                   = WACC[LastTinPeriod(PeriodInT),1]       ;dezelfde formule als LastWACC
ROICo                   = ROIC[LastTinPeriod(PeriodInT),1]
G                       = If(GrNOPLAT[LastTinPeriod(PeriodInT),1]>=LastWACC,NA,GrNOPLAT[LastTinPeriod(PeriodInT),1])  ; CONTROLE G NOT >= LastWACC
LeaseResBel             = MargWinstBelPerc*(-RKredL)

;;@@
; Overige kengetallen waardering
;
GrNOPLAT                = OnER(NOPLAT[T]/NOPLAT[T-1]-1,NA)
GrOpInvCap              = OnER(OpInvCap[T]/OpInvCap[T-1]-1,NA)
GrNOPLATperGrOIC        = OnER(GrNOPLAT[T]/GrOpInvCap[T-1],NA)
;NettoInvVoet            = OnER((MatAktInv+AktAfs)/NOPLAT,NA)
;BrutoInvvoet            = OnER(MatAktInv/GrossCashFlow,NA)
NettoInvVoet            = OnER((GrossInvestment-AktAfs)/NOPLAT,NA)
BrutoInvvoet            = OnER(GrossInvestment/GrossCashFlow,NA)
EVopGV                  = OnER(EigenVerm/PasInvCap,NA)
AinEVopGV               = OnER((AdjEquity-EigenVerm)/PasInvCap,NA)
AEVopGV                 = OnER(AdjEquity/PasInvCap,NA)
LVVopGV                 = OnER(LangVV/PasInvCap,NA)
