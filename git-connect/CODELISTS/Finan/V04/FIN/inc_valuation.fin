; Discounted cashflow en FCF
;
.Formulas Trend
DCFPV                   = OnEr( If(T=LastTinPeriod(PeriodInT),0,OnEr(NPV2(WACC,FreeCashFlow,T+1,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor),NA)) ,NA)
;DCFPV                   = OnEr( If(T=LastTinPeriod(PeriodInT),0,OnEr(NPV2(WACC,AdjustedFreeCashFlow,T+1,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor),NA)) ,NA)
DCFPVRest               = OnEr( NPV2(WACC,FCFinCVPeriod,T,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor) ,NA)
DCFPVRest1              = OnEr( NPV2(WACC,FCFinRestwaarde,T,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor) ,NA)
ValueNoOpCashFlow       = OnEr( If(T=LastTinPeriod(PeriodInT),0,-1*NPV2(WACC,NoOpCashFlow,T+1,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor)),NA)
.Formulas NoTrend,Trend
;DCFPVnu                 = OnEr( If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(WACC,AdjustedFreeCashFlow,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)) ,NA)
DCFPVnu                 = OnEr( If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(WACC,FreeCashFlow,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)) ,NA)
FCFinCVperiod           = If(T=LastTinPeriod(PeriodInT),FCFRestwaarde1/(LastWACC-G),NA)            ;JB feitelijk overbodig geworden
FCFinRestWaarde         = If(T=LastTinPeriod(PeriodInT), OnEr( (if(CalcRestwaarde<=0,FCFRestwaarde0,FCFRestwaarde1))/(LastWACC-G) ,NA) ,NA)
DCFPVRestNu             = OnEr( NPV2(WACC,FCFinRestWaarde,Max(FirstTinFormulaSet(Trend,PeriodInT),Min(ValDateColumn+1,LastTinFormulaSet(Trend,PeriodInT))),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor) ,NA)
DCFDebt                 = If(T<LastTinFormulaset(NoTrend),NA,DCFDebt1)
DCFNotOpInvCap          = If(T<LastTinFormulaset(NoTrend),NA,DCFNotOpInvCap1)
DCFEquityClaims         = If(T<LastTinFormulaset(NoTrend),NA,DCFEquityClaims1)
VerschilBeursEnDCFNu    = If(BeurswaardeNU=NA,NA,DCFValueNU-BeurswaardeNU)
DCFvaluePerShare        = OnER(DCFValue*ScaleFactor/(NumberOfShares*1000),NA)
DCFvaluePerShareNU      = OnER(DCFValueNu*ScaleFactor/(NumberOfShares[ValDateColumn,TsY(ValDateColumn)]*1000),NA)
NotOpInvCapNu           = NotOpInvCap[ValDateColumn,TsY(ValDateColumn)]
ValueNoOpCashFlowNu     = -1*If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(WACC,NoOpCashFlow,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA))
AdjDebtNu               = AdjDebt[ValDateColumn,TsY(ValDateColumn)]
CorValueDebt            = If(DataEntered(CorValueDebtNu) and (T=GetT(ValDateColumn,0)),CorValueDebtNu,CorValueDebt[T-1])
CorValueDebtNu          = CorValueDebt[ValDateColumn,TsY(ValDateColumn)]
WaardeAandDerdNu        = WaardeAandDerd[ValDateColumn,TsY(ValDateColumn)]
dcfValuePrefsNu         = dcfValuePrefs[ValDateColumn,TsY(ValDateColumn)]
dcfValueWarrNu          = dcfValueWarr[ValDateColumn,TsY(ValDateColumn)]
dcfValueManOpNu         = dcfValueManOp[ValDateColumn,TsY(ValDateColumn)]
BeursWaardeNu           = Beurswaarde[ValDateColumn,TsY(ValDateColumn)]

.Choices
CalcRestwaarde          = "$>Based on operational profit<$|$>Based on free cash flow<$"

.Formulas Trend
