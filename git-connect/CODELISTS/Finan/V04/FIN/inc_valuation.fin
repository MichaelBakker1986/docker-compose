; Controle waardering
.Formulas NoTrend,Trend
WaccCheckA              = If (HYearOvr(RiskFreeRate,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(Trend,PeriodInT))>0,0,1)
WaccCheckC              = If (HYearOvr(PremiumEquity,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(Trend,PeriodInT))>0,0,1)
WaccCheckD              = If ( (HYearOvr(CostOfDebt,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(Trend,PeriodInT))>0) or (HYearOvr(PremiumDebt,FirstTinFormulaSet(NoTrend),LastTinFormulaSet(Trend,PeriodInT))>0) ,0,1)
WaccCheckF1             = OnNA(Abs(EquityFrac-RealEquityFrac),0)
WaccCheckF              = If (WaccCheckF1[MinValueT(WaccCheckF1,FirstTinFormulaSet(Trend,PeriodInT),LastTinFormulaSet(Trend,PeriodInT)-TsY)]>0.1,1,If(WaccCheckF1[LastTinFormulaSet(Trend,PeriodInT)]>0.1,2,0))
WaccCheckG              = If((Case(EquityFracChoice,"0:TargetEquityFrac[MinValueT(TargetEquityFrac,(ValDateColumn+1),LastTinPeriod)]|1:RealEquityFrac[MinValueT(RealEquityFrac,(ValDateColumn+1),LastTinPeriod)]")<0),1,0)
WaccCheckG2             = WaccCheckG
StStateBal1             = If (Abs(OnER(ImmatAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY]   /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(ImmatAkt[LastTinFormulaSet(Trend,PeriodInT)]   /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal2             = If (Abs(OnER(OnrGoed[LastTinFormulaSet(Trend,PeriodInT)-TsY]    /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(OnrGoed[LastTinFormulaSet(Trend,PeriodInT)]    /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal3             = If (Abs(OnER(BedrUitr[LastTinFormulaSet(Trend,PeriodInT)-TsY]   /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(BedrUitr[LastTinFormulaSet(Trend,PeriodInT)]   /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal4             = If (Abs(OnER(FinVastAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY] /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(FinVastAkt[LastTinFormulaSet(Trend,PeriodInT)] /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal5             = If (Abs(OnER(Vrd[LastTinFormulaSet(Trend,PeriodInT)-TsY]        /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(Vrd[LastTinFormulaSet(Trend,PeriodInT)]        /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal6             = If (Abs(OnER(Vord[LastTinFormulaSet(Trend,PeriodInT)-TsY]       /BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(Vord[LastTinFormulaSet(Trend,PeriodInT)]       /BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
StStateBal7             = If (Abs(OnER(SomOvLiqMid[LastTinFormulaSet(Trend,PeriodInT)-TsY]/BalAkt[LastTinFormulaSet(Trend,PeriodInT)-TsY],NA) - OnER(SomOvLiqMid[LastTinFormulaSet(Trend,PeriodInT)]/BalAkt[LastTinFormulaSet(Trend,PeriodInT)],NA)) < 0.02,0,1)  ;; jb
