.Formulas NoTrend,Trend
APVAllEqValCFNu           = OnEr( If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(CostOfAllEq,FreeCashFlow,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)),NA)
HelpVarAPVfcfCV           = If(T=LastTinPeriod(PeriodInT),FCFRestwaarde1/(LastCostOfEq-G),NA)
LastCostOfEq              = CostOfAllEq[LastTinPeriod(PeriodInT),1]
APVAllEqValCVNu           = OnEr( NPV2(CostOfAllEq,HelpVarAPVfcfCV,Max(FirstTinFormulaSet(Trend,PeriodInT),Min(ValDateColumn+1,LastTinFormulaSet(Trend,PeriodInT))),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)  ;;RvA;;
APVtaxRate                = MargWinstBelPerc
GDebt                     = G
TaxShieldForecValNu       = OnEr( If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(CostOfDebt,TaxShield,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)),NA)
LastAPVTaxRate            = APVTaxRate[LastTinPeriod(PeriodInT),1]
LastCostOfDebt            = CostOfDebt[LastTinPeriod(PeriodInT),1]
;LastCostOfDebtNw          = LastCostOfEq
LastAPVDebt               = OnZero(AdjDebt[LastTinPeriod(PeriodInT)]+CorValueDebt[LastTinPeriod(PeriodInT)],NA)
CostOfDebtSpread          = APVCostOfDebt-CostOfDebt
CostOfDebtSurplus         = CostOfDebtSpread*AdjDebtTmin1/TsY
LastAdjDebt               = AdjDebt[LastTinPeriod(PeriodInT),1]
LastAPVCostOfDebt         = CostOfDebt[LastTinPeriod(PeriodInT),1]
CorValueDebtRestwaarde    = OnEr((LastAPVCostOfDebt-LastCostOfDebt)*LastAdjDebt,NA)
HelpVarDebtCorrCV         = If(T=LastTinPeriod(PeriodInT),CorValueDebtRestwaarde/(CostOfDebt-GDebt),NA)
DebtCorrNu                = If(DataEntered(DebtCorrTotNu),NA,OnEr(If(ValDateColumn=LastTinPeriod(PeriodInT),0,OnEr(NPV2(CostOfDebt,CostOfDebtSurplus,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)),NA))
DebtCorrCVNu              = OnEr( If(DataEntered(DebtCorrTotNu),NA,NPV2(CostOfDebt,HelpVarDebtCorrCV,Max(FirstTinFormulaSet(Trend,PeriodInT),ValDateColumn+1),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor)),NA)

DebtCorrTotNu             = OnZero(DebtCorrNu+DebtCorrCVNu,NA)
CostOfAllEq               = If(DataEntered(BetaAllEq),RiskFreeRate+PremiumUnleveredEq,WACC/(1-MargWinstBelPerc*(1-EquityFrac))) <-> BetaAllEq
PremiumEquityLeverage     = CostOfEq-CostOfAllEq
PremiumUnleveredEq        = RiskPremium*BetaAllEq
;BetaAllEq                 = If(DataEntered(CostOfAllEq),OnEr((CostOfAllEq-RiskFreeRate)/RiskPremium,NA),OnNA(BetaAllEq[T-1],1.5/5.5))
BetaAllEq                 = OnEr((CostOfAllEq-RiskFreeRate)/RiskPremium,NA)
AdjDebtTmin1              = (AdjDebt+AdjDebt[T-1])/2
APVCostOfDebt             = (1+OnErorNA(APVInterest/AdjDebtTmin1,NA))^TsY-1

APVInterestRestwaarde     = LastCostOfDebt*LastAPVDebt
;HelpVarTaxShieldCV        = If(T=LastTinPeriod(PeriodInT),(APVInterestRestWaarde*(1-OnEr(GDebt/LastCostOfDebt,NA))*LastAPVTaxRate)/(LastCostOfDebtNw-GDebt),NA)
;HelpVarTaxShieldCV        = If(T=LastTinPeriod(PeriodInT),(APVInterestRestWaarde*(1-OnEr(GDebt/LastCostOfDebt,NA))*LastAPVTaxRate)/(LastCostOfDebt-GDebt),NA)
HelpVarTaxShieldCV        = If(T=LastTinPeriod(PeriodInT),(APVInterestRestWaarde*LastAPVTaxRate)/If(GDebt>0,(CostOfAllEq-GDebt[1]),LastCostOfDebt),NA)

TaxShieldCVValNu          = OnEr( NPV2(CostOfDebt,HelpVarTaxShieldCV,Max(FirstTinFormulaSet(Trend,PeriodInT),Min(ValDateColumn+1,LastTinFormulaSet(Trend,PeriodInT))),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor),NA)

;FCFRestwaarde1          = ROICo*LastOpInvCap*IF(CalcRestwaarde,(1-OnEr(G/if(G>0,ROICn,ROICo),NA)),1)
;FCFinRestWaarde         = If(T=LastTinPeriod(PeriodInT), OnEr( (if(CalcRestwaarde<=0,FCFRestwaarde0,FCFRestwaarde1))/(LastWACC-G) ,NA) ,NA)
;DCFPVRestNu             = OnEr( NPV2(WACC,FCFinRestWaarde,Max(FirstTinFormulaSet(Trend,PeriodInT),Min(ValDateColumn+1,LastTinFormulaSet(Trend,PeriodInT))),LastTinPeriod(PeriodInT),ValuationDate-ExpirationFactor) ,NA)

.Formulas Trend
DebtCorr                  = If(DataEntered(DebtCorrTotNu) and T=ValDateColumn,NA,OnEr( If(T=LastTinPeriod(PeriodInT),0,OnEr(NPV2(CostOfDebt,CostOfDebtSurplus,T+1,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor),NA)),NA))
DebtCorrCV                = OnEr( If(DataEntered(DebtCorrTotNu) and T=ValDateColumn,NA,NPV2(CostOfDebt,HelpVarDebtCorrCV,T,LastTinPeriod(PeriodInT),LastDateInT-ExpirationFactor)),NA)
DebtCorrTot               = OnZero(DebtCorr+DebtCorrCV,NA)


