StStateRR2              = If (Abs(OnER(OmzetKosten[LastTinFormulaSet(Trend,PeriodInT)-TsY,1]/NettoOmzet[LastTinFormulaSet(Trend,PeriodInT)-TsY,1],NA) - OnER(OmzetKosten[LastTinFormulaSet(Trend,PeriodInT),1]/NettoOmzet[LastTinFormulaSet(Trend,PeriodInT),1],NA)) < 0.02,0,1)
StStateRR3              = If (Abs(OnER(BedrK[LastTinFormulaSet(Trend,PeriodInT)-TsY,1]      /NettoOmzet[LastTinFormulaSet(Trend,PeriodInT)-TsY,1],NA) - OnER(BedrK[LastTinFormulaSet(Trend,PeriodInT),1]      /NettoOmzet[LastTinFormulaSet(Trend,PeriodInT),1],NA)) < 0.02,0,1)
StStateRR4              = If (Abs(WinstBelPerc[LastTinFormulaSet(Trend,PeriodInT)-TsY,1]- WinstBelPerc[LastTinFormulaSet(Trend,PeriodInT),1]) < 0.025,0,1)
StStateRR5              = If (Abs(WMargeVoorBel[LastTinFormulaSet(Trend,PeriodInT)-TsY,1]- WMargeVoorBel[LastTinFormulaSet(Trend,PeriodInT),1]) < 0.01,0,1)
StStateBalResRek1       = OnNA(BalResRekCheck1[LastTinFormulaSet(Trend,PeriodInT)],0);BalResRekCheck1=OnER( GroeiNettoOmzet / (BalAkt[T]/BalAkt[T-TsY]-1),0)
