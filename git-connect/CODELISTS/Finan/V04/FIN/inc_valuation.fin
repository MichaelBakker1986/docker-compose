rmWegingsFactor         = If(T=ValDateColumn-2,1,(If(T=ValDateColumn-1,2,(If(T=ValDateColumn,3,0)))))
rmRelWeging             = OnER(rmWegingsFactor/HSum(rmWegingsFactor,FirstTinPeriod,LastTinPeriod(PeriodInT)),NA)
