; geïnvesteerd vermogen
;
.Formulas NoTrend,Trend,User
AktKoopGoodwill         = AktKoopGoodwill[T-1]+KoopGoodwill
CUMAktGAfs              = CumAktGAfs[T-1]+AktGAfs
CumBoekRes              = CumBoekRes[T-1]+((BoekWinst-BoekVerlies)*(1-MargWinstBelPerc))

;;@@
; Geactiveerde kosten en off-balance lease
.Formulas NoTrend,User,Trend
AktK                    = AktK1+AktK2+AktK3
AktL                    = AktL1+AktL2+AktL3
VorigAktL               = VorigAktL1+VorigAktL2+VorigAktL3
VorigAktK               = VorigAktK1+VorigAktK2+VorigAktK3
VorigAktK1              = AktK1+AktK1Afs-AktK1Inv
VorigAktK2              = AktK2+AktK2Afs-AktK2Inv
VorigAktK3              = AktK3+AktK3Afs-AktK3Inv
VorigAktL1              = AktL1+AktL1Afs-AktL1Inv
VorigAktL2              = AktL2+AktL2Afs-AktL2Inv
VorigAktL3              = AktL3+AktL3Afs-AktL3Inv

.Formulas NoTrend,User
AktK1Inv                = MutCalc*(AktK1-AktK1[T-1]+AktK1Afs)
AktK2Inv                = MutCalc*(AktK2-AktK2[T-1]+AktK2Afs)
AktK3Inv                = MutCalc*(AktK3-AktK3[T-1]+AktK3Afs)
AktL1Inv                = MutCalc*(AktL1-AktL1[T-1]+AktL1Afs)
AktL2Inv                = MutCalc*(AktL2-AktL2[T-1]+AktL2Afs)
AktL3Inv                = MutCalc*(AktL3-AktL3[T-1]+AktL3Afs)
AktK1AfsPerc            = OnER(1/AktK1Term,NA) <-> AktK1Term
AktK2AfsPerc            = OnER(1/AktK2Term,NA) <-> AktK2Term
AktK3AfsPerc            = OnER(1/AktK3Term,NA) <-> AktK3Term
AktL1AfsPerc            = OnER(1/AktL1Term,NA) <-> AktL1Term
AktL2AfsPerc            = OnER(1/AktL2Term,NA) <-> AktL2Term
AktL3AfsPerc            = OnER(1/AktL3Term,NA) <-> AktL3Term
AktK1Term               = If(DataEntered(AktK1AfsPerc),OnER(1/AktK1AfsPerc,NA),GuessTerm(AktK1,AktK1Afs,1,50,5)) <-> AktK1AfsPerc
AktK2Term               = If(DataEntered(AktK2AfsPerc),OnER(1/AktK2AfsPerc,NA),GuessTerm(AktK2,AktK2Afs,1,50,5)) <-> AktK2AfsPerc
AktK3Term               = If(DataEntered(AktK3AfsPerc),OnER(1/AktK3AfsPerc,NA),GuessTerm(AktK3,AktK3Afs,1,50,5)) <-> AktK3AfsPerc
AktL1Term               = If(DataEntered(AktL1AfsPerc),OnER(1/AktL1AfsPerc,NA),GuessTerm(AktL1,AktL1Afs,1,50,5)) <-> AktL1AfsPerc
AktL2Term               = If(DataEntered(AktL2AfsPerc),OnER(1/AktL2AfsPerc,NA),GuessTerm(AktL2,AktL2Afs,1,50,5)) <-> AktL2AfsPerc
AktL3Term               = If(DataEntered(AktL3AfsPerc),OnER(1/AktL3AfsPerc,NA),GuessTerm(AktL3,AktL3Afs,1,50,5)) <-> AktL3AfsPerc
AktK1Kp                 = Max(AktK1Afs*AktK1Term*TsY,AktK1)
AktK2Kp                 = Max(AktK2Afs*AktK2Term*TsY,AktK2)
AktK3Kp                 = Max(AktK3Afs*AktK3Term*TsY,AktK3)
AktL1Kp                 = Max(AktL1Afs*AktL1Term*TsY,AktL1)
AktL2Kp                 = Max(AktL2Afs*AktL2Term*TsY,AktL2)
AktL3Kp                 = Max(AktL3Afs*AktL3Term*TsY,AktL3)

.Formulas Trend
RPKREDL                 = RPKREDL[T-1]                                   ;### Rva: wat doet dit?
RKredL                  = AktL*RPKREDL                                   ;###
AktK1                   = AktK1[T-1]+AktK1Inv-AktK1Afs
AktK2                   = AktK2[T-1]+AktK2Inv-AktK2Afs
AktK3                   = AktK3[T-1]+AktK3Inv-AktK3Afs
AktL1                   = AktL1[T-1]+AktL1Inv-AktL1Afs
AktL2                   = AktL2[T-1]+AktL2Inv-AktL2Afs
AktL3                   = AktL3[T-1]+AktL3Inv-AktL3Afs
AktK1Inv                = If(Self[t-1]<=0,NA,OnER(AktK1Kp[T-1]/AktK1Term/TsY,AktK1Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktK2Inv                = If(Self[t-1]<=0,NA,OnER(AktK2Kp[T-1]/AktK2Term/TsY,AktK2Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktK3Inv                = If(Self[t-1]<=0,NA,OnER(AktK3Kp[T-1]/AktK3Term/TsY,AktK3Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktL1Inv                = If(Self[t-1]<=0,NA,OnER(AktL1Kp[T-1]/AktL1Term/TsY,AktL1Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktL2Inv                = If(Self[t-1]<=0,NA,OnER(AktL2Kp[T-1]/AktL2Term/TsY,AktL2Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktL3Inv                = If(Self[t-1]<=0,NA,OnER(AktL3Kp[T-1]/AktL3Term/TsY,AktL3Inv[T-1])*OnER(NettoOmzet/NettoOmzet[T-1],1))
AktK1Afs                = If(AktK1Kp<=0,AktK1[T-1],Min(OnER((AktK1Kp+AktK1Kp[T-1])/2/AktK1Term/TsY,NA),AktK1[T-1]+AktK1Inv))
AktK2Afs                = If(AktK2Kp<=0,AktK2[T-1],Min(OnER((AktK2Kp+AktK2Kp[T-1])/2/AktK2Term/TsY,NA),AktK2[T-1]+AktK2Inv))
AktK3Afs                = If(AktK3Kp<=0,AktK3[T-1],Min(OnER((AktK3Kp+AktK3Kp[T-1])/2/AktK3Term/TsY,NA),AktK3[T-1]+AktK3Inv))
AktL1Afs                = If(AktL1Kp<=0,AktL1[T-1],Min(OnER((AktL1Kp+AktL1Kp[T-1])/2/AktL1Term/TsY,NA),AktL1[T-1]+AktL1Inv))
AktL2Afs                = If(AktL2Kp<=0,AktL2[T-1],Min(OnER((AktL2Kp+AktL2Kp[T-1])/2/AktL2Term/TsY,NA),AktL2[T-1]+AktL2Inv))
AktL3Afs                = If(AktL3Kp<=0,AktL3[T-1],Min(OnER((AktL3Kp+AktL3Kp[T-1])/2/AktL3Term/TsY,NA),AktL3[T-1]+AktL3Inv))
AktK1Kp                 = ExpandOriginalValue(AktK1Kp,AktK1Inv,AktK1Term)
AktK2Kp                 = ExpandOriginalValue(AktK2Kp,AktK2Inv,AktK2Term)
AktK3Kp                 = ExpandOriginalValue(AktK3Kp,AktK3Inv,AktK3Term)
AktL1Kp                 = ExpandOriginalValue(AktL1Kp,AktL1Inv,AktL1Term)
AktL2Kp                 = ExpandOriginalValue(AktL2Kp,AktL2Inv,AktL2Term)
AktL3Kp                 = ExpandOriginalValue(AktL3Kp,AktL3Inv,AktL3Term)

