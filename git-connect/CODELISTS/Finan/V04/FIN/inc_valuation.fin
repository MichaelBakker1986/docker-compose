rmWegingChoice          = 0
rmCorrResNu             = If(rmWegingChoice,HSum(rmResNaWeging,FirstTinPeriod(PeriodInT),LastTinPeriod(PeriodInT)),rmCorrRes[ValDateColumn,1])
rmValueNU               = OnER( rmCorrResNU*(1-rmPercInWinstNU)/(rmCostofEqNU-rmGrNOPLATNU)+rmHerFinNU ,NA)

rmPercInWinstNu         = Max(OnER(rmInWinst[ValDateColumn,1]/rmResNaBel[ValDateColumn,1],NA),NA)
rmGrNoPLATNu            = OnER(Min(GrNOPLAT[ValDateColumn,1],CostOfEq[ValDateColumn,1]-1%),NA)                          ; Opm Oscar nav pres. van Lanschot
rmCostOfEqNu            = CostOfEq[ValDateColumn,1]
rmValuePerShareNu       = OnER(rmValueNU*ScaleFactor/(AantalAandelenNU*1000) ,NA)
