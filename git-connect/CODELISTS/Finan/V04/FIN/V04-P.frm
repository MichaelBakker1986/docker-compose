{&&language=English}{&&OEM}$>DuPont-scheme<$
.MenuHelp           $>Schedule of profit margin and rate of return before interest and taxes on profit.<$
.FINANversion 2.0
;
;

.Let Bedrijfsnaam = &NaamBedr[1]
.If "#$Bedrijfsnaam#"=""
  .Let Bedrijfsnaam = "#DocumentCode#"
.EndIf

.Header                 #$Bedrijfsnaam#, #$DayMonthYear#          ;#$DossierTitle#, #$DayMonthYear#  ;DossierTitle bestaat niet meer
.FontName               Arial
.FontSize               8
.Destination            Printer,PDF                                ; EJS 24-5-2011 PDF mag ook
.LineSpacing            1.0
.TM                     1
.LM                     6                                            ;;Rva 2001-04-17 was 10

.ReportDialog           Destinations,Header

.NumberFormat           Windows
.Landscape              On
.InitPageSize
.SetNA                  Zero
.Let                    EindY=YearInT(OnZeroOrNA(LastTinFormulaSet(NoTrend,MainPeriod),FirstTinFormulaSet(Trend,MainPeriod)))
.AskInt                 EindY="Rapportage t/m jaar: ",1900,2100,#EindY#
.Let                    K1=YearToT(#EindY#-2)
.Let                    K2=YearToT(#EindY#-1)
.Let                    K3=YearToT(#EindY#)
.If                     #K1#<=0
.If                     #K2#<=0
.Let                    K2="0\BLANK"
.EndIf
.Let                    K1="#K2#\TSY=1"
.Let                    K2="#K3#\TSY=1"
.Let                    K3="0\BLANK"
.Else
.Let                    K1="#K1#\TSY=1"
.Let                    K2="#K2#\TSY=1"
.Let                    K3="#K3#\TSY=1"
.EndIf
