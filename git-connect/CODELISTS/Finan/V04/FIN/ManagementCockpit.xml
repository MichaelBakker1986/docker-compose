<?xml version="1.0" encoding="UTF-8"?><gadget_screen_configuration xmlns="http://www.finan.nl/gadget_screen_configuration-1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.finan.nl/gadget_screen_configuration-1c:\GadgetScreenConfig.xsd">	<form>		<title>$&gt;Management cockpit&lt;$</title>		<enableYearSelection>true</enableYearSelection>		<firstTFormula>FirstTinFormulaSet(Trend,PeriodInSheet,On)</firstTFormula>		<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>		<TsY>1</TsY>	</form>	<sliders>		<slider>			<title></title>			<formatString>%s $&gt;weeks&lt;$</formatString>			<min>0</min>			<max>52</max>			<variableName>DebTermijn</variableName>			<growthMode>false</growthMode>			<isPercentage>false</isPercentage>		</slider>		<slider>			<title></title>			<formatString>%s $&gt;weeks&lt;$</formatString>			<min>0</min>			<max>52</max>			<variableName>HandCredTermijn</variableName>			<growthMode>false</growthMode>			<isPercentage>false</isPercentage>		</slider>		<slider>			<title></title>			<formatString>%s %%</formatString>			<min>0%</min>			<max>100%</max>			<variableName>OpCashPerc</variableName>			<growthMode>false</growthMode>			<isPercentage>True</isPercentage>		</slider>	</sliders>	<radiobuttons>		<radiobutton showValues="true" show3D="true">			<title>$&gt;Liquidity&lt;$ (Current ratio)</title>			<axisTitle>Current ratio</axisTitle>			<chartType>bar</chartType>			<lowLimit show="false" color="FF0000">0.9</lowLimit>			<highLimit show="false" color="FF0000">1.1</highLimit>			<variables>				<variable>					<transparency>50</transparency>					<color>007eb3</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>CurrentRatio</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>			</variables>			<minY></minY>			<maxY></maxY>		</radiobutton>		<radiobutton showValues="true" show3D="true">			<title>$&gt;Rate of return on shareholder's equity&lt;$</title>			<axisTitle>$&gt;Profitability&lt;$ (%)</axisTitle>			<chartType>bar</chartType>			<lowLimit show="false" color="FF0000">0.9</lowLimit>			<highLimit show="false" color="FF0000">1.1</highLimit>			<variables>				<variable>					<transparency>50</transparency>					<color>007eb3</color>					<isPercentage>true</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>RtblEigenVerm</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>			</variables>			<minY></minY>			<maxY></maxY>		</radiobutton>		<radiobutton showValues="true" show3D="true">			<title>$&gt;Solvency&lt;$</title>			<axisTitle>$&gt;Solvency&lt;$ (%)</axisTitle>			<chartType>bar</chartType>			<lowLimit show="false" color="FF0000">0.9</lowLimit>			<highLimit show="false" color="FF0000">1.1</highLimit>			<variables>				<variable>					<transparency>50</transparency>					<color>007eb3</color>					<isPercentage>true</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>SolvBal</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>			</variables>			<minY></minY>			<maxY></maxY>		</radiobutton>		<radiobutton showValues="true" show3D="true">			<title></title>			<axisTitle>$&gt;Operating net working capital&lt;$</axisTitle>			<chartType>bar</chartType>			<lowLimit show="true" color="FF0000">0</lowLimit>			<highLimit show="false" color="FF0000">1.1</highLimit>			<variables>				<variable>					<transparency>0</transparency>					<color>007eb3</color>					<isPercentage>false</isPercentage>					<firstTFormula>FirstTinPeriod(MainPeriod)</firstTFormula>					<variableName>WerkKap</variableName>					<lastTFormula>LastTinPeriod(PeriodInSheet)</lastTFormula>					<title></title>				</variable>			</variables>			<minY></minY>			<maxY></maxY>		</radiobutton>	</radiobuttons>	<help>		<title>$&gt;Management cockpit&lt;$</title>		<helpText>$&gt;By moving the sliders, you can see the effects on the financial ratios.&lt;$|$&gt;At the right hand side you can select the ratio, which will be shown in the graph.&lt;$|$&gt;The button 'save and close' will save your changes.&lt;$||$&gt;Attention: the changes will be save in the current FINAN document.&lt;$|$&gt;If you don't want to save the changes, use the button 'cancel'.&lt;$</helpText>	</help></gadget_screen_configuration>