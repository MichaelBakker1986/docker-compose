.Formulas NoTrend, Trend
VATRates                       = VATRateOnSales
VATRateOnPurchases             = If(VATFrequency=0,NA,Case(DefaultVATRate,[0:0|1:VATRateLow|2:VATRateHigh|11:VATRateMixed1|12:VATRateMixed2|13:VATRateMixed3]))
VATRateOnSales                 = If(VATFrequency=0,NA,Case(DefaultVATRate,[0:0|1:VATRateLow|2:VATRateHigh|11:VATRateMixed1|12:VATRateMixed2|13:VATRateMixed3]))
VATRateOnInvestments           = If(VATFrequency=0,NA,VATRateHigh)

DefaultVATRate                 = 2

VATFrequency                   = 12
;VATRateHigh                    = If(LastDateInT > DMYtoDate(1,10,2012) ,0.21 , 0.19)
;VATRateLow                     = 6%
;VATRateMixed1                  = Self[T-1]
;VATRateMixed2                  = Self[T-1]
;VATRateMixed3                  = Self[T-1]
VATRateHigh                    = OnNA(OnER(MatrixLookup("TaxRates.xls",&MatrixNameTax[1],LastDateInT,1),NA),OnNA(MatrixLookup("TaxRates.xls","TaxRates",LastDateInT,1),Self[T-1]))
VATRateLow                     = OnNA(OnER(MatrixLookup("TaxRates.xls",&MatrixNameTax[1],LastDateInT,2),NA),OnNA(MatrixLookup("TaxRates.xls","TaxRates",LastDateInT,2),Self[T-1]))
VATRateMixed1                  = OnNA(OnER(MatrixLookup("TaxRates.xls",&MatrixNameTax[1],LastDateInT,3),NA),OnNA(MatrixLookup("TaxRates.xls","TaxRates",LastDateInT,3),Self[T-1]))
VATRateMixed2                  = OnNA(OnER(MatrixLookup("TaxRates.xls",&MatrixNameTax[1],LastDateInT,4),NA),OnNA(MatrixLookup("TaxRates.xls","TaxRates",LastDateInT,4),Self[T-1]))
VATRateMixed3                  = OnNA(OnER(MatrixLookup("TaxRates.xls",&MatrixNameTax[1],LastDateInT,5),NA),OnNA(MatrixLookup("TaxRates.xls","TaxRates",LastDateInT,5),Self[T-1]))

.Formulas Trend
VATPaymentFraction             = OnER((T>=FirstTinFormulaset(Trend))*(VATFrequency/TsY),NA)+OnER(VATPaymentFraction[T-1]-RoundUp(VATPaymentFraction[T-1])+((RoundUp(VATPaymentFraction[T-1])-VATPaymentFraction[T-1])>0),NA);-OnER((T>=FirstTinFormulaset(Trend))*(VATFrequency/TsY-(1/360)/TsY),NA)
VATPayableFraction             = Max(RoundUp(VATPaymentFraction,0)-1,0)
VATPayableFractionPrevT        = (VATPaymentFraction[T-1]>0)*(Round(VATPaymentFraction[T-1],0)=VATPaymentFraction[T-1])
;VATPayableFractionPrevT        = (((VATPaymentFraction[T-1]>0)*(Round(VATPaymentFraction[T-1],0)))=VATPayableFraction[T-1])And(VATPayableFraction[T-1]>0)

