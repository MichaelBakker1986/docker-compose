;----------------------------
; Oriëntatiegegevens

.Variables

PPPP    1A   1 1        ?      =CompanyName                                                        \=CompanyName -> CompanyName
PPPP    1A     1               =CompanyPlaceOfResidence                                            \=CompanyPlaceOfResidence -> CompanyPlaceOfResidence
PIII    1N C   1        ?      =Rechtsvorm                                                         \=Rechtsvorm ->Rechtsvorm
PPPP    1A     1        ?      =SectorTitle                                                        \=SectorTitle -> SectorTitle


.Variables
PPPP    1A   1 1        ?      $>General information<$                                             \GeneralInformation
PPPP    1A     2               $>Name of company<$                                                 \CompanyName
PPPP    1A     2               $>Address<$                                                         \CompanyAdress
PPPP    1A     2               $>Zip code<$                                                        \CompanyZipCode
PPPP    1A     2               $>City/town<$                                                       \CompanyPlaceOfResidence
PPPP    1A     2               $>Phone number<$                                                    \CompanyPhoneNumber
PIII    1A     2               $>Sector of industry<$                                              \SectorTitle
PIIII   1A     2               $>Industry code<$                                                   \SectorCode
PIII    1A     2               $>Sector of industry<$ ($>secondary<$)                              \SectorTitle2
PIIII   1A     2               $>Industry code<$ ($>secondary<$)                                   \SectorCode2
PPPP    1A     2               $>Commercial register Chamber of Commerce<$                         \ChamberOfCommerceNumber


;----------------------------
; Rechtsvorm

.Variables
PI      1N C   2        ?      $>Legal status<$                                                    \Rechtsvorm
;C Deze variabel mag alleen gewijzigd worden, wanneer er nog geen eigen vermogen is ingevuld op de balans

.Templates
ChamberOfCommerceNumber           = "00000000"
CompanyZipCode                    = "0000 >LL"

.Formulas NoTrend,Trend
CompanyName                       = &FPS_VAR_Naam[1]
CompanyAdress                     = &FPS_VAR_Straat[1]&" "&FPS_VAR_Huisnummer[1]
CompanyZipCode                    = &FPS_VAR_Postcode[1]
CompanyPlaceOfResidence           = &FPS_VAR_Woonplaats[1]
CompanyPhoneNumber                = &FPS_VAR_PHONENUMBER[1]
SectorTitle                       = &FPS_VAR_BIK_Omschr
;SectorTitle2                      = &
ChamberOfCommerceNumber           = &FPS_VAR_KVKnr[1]

.Formulas NoTrend
LimitedLiabilityCompany           = ReportAsLimitedLiabilityCompany
Rechtsvorm                        = OnNA(OnER(Val(&FPS_VAR_Rechtsvorm_nr[1]),NA),If(Abs(CapitalPartners[LastTinFormulaSet(Notrend,MainPeriod)])>0,11,1))
ReportAsLimitedLiabilityCompany   = If(Rechtsvorm[1]<=9,1,0)

.Formulas Trend
LimitedLiabilityCompany            = ReportAsLimitedLiabilityCompany

.Choices
Rechtsvorm                         = &TableAsChoices("LegalForm")

.Formulas Locked
Rechtsvorm                         = DataEntered(FPS_VAR_Rechtsvorm_nr)
;----------------------------
; Oppervlakte

.Variables
PIIIP 0 AN   1 2        ?      $>Company quarters area (m2)<$                                      \TotalIndoorSpace
PIIIP 0 AN     3               $>Showroom space<$                                                  \ShowroomSpace
PIIIP 0 AN     3               $>Warehouse space<$                                                 \WarehouseSpace
PIIIP 0 AN     3               $>Production-space<$                                                \ProductionSpace
PIIIP 0 AN     3               $>Office space<$                                                    \OfficeSpace
IIIIP 0 AN  *  3               $>Other space<$                                                     \OtherIndoorSpace
PIIIP 0=AN    -3               $>Total indoor space<$                                              \=TotalIndoorSpace
IIIIP 0 AN     3               $>Land<$                                                            \LandSurface
PPPPP 0=AN    -3=              $>Total area (m2)<$                                                 \TotalArea

.Formulas Trend
ShowroomSpace                  = Self[T-1]
WarehouseSpace                 = Self[T-1]
ProductionSpace                = Self[T-1]
OfficeSpace                    = Self[T-1]
OtherIndoorSpace               = Self[T-1]
LandSurface                    = Self[T-1]

;----------------------------
; Aantal medewerkers
.Variables

PIII  1 AN     2        ?      $>Total number of employees<$                                       \TotalNumberOfEmployees
IIII  1 AN  *  3               &"$>Number of employees department<$"&" "&Department_tpNumberOfEmployees[0]\DepartmentNumberOfEmployeesList=Department_tpNumberOfEmployees -> Department_tpNumberOfEmployees
PIII  1=AN    -3=              =TotalNumberOfEmployees                                             \=TotalNumberOfEmployees
PPPP M  CN % 1 3               $>Growth in number of employees<$                                   \GrowthInTotalNumberOfEmployees

.Formulas NoTrend,Trend
GrowthInTotalNumberOfEmployees = OnER(If(TimeAggregated,TotalNumberOfEmployees/TotalNumberOfEmployees[GetT(T,-TsY,0,TsY)]-1,TotalNumberOfEmployees/TotalNumberOfEmployees[GetT(T,-TsY,0,TsY)]-1),NA)

.Hints
GrowthInTotalNumberOfEmployees = "$$CalcYearAs=MeanCalc"

