{&&Language=English}
;==============================================================================================================
.Variables
P       1A     1               $>Project finance<$                                                 \ProjectFinance
;==============================================================================================================

.Variables
PPP     PA     2               $>Project period<$                                                  \ProjectPeriode
PI      PN D   3               $>Begin date project<$                                              \ProjectStartDate
PP    0 PN     4               $>Column ID (Begin)<$                                               \ProjectStart
PI      PN D   3               $>End date project<$                                                \ProjectEndDate
PP    0 PN     4               $>Column ID (End)<$                                                 \ProjectEnd
PII     BN C   3               $>Realized<$                                                        \ProjectHistory

.Choices
ProjectHistory = "No|Yes"

.Formulas NoTrend
ProjectHistory = 1

.Formulas Trend
ProjectHistory = 0

.Formulas NoTrend, Trend
ProjectStartDate       = FirstDateinT(FirstTinFormulaSet(Notrend, MainPeriod))
ProjectEndDate         = LastDateInT(LastTinFormulaSet(Trend, MainPeriod))
ProjectStart           = ValueT(DateToT(ProjectStartDate,PeriodInT))
ProjectEnd             = ValueT(DateToT(ProjectEndDate,PeriodInT))
ProjectPeriode         = "From "&DateStr(ProjectStartDate,2)&" till "&DateStr(ProjectEndDate,2)&"."

.Variables
PPPP    BN C   3               $>Project starting<$                                                \ProjectStarting=ProjectStartingCosts
PIII    BN C   4               $>Project starting costs<$                                          \ProjectStartingCosts
PIII    FN     4               $>Adding project active costs<$                                     \ProjectActiveCosts
PPPP M  CN    -4               $>Project active costs last period<$                                \ProjectActiveCostsLastperiod
PPPP    FN     4               $>Adding project active costs<$                                     \ProjectAddActiveCosts
PIII   -FN     4               $>Project active costs amortization<$                               \ProjectActiveCostsAmor
PPPP    BN    -4  =            $>Accumulated project active costs<$                                \ProjectActiveCostsCum
PIII    1N C   3               =ProjectFinalPayment                                                \=ProjectFinalPayment

.Formulas NoTrend,Trend
ProjectUseActiveCosts     = If(HSum(ProjectActiveCostsCum,1,LastTinPeriod)>0,1,0) ; JB Bijbehorende variabele is gedeclareerd in AnnualStatement.fin
ProjectActiveCosts        = If(ProjectStartingCosts=1,(CostOfSales+OperatingExpenses+DepreciationOfAssets-ExtraordinaryIncomeAndExpenses-FinancialResult),NA)

ProjectActiveCostsCum     = OnZero(ProjectActiveCostsCum[T-1]+ProjectAddActiveCosts-ProjectActiveCostsAmor,NA)
ProjectActiveCostsCumV05org = ProjectActiveCostsCum ; JB Bijbehorende variabele is gedeclareerd in inc_V05_OtherHissenVars.fin
ProjectActiveCostsLastperiod = ProjectActiveCostsCum[T-1]
ProjectAddActiveCosts     = (ProjectStartingCosts=1)*(ProjectActiveCosts)
ProjectStartingCosts      = OnNA(ProjectStartingCosts[T-1],0)

.Choices
ProjectStartingCosts = "No|Yes"

.Formulas Trend
ProjectActiveCostsAmor     = OnZero(Min(ProjectActiveCostsCum[T-1]+ProjectActiveCosts,NetSales-CostOfSales),NA)


