CurrentAccountAffiliatedCompaniesClearingHistory   = CurrentAccountAffiliatedCompanies

.Formulas Trend
TradeReceivablesTemp           = If(TradeReceivablesTermTemp<>NA,Min(Max(0,GetPoint(ProductSalesInvoicedNet,TradeReceivablesTermTemp/(52+1/7))),TradeReceivablesTotal[T-1]+SalesIncludingVAT)-(TradeReceivablesSalesGroups+TradeReceivables),NA)
TradeReceivablesTermTemp       = If(DataEntered(TradeReceivablesTerm),TradeReceivablesTerm,If(T<>FirstTinformulaset(Trend,PeriodInT),Self[T-1],NA))

;JB:  Onderstaande formule bouwt de debiteuren af op basis van de laatst ingegeven debiteurentermijn in dagen in de historie.
;     Afbouw gaat op gelijke wijze als opbouw, waarbij een maand exact 1/12 deel is van het jaar (ongeacht de maand).
TradeReceivablesClearingHistory                    = Max(OnER(TradeReceivables[LastTinFormulaSet(NoTrend)]*(TradeReceivablesTermDays[LastTinFormulaSet(NoTrend)]/365-(T-LastTinFormulaSet(NoTrend))/TsY)/(TradeReceivablesTermDays[LastTinFormulaSet(NoTrend)]/365),NA),NA)
TradeReceivablesForecast                           = OnZero(TradeReceivables-TradeReceivablesClearingHistory,NA)

;TradeReceivables                Indien algemene termijn is ingevuld        , baseer de waarde op die termijn -/- wat reeds is verdeeld in specificaties (discutabel: mag dit negatief zijn? mag nu, want is correctie op totaal)
;                                Anders: baseer op de projectieschakelaar:
;                                        10 : leegloop op basis historische termijn
;                                        20 : totaal debiteurenbedrag blijft gelijk
;                                        30 : gemiddelde debiteurentermijn blijft gelijk
; is een voorwaarde opgenomen dat de debiteuren nooit meer kunnen stijgen dan de omzet van de betreffende kolom(TradeReceivables[T-1]+ProductSalesInvoicedNet)
TradeReceivables               = If(DataEntered(TradeReceivablesTermDaysTotal),Min((TradeReceivablesTotal[T-1]+ProductSalesInvoicedNet)-TradeReceivablesSalesGroups,GetPoint(ProductSalesInvoicedNet           ,TradeReceivablesTermDaysTotal/365)-TradeReceivablesSalesGroups),OnZero(Case(Projection_TradeReceivables,[10:TradeReceivablesClearingHistory|20:TradeReceivablesTotal[T-1]-TradeReceivablesSalesGroups|30:Max(0,Min((TradeReceivablesTotal[T-1]+ProductSalesInvoicedNet)-TradeReceivablesSalesGroups,GetPoint(ProductSalesInvoicedNet           ,TradeReceivablesTermDays[T-1]/365)-TradeReceivablesSalesGroups))]),NA)) <-> TradeReceivablesTermDaysTotal

ProvisionDoubtfulAccountsReceivable                = OnZero(ProvisionDoubtfulAccountsReceivableClearingHistory + ProvisionDoubtfulAccountsReceivableForecast,NA)
OtherReceivables                                   = OnZero(OtherReceivablesClearingHistory                    + OtherReceivablesForecast                   ,NA)
OtherTransitionalAssets										  			 = OnZero(OtherTransitionalAssetsClearingHistory             + OtherTransitionalAssetsForecast            ,NA)
VATReceivable                                      = OnZero(VATReceivableClearingHistory                       + VATReceivableForecast                      ,NA)
OtherAccountsReceivable                            = OnZero(OtherAccountsReceivableClearingHistory             + OtherAccountsReceivableForecast            ,NA) <-> OtherAccountsReceivableTerm
CurrentAccountManagementDebit                     = OnZero(CurrentAccountManagementDebitClearingHistory        + CurrentAccountManagementDebitForecast      ,NA)
ReceivablesGroupCompanies                          = OnZero(ReceivablesGroupCompaniesClearingHistory           + ReceivablesGroupCompaniesForecast          ,NA)
CurrentAccountManagementCredit                     = OnZero(CurrentAccountManagementCreditClearingHistory      + CurrentAccountManagementCreditForecast     ,NA)
CurrentAccountGroupCompanies                       = OnZero(CurrentAccountGroupCompaniesClearingHistory        + CurrentAccountGroupCompaniesForecast       ,NA)
ReceivablesAffiliatedCompanies                     = OnZero(ReceivablesAffiliatedCompaniesClearingHistory      + ReceivablesAffiliatedCompaniesForecast     ,NA)
CurrentAccountAffiliatedCompanies                  = OnZero(CurrentAccountAffiliatedCompaniesClearingHistory   + CurrentAccountAffiliatedCompaniesForecast  ,NA)

ProvisionDoubtfulAccountsReceivableClearingHistory = OnZero(Self[T-1],NA)
OtherReceivablesClearingHistory                    = OnZero(Self[T-1],NA)
OtherTransitionalAssetsClearingHistory             = OnZero(Self[T-1],NA)
VATReceivableClearingHistory                       = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),VATReceivable[T-1]                    ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald
OtherAccountsReceivableClearingHistory             = Max(OnER(OtherAccountsReceivable[LastTinFormulaSet(NoTrend)]*(OtherAccountsReceivableTerm[LastTinFormulaSet(NoTrend)]/(52+1/7)-(T-LastTinFormulaSet(NoTrend))/TsY)/(OtherAccountsReceivableTerm[LastTinFormulaSet(NoTrend)]/(52+1/7)),NA),NA)
CurrentAccountManagementDebitClearingHistory       = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),CurrentAccountManagementDebit[T-1]    ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)
ReceivablesGroupCompaniesClearingHistory           = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),ReceivablesGroupCompanies[T-1]        ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)
CurrentAccountManagementCreditClearingHistory      = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),CurrentAccountManagementCredit[T-1]   ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)
CurrentAccountGroupCompaniesClearingHistory        = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),CurrentAccountGroupCompanies[T-1]     ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)
ReceivablesAffiliatedCompaniesClearingHistory      = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),ReceivablesAffiliatedCompanies[T-1]   ,OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)
CurrentAccountAffiliatedCompaniesClearingHistory   = If(CompanyProjectionTemplate=1,If(T=FirstTinFormulaSet(Trend),CurrentAccountAffiliatedCompanies[T-1],OnZero(Self[T-1],NA)),NA) ;Dus historische waarde wordt direct weggehaald OnZero(Self[T-1],NA)

ProvisionDoubtfulAccountsReceivableForecast        = OnZero(If(DataEntered(ProvisionDoubtfulAccountsReceivable),ProvisionDoubtfulAccountsReceivable  - ProvisionDoubtfulAccountsReceivableClearingHistory,Self[T-1])                     ,NA)
OtherReceivablesForecast                           = OnZero(If(DataEntered(OtherReceivables)                   ,OtherReceivables                     - OtherReceivablesClearingHistory                   ,Self[T-1])                     ,NA)
OtherTransitionalAssetsForecast                    = OnZero(If(DataEntered(OtherTransitionalAssets)            ,OtherTransitionalAssets              - OtherTransitionalAssetsClearingHistory            ,Self[T-1])                     ,NA)
VATReceivableForecast                              = OnZero(If(DataEntered(VATReceivable)                      ,VATReceivable                        - VATReceivableClearingHistory                      ,If(CompanyProjectionTemplate=1,Self[T-1],Max(-AccumulatedVATBalance,NA))),NA)
TaxOnProfitsReceivableForecast                     = OnZero(If(DataEntered(TaxOnProfitsReceivable)             ,TaxOnProfitsReceivable               - TaxOnProfitsReceivableClearingHistory             ,OnZero(OnEr(MAX(TaxOnProfits[GetT(T,-TsY,0,TsY)]/TsY-TaxOnProfits+If(T=ValueOfT(FirstTinYear),NA,TaxOnProfitsReceivable[T-1]-TaxOnProfitsPayable[T-1]),NA),NA),NA)),NA)
OtherAccountsReceivableForecast                    = OnZero(If(DataEntered(OtherAccountsReceivable)            ,OtherAccountsReceivable              - OtherAccountsReceivableClearingHistory            ,If(CompanyProjectionTemplate=1,OtherAccountsReceivable[T-1],If((GetPoint(SalesIncludingVat,OtherAccountsReceivableTerm/(52+1/7))  -Self[T-1])>SalesIncludingVat,SalesIncludingVat+Self[T-1],GetPoint(SalesIncludingVat,OtherAccountsReceivableTerm/(52+1/7))))),NA)
CurrentAccountManagementDebitForecast              = OnZero(If(DataEntered(CurrentAccountManagementDebit)      ,CurrentAccountManagementDebit        - CurrentAccountManagementDebitClearingHistory      ,If(CompanyProjectionTemplate=1,Self[T-1],If(T=FirstTinFormulaset(Trend),Max(CurrentAccountManagementDebit[T-1]-CurrentAccountManagementCredit[T-1],NA),Self[T-1]))     )                     ,NA)
CurrentAccountManagementCreditForecast             = OnZero(If(DataEntered(CurrentAccountManagementCredit)     ,CurrentAccountManagementCredit       - CurrentAccountManagementCreditClearingHistory     ,If(CompanyProjectionTemplate=1,Self[T-1],If(T=FirstTinFormulaset(Trend),Max(CurrentAccountManagementCredit[T-1]-CurrentAccountManagementDebit[T-1],NA),Self[T-1]))     )                     ,NA)
