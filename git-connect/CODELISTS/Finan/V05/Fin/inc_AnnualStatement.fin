InterestCAPayableFractionPrevT    = ((InterestCAPaymentFraction[T-1]*1)>0)*(Round(InterestCAPaymentFraction[T-1]*1,0)=InterestCAPaymentFraction[T-1])
;PaymentOfInterestOnCurrentAccount = If((T-Max(TsY/BalanceCurrentAccountFrequency,1)+1)<ValueOfT(FirstValueT(InterestExpensesOnCurrentAccount,FirstTinFormulaSet(Notrend,MainPeriod),T)),NA,If(Abs(HSum(PaymentOfInterestOnCurrentAccount,GetT(T,-Max(TsY/BalanceCurrentAccountFrequency,1)+1),T-1))>0,NA,HSum(InterestExpensesOnCurrentAccount,GetT(T,-Max(TsY/BalanceCurrentAccountFrequency,1)+1),T)))
;PaymentOfInterestOnCurrentAccount = If(Round(FesExpression(PeriodFraction(FirstTinFormulaset(Trend), BalanceCurrentAccountFrequency, 0)),6)>=1,DeferredInterestOnCurrentAccount[T-1],NA)
;PaymentOfInterestOnCurrentAccount = OnER(OnZero(DeferredInterestOnCurrentAccount[T-1]+InterestExpensesOnCurrentAccount-InterestCAPayableFraction/InterestCAPaymentFraction*InterestExpensesOnCurrentAccount-If(InterestCAPayableFractionPrevT Or (T=FirstTinFormulaSet(Trend)),DeferredInterestOnCurrentAccount[T-1],NA),NA),NA)
PaymentOfInterestOnCurrentAccount = OnZero(InterestExpensesOnCurrentAccount-Mut(DeferredInterestOnCurrentAccount),NA)
;DeferredInterestOnCurrentAccount  = OnZero(DeferredInterestOnCurrentAccount[T-1]+InterestExpensesOnCurrentAccount-PaymentOfInterestOnCurrentAccount,NA)
