;StandingRight                  = StandingRight[T-1]-StandingRightPayable
;DeferredTaxes                  = DeferredTaxes[T-1]
;ProvisionForReorganisation     = ProvisionForReorganisation[T-1]
;OtherProvisions                = OtherProvisions[T-1]

OtherPayables                  = If((CompanyProjectionTemplate=1) And Not DataEntered(OtherPayablesTerm),Self[T-1],OnZero(If(DataEntered(OtherPayablesTerm)   ,Max(0,GetPoint(OperatingExpensesExclPersonnelInclVAT,OtherPayablesTerm/(52+1/7)))   -AccountsPayableExpensesSubtotal,If(AccountsPayableExpensesSubtotal=NA,Max(0,GetPoint(OperatingExpensesExclPersonnelInclVAT,OtherPayablesTerm[T-1]/(52+1/7)))   -AccountsPayableExpensesSubtotal,OtherPayablesClearingHistory   )),NA)) <-> OtherPayablesTerm

OtherPayablesTerm              = GetFrac(OperatingExpensesExclPersonnelInclVAT    ,AccountsPayableExpenses)*(52+1/7) <-> OtherPayables

AccumulatedVATBalance          = OnER(OnZero(AccumulatedVATBalance[T-1]+VATBalance-(VATPayableFraction*1)/(VATPaymentFraction*1)*VATBalance-If(VATPayableFractionPrevT Or (T=FirstTinFormulaSet(Trend)),(VATPayable[T-1]-VATReceivable[T-1]),NA),NA),NA)

