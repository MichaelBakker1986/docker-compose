;Department_tpPensionPremiums                     = OnEr(If(DataEntered(Department_tpIncreasePensionPremiums),OnZero(Self[GetT(T,-TsY,0,TsY)]*(1+Department_tpIncreasePensionPremiums*Department_tpTotalPersonnelExpenses[GetT(T,-TsY,0,TsY)]/Self[GetT(T,-TsY,0,TsY)]),NA),ExpandFraction(Self,Department_tpWages)),NA) <-> Department_tpIncreasePensionPremiums
Department_tpSocialSecurities                    = OnEr(If(DataEntered(Department_tpIncreaseSocialSecurities),OnZero(Self[GetT(T,-TsY,0,TsY)]*(1+Department_tpIncreaseSocialSecurities),NA),If((Department_tpWagesForecast=18),NA,If((Department_tpWagesForecast=2) And (ExpandFraction(Self,Department_tpWages)=NA),Self[GetT(T,-TsY,0,TsY)]*(1+Department_tpAverageIncreaseWages),If((Department_tpWagesForecast=32) And (ExpandFraction(Self,Department_tpWages)=NA),OnZero(Self[GetT(T,-TsY,0,TsY)]*DefaultPriceIndex/DefaultPriceIndex[GetT(T,-TsY,0,TsY)],NA),ExpandFraction(Self,Department_tpWages))))),NA) <-> Department_tpIncreaseSocialSecurities
Department_tpPensionPremiums                     = OnEr(If(DataEntered(Department_tpIncreasePensionPremiums), OnZero(Self[GetT(T,-TsY,0,TsY)]*(1+Department_tpIncreasePensionPremiums) ,NA),If((Department_tpWagesForecast=18),NA,If((Department_tpWagesForecast=2) And (ExpandFraction(Self,Department_tpWages)=NA),Self[GetT(T,-TsY,0,TsY)]*(1+Department_tpAverageIncreaseWages),If((Department_tpWagesForecast=32) And (ExpandFraction(Self,Department_tpWages)=NA),OnZero(Self[GetT(T,-TsY,0,TsY)]*DefaultPriceIndex/DefaultPriceIndex[GetT(T,-TsY,0,TsY)],NA),ExpandFraction(Self,Department_tpWages))))),NA) <-> Department_tpIncreasePensionPremiums
;Bij keuze 2 en 32 kunnen er door een seizoenspatroon te volgen lege kolommen ontstaan. Daarna worden er geen pensioenpremies en soc.lasten meer berekend. Vandaar het extra afvangen van deze twee situaties.

Department_tpIncreaseWagesNet                    = OnER(OnZero((Department_tpWages/Department_tpWages[GetT(T,-TsY,0,TsY)])-1,NA),NA)
Department_tpIncreaseSocialSecurities            = OnER(OnZero((Department_tpSocialSecurities/Department_tpSocialSecurities[GetT(T,-TsY,0,TsY)])-1,NA),NA)
Department_tpIncreasePensionPremiums             = OnER(OnZero((Department_tpPensionPremiums/Department_tpPensionPremiums[GetT(T,-TsY,0,TsY)])-1,NA),NA)

Department_tpTotalExpensesAccrued                = Department_tpTotalExpensesAccrued[T-1]+ Department_tpTotalPersonnelExpenses - Department_tpTotalExpensesLiq
Department_tpTotalExpensesLiq                    = OnZero(If(DataEntered(Department_tpTotalExpensesAccrued),Department_tpTotalPersonnelExpenses-Mut(Department_tpTotalExpensesAccrued),If(Department_tpUseSeasonPatternForLiquidity=1,Min((Department_tpTotalPersonnelExpenses*TsY) / HSum(Department_tpWagesSeasonPattern,1,TsY) * Department_tpWagesSeasonPattern,Department_tpTotalExpensesAccrued[T-1]+Department_tpTotalPersonnelExpenses),Department_tpTotalPersonnelExpenses)),NA) <-> Department_tpTotalExpensesAccrued
;JB 2012-11-06: Totaal mag nooit groter zijn dan totaal overlopend+kosten[T]

.Formulas NoTrend,Trend
Department_tpWagesSeasonPattern                  = OnEr(OnNA(Self[GetT(T,-TsY,0,TsY)],Case(DateToMonth(LastDateInT),[#SeasonPatternPersonnelExpenses#])),NA)
Department_tpWagesSeasonPatternPerc              = If(TsY=1,1,OnER(Department_tpWagesSeasonPattern/HSum(Department_tpWagesSeasonPattern,1,TsY),NA))     ; Let op: Noemer op basis van 1e historische jaar.
Department_tpAverageWages                        = OnER(Department_tpTotalPersonnelExpenses / Department_tpNumberOfEmployees,NA)
Department_tpGrowthNumberOfEmployees             = OnER(Department_tpNumberOfEmployees/Department_tpNumberOfEmployees[GetT(T,-TsY,0,TsY)] -1, NA)

;===========================================
;Department_tpIncreaseWages (loonkostengroei)
Department_tpIncreaseWages                       = OnER(OnZero((Department_tpTotalPersonnelExpenses/Department_tpTotalPersonnelExpenses[GetT(T,-TsY,0,TsY)])-1,NA),NA)

;LOCKING
;Deze variabele is gelocked als Department_tpTotalPersonnelExpenses[T-TsY] leeg is, want groei kan dan niet bepaald worden --> JB - werkt nog niet, formule ion V05-visible.fsf uitgecommntarieerd.
;FLIPFLOP VIA FESCOMMANDS
;Department_tpIncreaseWages heeft een FlipFlop met een zestal andere variabelen.
; Bij invoer Department_tpIncreaseWages wordt deze waarde ook ingevoerd bij:
; - Department_tpIncreaseWagesNet         (heeft weer een FlipFlop met Department_tpWages)
; - Department_tpIncreaseSocialSecurities (heeft weer een FlipFlop met Department_tpSocialSecurities)
; - Department_tpIncreasePensionPremiums  (heeft weer een FlipFlop met Department_tpPensionPremiums)
; Bij invoer Department_tpWages, Department_tpSocialSecurities, Department_tpPensionPremiums wordt Department_tpIncreaseWages gewist en weer bepaald door zijn formule
;===========================================
Department_tpAverageIncreaseWages                = If(DataEntered(Department_tpWages) Or DataEntered(Department_tpIncreaseWages) Or TimeAggregated,OnZero((1+Department_tpIncreaseWages)/(OnERorNA(Department_tpNumberOfEmployees/Department_tpNumberOfEmployees[GetT(T,-TsY,0,TsY)],1))-1,NA),DefaultWagesIncreaseTrend)

