.Formulas Trend
SubordinatedDebt_tpRepaymentTerm          =If(SubordinatedDebt_tpRepaymentType=10, OnZero(If(DataEntered(SubordinatedDebt_tpRepayment),OnER(SubordinatedDebt_tpRepayment/SubordinatedDebt_tpNumberOfPaymentsInT,NA),If(T=FirstTinFormulaSet(Trend),OnER(SubordinatedDebt_tpRepayment[LastTinFormulaSet(NoTrend),1]/SubordinatedDebt_tpPaymentFrequency,NA),Self[T-1]) + OnER(SubordinatedDebt_tpNewLoan/(SubordinatedDebt_tpTerm*SubordinatedDebt_tpPaymentFrequency),NA)),NA),NA)
;Bovenstaand is aflossing                 =        Indien 1e PrognoseJaar,        neem de aflossings van laatste historische termijn,                                                                    neem anders de aflossing van vorige kolom  +aflossing bij nieuw leningdeel
;Deze berekening is een hulpvariabele die voorkomt dat de betaalde aflossing een complexe formule wordt

SubordinatedDebt_tpRepayment             = If(CompanyProjectionTemplate=1,NA,If(SubordinatedDebt_tpRepaymentType=10,OnZero(Min(SubordinatedDebt_tpPrincipal[T-1]+SubordinatedDebt_tpNewLoan-SubordinatedDebt_tpRemission,SubordinatedDebt_tpNumberOfPaymentsInT*SubordinatedDebt_tpRepaymentTerm),NA),If(T=Datetot(SubordinatedDebt_tpEndDate,0),Min(SubordinatedDebt_tpPrincipal[T-1]-SubordinatedDebt_tpRemission,SubordinatedDebt_tpBulletPayment),NA)))
SubordinatedDebt_tpShortTermPart         = If(SubordinatedDebt_tpRepaymentType=10,OnZero(Min(OnER(SubordinatedDebt_tpNewLoan/SubordinatedDebt_tpTerm,NA)+SubordinatedDebt_tpShortTermPart[T-1],SubordinatedDebt_tpPrincipal)*DefaultShortTermPartOfLongTermDebts,NA),If((T>=(DateToT(SubordinatedDebt_tpEndDate,0)-TsY)) And (T<(DateToT(SubordinatedDebt_tpEndDate,0))),Min(SubordinatedDebt_tpPrincipal[T-1]-SubordinatedDebt_tpRemission,SubordinatedDebt_tpBulletPayment),NA))
SubordinatedDebt_tpPrincipal             = OnZero(SubordinatedDebt_tpPrincipal[T-1]-SubordinatedDebt_tpRepayment-SubordinatedDebt_tpRemission+SubordinatedDebt_tpNewLoan,NA)
