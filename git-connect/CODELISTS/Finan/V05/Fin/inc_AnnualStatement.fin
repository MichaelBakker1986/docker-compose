.Formulas Trend
Product_tpInventory            = If(Product_tpWeeksInventory=NA,ExpandFraction(Product_tpInventory,Product_tpCost),GetPoint(Product_tpCost,Product_tpWeeksInventory/(52+1/7))) <-> Product_tpWeeksInventory
Product_tpWeeksInventory       = If(DataEntered(Product_tpInventory) Or TimeAggregated ,If(DataEntered(Self) And TimeAggregated,GetValue(Self,T,TsY(T)),GetFrac(Product_tpCost,Product_tpInventory)*(52+1/7)),Max(GetFrac(Product_tpCost,(Product_tpInventory[T-1]-Product_tpCost[T]))*(52+1/7),Product_tpWeeksInventory[T-1])) <-> Product_tpInventory
Product_tpMargin               = If(DataEntered(Product_tpCost) Or TimeAggregated,If(Product_tpCost=NA,NA,OnER(1-Product_tpCost/Product_tpSales,NA)),If(Product_tpForecast=11,Product_tpMargin[T-1],ExpandLevel(Self))) <-> Product_tpCost
Product_tpCost                 = If(DataEntered(Product_tpMargin),OnER((1-Product_tpMargin)*Product_tpSales,NA),If(Product_tpCostForecast=18,NA,If((Product_tpMargin=NA) And ((Product_tpSales[T-1]=NA) Or (Product_tpSales[T-1]=0)),Product_tpCost[T-1],If((Product_tpMargin=NA),NA,OnER((1-Product_tpMargin)*Product_tpSales,NA))))) <-> Product_tpMargin
Product_tpSales                = OnEr(If(Product_tpForecast=18,NA,If(Product_tpForecast=11,If(DataEntered(Product_tpGrowth) And (Product_tpSales[GetT(T,-TsY,0,TsY)]>0),(1+Product_tpGrowth)*Self[GetT(T,-TsY,0,TsY)],Self[T-1]),(1+Product_tpGrowth)*Self[GetT(T,-TsY,0,TsY)])),NA) <-> Product_tpGrowth
Product_tpGrowth               = OnNA(If(TimeAggregated Or DataEntered(Product_tpSales),OnER(OnZero(Product_tpSales/Product_tpSales[GetT(T,-TsY,0,TsY)]-1,NA),NA),OnER(Case(Product_tpForecast,[11:(Product_tpSales[T-1]/Product_tpSales[GetT(T,-TsY,0,TsY)])-1|16:0|12:ExpandGrowth(Product_tpSales,DefaultSalesMinGrowth,DefaultSalesMaxGrowth)/(Product_tpSales[GetT(T,-TsY,0,TsY)])-1|15:DefaultPriceIndex/DefaultPriceIndex[GetT(T,-TsY,0,TsY)]-1]),NA)),If(Product_tpSales<>NA,0,NA))  ;JB 2014-08-21: OnNA toegevoegd om veld altijd een waarde te geven voor het tonen in 5-m-wizard
;Onderstaande groeiformule heeft een afwijkende keuze 12. Daarbij wordt het ExpandGrowth effect op maandbasis bepaald in plaats van op jaarbasis. Wellicht nog een groeikeuze voor de toekomst.
;Product_tpGrowth               = If(TimeAggregated,OnER(OnZero(Product_tpSales/Product_tpSales[GetT(T,-TsY,0,1)]-1,NA),NA),OnER(If(DataEntered(Product_tpSales),Product_tpSales/Product_tpSales[GetT(T,-TsY,0,TsY)]-1,Case(Product_tpForecast,[11:(Product_tpSales[T-1]/Product_tpSales[GetT(T,-TsY,0,TsY)])-1|16:0|12:OnNotPos(Min(Self[GetT(T,-TsY,0,TsY)],DefaultSalesMaxGrowth),OnNeg(Self[GetT(T,-TsY,0,TsY)],Max(Self[GetT(T,-TsY,0,TsY)],DefaultSalesMinGrowth)))|13:Self[GetT(T,-TsY,0,TsY)]|15:DefaultPriceIndex/DefaultPriceIndex[GetT(T,-TsY,0,TsY)]-1])),NA))
;Product_tpSales                = OnEr(If(DataEntered(Product_tpGrowth),Product_tpSales[GetT(T,-TsY,0,TsY)]*(1+Product_tpGrowth),Case(Product_tpForecast,[11:Self[T-1]|16:Self[GetT(T,-TsY,0,TsY)]|12:ExpandGrowth(Self,DefaultSalesMinGrowth,DefaultSalesMaxGrowth)|15:OnZero(Self[GetT(T,-TsY,0,TsY)]*DefaultPriceIndex/DefaultPriceIndex[GetT(T,-TsY,0,TsY)],NA)])),NA) <-> Product_tpGrowth

Product_tpSalesAccruedAssets      = OnZero(Max(Product_tpSalesAccruedAssets[T-1] - Product_tpSalesAccruedLiabilities[T-1] + (Product_tpSales - Product_tpSalesInvoiced),0),NA)
Product_tpSalesAccruedLiabilities = OnZero(Max(Product_tpSalesAccruedLiabilities[T-1] - Product_tpSalesAccruedAssets[T-1] - (Product_tpSales - Product_tpSalesInvoiced),0),NA)
Product_tpSalesInvoiced        = If(Product_tpUseSeasonPatternForLiquidity=1,If(T=LastTinYear,FesExpression(GetValue(Product_tpSales,T,1,Bookyear))-HSum(Self,FirstTinYear,T-1),FesExpression(GetValue(Product_tpSales,T,1,Bookyear)) * Product_tpSeasonPatternPerc),Product_tpSales)

Product_tpSalesLiq             = OnZero((Product_tpSalesInvoicedIncVAT-Product_tpMutationTradeReceivables+Product_tpMutationReceivedPrePayments)/(1+Product_tpVATRateSales),NA)
;Product_tpSalesLiq             = OnZero(If(DataEntered(Product_tpTradeReceivablesTerm),(Product_tpSalesInvoicedIncVAT-Product_tpMutationTradeReceivables)+Product_tpMutationReceivedPrePayments,Product_tpSalesInvoicedIncVAT + Product_tpMutationReceivedPrePayments - GetPoint(Product_tpSalesInvoicedIncVAT,Product_tpTradeReceivablesTerm/365) + Product_tpTradeReceivables)/(1+Product_tpVATRateSales),NA)
Product_tpSalesLiqIncVAT       = OnZero(Product_tpSalesLiq*(1+Product_tpVATRateSales),NA)
;                                                                                                                                                                                                ---deze voorwaarde voorkomt dat de mutatie debiteuren zo groot wordt dat deze leidt tot negatieve inkomsten----
Product_tpCostCashExpenses           = OnZero((Product_tpCostPurchaseIncVAT-Product_tpMutationCostOfSalesPayable+Product_tpMutationCostOfSalesAdvancePayments)/(1+Product_tpVATRatePurchases),NA)
Product_tpCostCashExpensesIncVAT     = OnZero(Product_tpCostCashExpenses*(1+Product_tpVATRatePurchases),NA)
Product_tpCostOfSalesPayableTerm     = If(DataEntered(Product_tpCostCashExpenses) or TimeAggregated,GetFrac(Product_tpCostPurchaseIncVAT,Product_tpCostOfSalesPayable)*365,If((Projection_TradePayables=10) And (T=FirstTinformulaset(Trend)),OnZero(TradePayablesTermDays[LastTinFormulaSet(NoTrend)]    * (Product_tpCost[LastTinFormulaSet(NoTrend)]  <> NA),NA),Product_tpCostOfSalesPayableTerm[T-1])) <-> Product_tpCostCashExpenses

Product_tpTradeReceivablesTerm       = If(DataEntered(Product_tpSalesLiq)         or TimeAggregated, GetFrac(Product_tpSalesInvoicedIncVAT,Product_tpTradeReceivables) *365,If((Projection_TradeReceivables=10) And (T=FirstTinformulaset(Trend)),OnZero(TradeReceivablesTermDays[LastTinFormulaSet(NoTrend)] * (Product_tpSales[LastTinFormulaSet(NoTrend)] <> NA),NA),Product_tpTradeReceivablesTerm[T-1]))   <-> Product_tpSalesLiq
Product_tpTradeReceivables           = If(DataEntered(Product_tpSalesLiq)        ,Product_tpSalesInvoicedIncVAT   -Product_tpSalesLiqIncVAT        +Product_tpTradeReceivables[T-1]  +Product_tpMutationReceivedPrePayments       ,If(CompanyProjectionTemplate=1,Self[T-1],If((GetPoint(Product_tpSalesInvoicedIncVAT,Product_tpTradeReceivablesTerm/365)  -Self[T-1])> OnNA(Product_tpSalesInvoicedIncVAT,0),Product_tpSalesInvoicedIncVAT+Self[T-1],GetPoint(Product_tpSalesInvoicedIncVAT,Product_tpTradeReceivablesTerm  /365))))
;                                                                                                                                                                                                                                  Indien de berekende groei groter is dan de omzet, dan dit afvangen, want debiteuren kunnen niet meer stijgen dan omzet in dezelfde periode
Product_tpCostOfSalesPayable         = If(DataEntered(Product_tpCostCashExpenses),Product_tpCostPurchaseIncVAT    -Product_tpCostCashExpensesIncVAT+Product_tpCostOfSalesPayable[T-1]+Product_tpMutationCostOfSalesAdvancePayments,If((GetPoint(Product_tpCostPurchaseIncVAT ,Product_tpCostOfSalesPayableTerm/365)-Self[T-1])> OnNA(Product_tpCostPurchaseIncVAT,0) ,Product_tpCostPurchaseIncVAT +Self[T-1],GetPoint(Product_tpCostPurchaseIncVAT ,Product_tpCostOfSalesPayableTerm/365)))

;Product_tpCostOfSalesAdvancePayments = OnZero(Product_tpCostCashExpenses*(1+Product_tpVATRatePurchases)-Product_tpCostPurchaseIncVAT+Product_tpCostOfSalesPayable,NA)
Product_tpCostPurchaseCostOfSales    = If(Product_tpCostCashPurchaseSetting=20,OnZero(If((Product_tpMargin=NA),NA,OnER((1-Product_tpMargin)*Product_tpSales,NA))*Product_tpSalesInvoiced,NA),Product_tpCost)
Product_tpCostAccruedCumulative      = OnZero(Product_tpCostAccruedCumulative[T-1]+Product_tpCostAccrued,NA)

Product_tpMutationSalesAccruedAssets         = Mut(Product_tpSalesAccruedAssets)
Product_tpMutationSalesAccruedLiabilities    = Mut(Product_tpSalesAccruedLiabilities)
Product_tpMutationTradeReceivables           = Mut(Product_tpTradeReceivables)
Product_tpMutationReceivedPrePayments        = Mut(Product_tpReceivedPrePayments)
