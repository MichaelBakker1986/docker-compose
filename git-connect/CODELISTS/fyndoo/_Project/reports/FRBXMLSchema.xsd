	<xs:complexType name="Heading" mixed="true">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element ref="data" />
		</xs:choice>
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute name="visible" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="Columns">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="col">
				<xs:complexType>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="table" type="Table" />
						<xs:element name="graph" type="Graph" />
						<xs:element name="p" type="Paragraph" />
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:choice>
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute name="visible" type="xs:string" />
		<xs:attribute name="percentage" type="xs:boolean" />
	</xs:complexType>
	<xs:complexType name="Table">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="tr">
				<xs:complexType>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="th" type="TDTH" />
						<xs:element name="td" type="TDTH" />
					</xs:choice>
					<xs:attribute name="class" type="xs:string" />
					<xs:attribute name="visible" type="xs:string" />
					<xs:attribute name="repeat">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="newpage" />
								<xs:enumeration value="newpageonly" />
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="data" />
		</xs:choice>
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute name="visible" type="xs:string" />
		<xs:attribute name="width" type="xs:integer" />
		<xs:attribute name="type" type="docxWidthType"/>
	</xs:complexType>
	<xs:complexType name="TOC">
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<xs:element name="tocdepth" type="xs:integer" />
			<xs:element name="title" type="xs:string" />
		</xs:choice>
		<xs:attribute name="class" type="xs:string" />
		<xs:attribute name="visible" type="xs:string" />
