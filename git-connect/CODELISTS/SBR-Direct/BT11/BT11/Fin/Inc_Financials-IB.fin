.Formulas NoTrend, Trend
IBAangifte=not(visible(IBAangifte)) or (bdabstrSectionIncomeTaxSocialInsurancePremiumTitleResult and bdabstrBox1IncomeWorkAndOwnHouseTitleResult and bdabstrBox2SubstantialBenefitIncomeTitleResult and bdabstrBox3IncomeSavingsStocksTitleResult and frclrNotesBankingIncomeResult)
bdabstrSectionIncomeTaxSocialInsurancePremiumTitleResult = not(HSUM(IncompleteIncomeTaxCheck,1,MaxT)) and HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT) 
bdabstrBox1IncomeWorkAndOwnHouseTitleResult = not(HSUM(IncompleteBox1Check,1,MaxT))  and HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)
bdabstrBox2SubstantialBenefitIncomeTitleResult = not(HSUM(IncompleteBox2Check,1,MaxT))  and HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)
bdabstrBox3IncomeSavingsStocksTitleResult = not(HSUM(IncompleteBox3Check,1,MaxT))  and HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)
frclrNotesBankingIncomeResult = not(HSUM(IncompleteToelichtingIBCheck,1,MaxT))  and HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)

bdabstrSectionIncomeTaxSocialInsurancePremiumTitleResultWarning = &if(HSUM(IncompleteIncomeTaxCheck,1,MaxT),"Niet alle verplichte gegevens zijn ingevuld.",if(not(HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)),"Voor minstens 1 jaar moet data worden ingevuld.|",""))
bdabstrBox1IncomeWorkAndOwnHouseTitleResultWarning = &if(HSUM(IncompleteBox1Check,1,MaxT),"Niet alle verplichte gegevens zijn ingevuld.",if(not(HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)),"Voor minstens 1 jaar moet data worden ingevuld.|",""))
bdabstrBox2SubstantialBenefitIncomeTitleResultWarning = &if(HSUM(IncompleteBox2Check,1,MaxT),"Niet alle verplichte gegevens zijn ingevuld.",if(not(HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)),"Voor minstens 1 jaar moet data worden ingevuld.|",""))
bdabstrBox3IncomeSavingsStocksTitleResultWarning = &if(HSUM(IncompleteBox3Check,1,MaxT),"Niet alle verplichte gegevens zijn ingevuld.",if(not(HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)),"Voor minstens 1 jaar moet data worden ingevuld.|",""))
frclrNotesBankingIncomeResultWarning = &if(HSUM(IncompleteToelichtingIBCheck,1,MaxT),"Niet alle verplichte gegevens zijn ingevuld.",if(not(HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT)),"Voor minstens 1 jaar moet data worden ingevuld.|",""))

IncompleteIncomeTaxCheck=FES_DATAENTEREDINCOLUMN and Exists(X,SelectDescendants(bdabstrSectionIncomeTaxSocialInsurancePremiumTitleResult),InputRequired(X) and not(DataAvailable(X)))
IncompleteBox1Check=FES_DATAENTEREDINCOLUMN and Exists(X,SelectDescendants(bdabstrBox1IncomeWorkAndOwnHouseTitleResult),InputRequired(X) and not(DataAvailable(X)))
IncompleteBox2Check=FES_DATAENTEREDINCOLUMN and Exists(X,SelectDescendants(bdabstrBox2SubstantialBenefitIncomeTitleResult),InputRequired(X) and not(DataAvailable(X)))
IncompleteBox3Check=FES_DATAENTEREDINCOLUMN and Exists(X,SelectDescendants(bdabstrBox3IncomeSavingsStocksTitleResult),InputRequired(X) and not(DataAvailable(X)))
IncompleteToelichtingIBCheck=(FES_DATAENTEREDINCOLUMN and Exists(X,SelectDescendants(frclrNotesBankingIncomeResult),InputRequired(X) and not(DataAvailable(X)))) or (HSUM(FES_DATAENTEREDINCOLUMN,1,MaxT) = 0)
 
