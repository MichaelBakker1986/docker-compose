; Categories with model summation and protection

; Assets:

.Variable venjbw2iAssets, Locked=On
.Variable venjbw2iAssets, Formula=(venjbw2iAssetsNoncurrent+venjbw2iAssetsCurrent)

.Variable hc16_frclrBalanceSheet_pvenjbw2iAssets, Locked=On
.Variable hc16_frclrBalanceSheet_pvenjbw2iAssets, Formula=(hc16_venjbw2iAssets_pvenjbw2iAssetsNoncurrent+hc16_venjbw2iAssets_pvenjbw2iAssetsCurrent)

.Variable hc45_frclrConsolidatedBalanceSheet_pvenjbw2iAssets, Locked=On
.Variable hc45_frclrConsolidatedBalanceSheet_pvenjbw2iAssets, Formula=(hc45_venjbw2iAssets_pvenjbw2iAssetsNoncurrent+hc45_venjbw2iAssets_pvenjbw2iAssetsCurrent)

; AssetsNonCurrent:

.Variable venjbw2iAssetsNonCurrent, Locked=On
.Variable venjbw2iAssetsNonCurrent, Formula=(venjbw2iAssetsNoncurrent_pvenjbw2iIntangibleAssets+venjbw2iAssetsNoncurrent_pvenjbw2iPropertyPlantEquipment+rjiInvestmentProperties+venjbw2iAssetsNoncurrent_pvenjbw2iFinancialAssets)

.Variable hc16_venjbw2iAssets_pvenjbw2iAssetsNoncurrent, Locked=On
.Variable hc16_venjbw2iAssets_pvenjbw2iAssetsNoncurrent, Formula=(hc16_venjbw2iAssetsNoncurrent_pvenjbw2iIntangibleAssets+hc16_venjbw2iAssetsNoncurrent_pvenjbw2iPropertyPlantEquipment+hc16_venjbw2iAssetsNoncurrent_prjiInvestmentProperties+hc16_venjbw2iAssetsNoncurrent_pvenjbw2iFinancialAssets)

.Variable hc45_venjbw2iAssets_pvenjbw2iAssetsNoncurrent, Locked=On
.Variable hc45_venjbw2iAssets_pvenjbw2iAssetsNoncurrent, Formula=(hc45_venjbw2iAssetsNoncurrent_pvenjbw2iIntangibleAssets+hc45_venjbw2iAssetsNoncurrent_pvenjbw2iPropertyPlantEquipment+hc45_venjbw2iAssetsNoncurrent_prjiInvestmentProperties+hc45_venjbw2iAssetsNoncurrent_pvenjbw2iFinancialAssets)

; AssetsCurrent

.Variable venjbw2iAssetsCurrent, Locked=On
.Variable venjbw2iAssetsCurrent, Formula=(venjbw2iAssetsCurrent_pvenjbw2iInventories+rjiConstructionContractsAssets+venjbw2iAssetsCurrent_pvenjbw2iReceivables+venjbw2iSecuritiesCurrent+venjbw2iAssetsCurrent_pvenjbw2iCashAndCashEquivalents)

.Variable hc16_venjbw2iAssets_pvenjbw2iAssetsCurrent, Locked=On
.Variable hc16_venjbw2iAssets_pvenjbw2iAssetsCurrent, Formula=(hc16_venjbw2iAssetsCurrent_pvenjbw2iInventories+hc16_venjbw2iAssetsCurrent_prjiConstructionContractsAssets+hc16_venjbw2iAssetsCurrent_pvenjbw2iReceivables+hc16_venjbw2iAssetsCurrent_pvenjbw2iSecuritiesCurrent+hc16_venjbw2iAssetsCurrent_pvenjbw2iCashAndCashEquivalents)

.Variable hc45_venjbw2iAssets_pvenjbw2iAssetsCurrent, Locked=On
.Variable hc45_venjbw2iAssets_pvenjbw2iAssetsCurrent, Formula=(hc45_venjbw2iAssetsCurrent_pvenjbw2iInventories+hc45_venjbw2iAssetsCurrent_prjiConstructionContractsAssets+hc45_venjbw2iAssetsCurrent_pvenjbw2iReceivables+hc45_venjbw2iAssetsCurrent_pvenjbw2iSecuritiesCurrent+hc45_venjbw2iAssetsCurrent_pvenjbw2iCashAndCashEquivalents)

; Liabilities:

.Variable venjbw2iEquityAndLiabilities, Locked=On
.Variable venjbw2iEquityAndLiabilities, Formula=(venjbw2iEquity+venjbw2iEquityAndLiabilities_pvenjbw2iProvisions+venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesNoncurrent+venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesCurrent)

.Variable hc16_frclrBalanceSheet_pvenjbw2iEquityAndLiabilities, Locked=On
.Variable hc16_frclrBalanceSheet_pvenjbw2iEquityAndLiabilities, Formula=(hc16_venjbw2iEquityAndLiabilities_pvenjbw2iEquity+hc16_venjbw2iEquityAndLiabilities_pvenjbw2iProvisions+hc16_venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesNoncurrent+hc16_venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesCurrent)

.Variable hc45_frclrConsolidatedBalanceSheet_pvenjbw2iEquityAndLiabilities, Locked=On
.Variable hc45_frclrConsolidatedBalanceSheet_pvenjbw2iEquityAndLiabilities, Formula=(venjbw2iEquityGroup+hc45_venjbw2iEquityAndLiabilities_pvenjbw2iProvisions+hc45_venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesNoncurrent+hc45_venjbw2iEquityAndLiabilities_pvenjbw2iLiabilitiesCurrent)

.Variable venjbw2iEquityGroup, Formula=venjbw2iEquityGroup_pvenjbw2iEquity+venjbw2iNoncontrollingInterest, Locked=On
.Variable rjiPurchasePropertyPlantEquipment, Formula=frciReplacementPurchasePropertyPlantEquipment+frciExpansionPurchasePropertyPlantEquipment, Locked=On





