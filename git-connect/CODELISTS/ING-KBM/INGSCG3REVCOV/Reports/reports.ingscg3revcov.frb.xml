        <td><xsl:value-of select="fes:getVariableTitle($doc, 'Convenant_tpSoort', $tupleIndices)"/></td>
        <td><xsl:value-of select="fes:getFormattedValue($doc, 'Convenant_tpSoort', $tupleIndices)"/></td>
    </tr>
    <tr class="lefttwocols">
        <td><xsl:value-of select="fes:getVariableTitle($doc, 'Convenant_tpTekst', $tupleIndices)"/></td>
        <td><xsl:value-of select="fes:getFormattedValue($doc, 'Convenant_tpTekst', $tupleIndices)" /></td>
    </tr>
    <tr class="lefttwocols">
        <td><xsl:value-of select="fes:getVariableTitle($doc, 'Convenant_tpToelichting', $tupleIndices)"/></td>
