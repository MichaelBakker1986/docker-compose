LeeftijdEigenGeb = If(LeeftijdGebEigendom=NA,NA,IF(LeeftijdGebEigendom>10,3,IF(LeeftijdGebEigendom>5,2,1)))
OmvangTypering   = If(DierenOmvangTovNorm=NA,NA,if(DierenOmvangTovNorm>1.50,4,if(DierenOmvangTovNorm>1.10,3,if(DierenOmvangTovNorm>=1,2,1))))

