BuitengewResBnk         = fmBuitengewRes[FirstTinFormulaSet(Trend,MainPeriod),1]
LoanToValue= OnEr(TotaalKredietOG/WaardeOG,NA)
CurrentRatioBnk = OnEr((fmVordBnk+fmVrdBnk+fmLiqMidBnk-fmCorrectieVlottendeActivaBnk)/fmKortVVBnk,NA)
